        <character constraint="in cross-section" constraintid="o8929" is_modifier="false" name="shape" src="d0_s5" value="m--shaped" value_original="m--shaped" />
      </biological_entity>
      <biological_entity constraint="lateral adaxial" id="o8927" name="vein" name_original="veins" src="d0_s5" type="structure">
        <character is_modifier="true" name="quantity" src="d0_s5" value="2" value_original="2" />
        <character constraint="than midvein" constraintid="o8928" is_modifier="false" name="prominence" src="d0_s5" value="more prominent" value_original="more prominent" />
      </biological_entity>
      <biological_entity id="o8928" name="midvein" name_original="midvein" src="d0_s5" type="structure" />
