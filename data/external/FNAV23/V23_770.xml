        <character is_modifier="false" name="architecture" notes="" src="d0_s0" value="short-rhizomatous" value_original="short-rhizomatous" />
        <character name="growth_form" value="plant" />
      </biological_entity>
      <biological_entity id="o8916" name="clump" name_original="clumps" src="d0_s0" type="structure">
        <character is_modifier="true" name="size" src="d0_s0" value="large" value_original="large" />
      </biological_entity>
      <relation from="o8915" id="r1064" name="in" negation="false" src="d0_s0" to="o8916" />
    </statement>
    <statement id="d0_s1">
      <text>rhizomes brown or black, without yellowbrown felty covering.</text>
      <biological_entity id="o8917" name="rhizome" name_original="rhizomes" src="d0_s1" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s1" value="brown" value_original="brown" />
        <character is_modifier="false" name="coloration" src="d0_s1" value="black" value_original="black" />
      </biological_entity>
      <biological_entity id="o8918" name="covering" name_original="covering" src="d0_s1" type="structure">
        <character is_modifier="true" name="coloration" src="d0_s1" value="yellowbrown" value_original="yellowbrown" />
        <character is_modifier="true" name="pubescence" src="d0_s1" value="felty" value_original="felty" />
      </biological_entity>
      <relation from="o8917" id="r1065" name="without" negation="false" src="d0_s1" to="o8918" />
    </statement>
    <statement id="d0_s2">
      <text>Culms reddish purple at base.</text>
      <biological_entity id="o8919" name="culm" name_original="culms" src="d0_s2" type="structure">
        <character constraint="at base" constraintid="o8920" is_modifier="false" name="coloration" src="d0_s2" value="reddish purple" value_original="reddish purple" />
      </biological_entity>
      <biological_entity id="o8920" name="base" name_original="base" src="d0_s2" type="structure" />
    </statement>
    <statement id="d0_s3">
      <text>Leaves: basal sheaths sometimes fibrous;</text>
      <biological_entity id="o8921" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
