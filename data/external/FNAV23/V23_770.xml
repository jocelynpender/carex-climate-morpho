        <character is_modifier="false" modifier="sometimes" name="texture" src="d0_s3" value="fibrous" value_original="fibrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>sheath fronts reddish or green, veined, herbaceous;</text>
      <biological_entity id="o8923" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
      <biological_entity constraint="sheath" id="o8924" name="front" name_original="fronts" src="d0_s4" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s4" value="reddish" value_original="reddish" />
