        <character is_modifier="false" name="architecture" src="d0_s4" value="veined" value_original="veined" />
        <character is_modifier="false" name="growth_form_or_texture" src="d0_s4" value="herbaceous" value_original="herbaceous" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>blades with 2 lateral adaxial veins more prominent than midvein, M-shaped in cross-section when young, glabrous or scabrous, strongly glaucous.</text>
      <biological_entity id="o8925" name="leaf" name_original="leaves" src="d0_s5" type="structure" />
