        <character is_modifier="false" modifier="strongly" name="pubescence" src="d0_s5" value="glaucous" value_original="glaucous" />
      </biological_entity>
      <relation from="o8926" id="r1066" name="with" negation="false" src="d0_s5" to="o8927" />
    </statement>
    <statement id="d0_s6">
      <text>Inflorescences racemose, with 5–8 spikes;</text>
      <biological_entity id="o8930" name="inflorescence" name_original="inflorescences" src="d0_s6" type="structure">
        <character is_modifier="false" name="arrangement" src="d0_s6" value="racemose" value_original="racemose" />
