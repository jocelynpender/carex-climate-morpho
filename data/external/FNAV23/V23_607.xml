        <character is_modifier="false" modifier="often; distally" name="coloration" src="d0_s2" value="tinged brown" value_original="tinged brown" />
      </biological_entity>
      <biological_entity id="o3050" name="summit" name_original="summit" src="d0_s2" type="structure">
        <character is_modifier="false" modifier="usually" name="shape" src="d0_s2" value="u--shaped" value_original="u--shaped" />
