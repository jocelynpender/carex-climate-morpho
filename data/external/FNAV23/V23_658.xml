        <character constraint="than inflorescences" constraintid="o28619" is_modifier="false" name="height_or_length_or_size" src="d0_s10" value="shorter" value_original="shorter" />
      </biological_entity>
      <biological_entity id="o28619" name="inflorescence" name_original="inflorescences" src="d0_s10" type="structure" />
    </statement>
    <statement id="d0_s11">
