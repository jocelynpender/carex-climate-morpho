  <description type="morphology">
    <statement id="d0_s0">
      <text>Plants densely cespitose;</text>
      <biological_entity id="o28600" name="whole_organism" name_original="" src="" type="structure">
        <character is_modifier="false" modifier="densely" name="growth_form" src="d0_s0" value="cespitose" value_original="cespitose" />
        <character name="growth_form" value="plant" />
      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>rhizomes sometimes short-prolonged, appearing elongate in old clumps.</text>
      <biological_entity id="o28601" name="rhizome" name_original="rhizomes" src="d0_s1" type="structure">
        <character is_modifier="false" modifier="sometimes" name="length" src="d0_s1" value="short-prolonged" value_original="short-prolonged" />
        <character constraint="in clumps" constraintid="o28602" is_modifier="false" name="shape" src="d0_s1" value="elongate" value_original="elongate" />
      </biological_entity>
      <biological_entity id="o28602" name="clump" name_original="clumps" src="d0_s1" type="structure">
        <character is_modifier="true" name="life_cycle" src="d0_s1" value="old" value_original="old" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Culms 15–120 cm;</text>
      <biological_entity id="o28603" name="culm" name_original="culms" src="d0_s2" type="structure">
        <character char_type="range_value" from="15" from_unit="cm" name="some_measurement" src="d0_s2" to="120" to_unit="cm" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>vegetative culms few, inconspicuous, usually fewer than 15 leaves, not strikingly 3-ranked.</text>
      <biological_entity id="o28604" name="culm" name_original="culms" src="d0_s3" type="structure">
        <character is_modifier="true" name="reproduction" src="d0_s3" value="vegetative" value_original="vegetative" />
        <character is_modifier="false" name="quantity" src="d0_s3" value="few" value_original="few" />
        <character is_modifier="false" name="prominence" src="d0_s3" value="inconspicuous" value_original="inconspicuous" />
        <character char_type="range_value" from="0" modifier="usually" name="quantity" src="d0_s3" to="15" />
        <character is_modifier="false" modifier="not strikingly" name="arrangement" notes="" src="d0_s3" value="3-ranked" value_original="3-ranked" />
      </biological_entity>
      <biological_entity id="o28605" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
    </statement>
    <statement id="d0_s4">
      <text>Leaves: sheaths adaxially white-hyaline, summits U-shaped or sometimes prolonged to 2 mm beyond collar and rounded, smooth;</text>
      <biological_entity id="o28606" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
      <biological_entity id="o28607" name="sheath" name_original="sheaths" src="d0_s4" type="structure">
        <character is_modifier="false" modifier="adaxially" name="coloration" src="d0_s4" value="white-hyaline" value_original="white-hyaline" />
      </biological_entity>
      <biological_entity id="o28608" name="summit" name_original="summits" src="d0_s4" type="structure">
        <character is_modifier="false" name="shape" src="d0_s4" value="u--shaped" value_original="u--shaped" />
        <character is_modifier="false" modifier="sometimes" name="length" src="d0_s4" value="prolonged" value_original="prolonged" />
        <character char_type="range_value" constraint="beyond collar" constraintid="o28609" from="0" from_unit="mm" name="some_measurement" src="d0_s4" to="2" to_unit="mm" />
        <character is_modifier="false" name="shape" notes="" src="d0_s4" value="rounded" value_original="rounded" />
        <character is_modifier="false" name="architecture_or_pubescence_or_relief" src="d0_s4" value="smooth" value_original="smooth" />
      </biological_entity>
      <biological_entity id="o28609" name="collar" name_original="collar" src="d0_s4" type="structure" />
    </statement>
    <statement id="d0_s5">
      <text>distal ligules 2.2–3.5 mm;</text>
      <biological_entity id="o28610" name="leaf" name_original="leaves" src="d0_s5" type="structure" />
      <biological_entity constraint="distal" id="o28611" name="ligule" name_original="ligules" src="d0_s5" type="structure">
        <character char_type="range_value" from="2.2" from_unit="mm" name="some_measurement" src="d0_s5" to="3.5" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s6">
      <text>blades 3–5 per fertile culm, 12–30 cm × 1.5–3.5 mm.</text>
      <biological_entity id="o28612" name="leaf" name_original="leaves" src="d0_s6" type="structure" />
      <biological_entity id="o28613" name="blade" name_original="blades" src="d0_s6" type="structure">
        <character char_type="range_value" constraint="per culm" constraintid="o28614" from="3" name="quantity" src="d0_s6" to="5" />
        <character char_type="range_value" from="12" from_unit="cm" name="length" notes="" src="d0_s6" to="30" to_unit="cm" />
        <character char_type="range_value" from="1.5" from_unit="mm" name="width" notes="" src="d0_s6" to="3.5" to_unit="mm" />
      </biological_entity>
      <biological_entity id="o28614" name="culm" name_original="culm" src="d0_s6" type="structure">
        <character is_modifier="true" name="reproduction" src="d0_s6" value="fertile" value_original="fertile" />
      </biological_entity>
    </statement>
    <statement id="d0_s7">
      <text>Inflorescences open, brown, (1.3–) 2.5–5 (–6.5) cm × 5–18 mm;</text>
      <biological_entity id="o28615" name="inflorescence" name_original="inflorescences" src="d0_s7" type="structure">
        <character is_modifier="false" name="architecture" src="d0_s7" value="open" value_original="open" />
        <character is_modifier="false" name="coloration" src="d0_s7" value="brown" value_original="brown" />
        <character char_type="range_value" from="1.3" from_unit="cm" name="atypical_length" src="d0_s7" to="2.5" to_inclusive="false" to_unit="cm" />
        <character char_type="range_value" from="5" from_inclusive="false" from_unit="cm" name="atypical_length" src="d0_s7" to="6.5" to_unit="cm" />
        <character char_type="range_value" from="2.5" from_unit="cm" name="length" src="d0_s7" to="5" to_unit="cm" />
        <character char_type="range_value" from="5" from_unit="mm" name="width" src="d0_s7" to="18" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s8">
      <text>proximal internode (3–) 6–13 (–23) mm;</text>
      <biological_entity constraint="proximal" id="o28616" name="internode" name_original="internode" src="d0_s8" type="structure">
        <character char_type="range_value" from="3" from_unit="mm" name="atypical_some_measurement" src="d0_s8" to="6" to_inclusive="false" to_unit="mm" />
        <character char_type="range_value" from="13" from_inclusive="false" from_unit="mm" name="atypical_some_measurement" src="d0_s8" to="23" to_unit="mm" />
        <character char_type="range_value" from="6" from_unit="mm" name="some_measurement" src="d0_s8" to="13" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s9">
      <text>2d internode 2–12 mm;</text>
      <biological_entity id="o28617" name="internode" name_original="internode" src="d0_s9" type="structure">
        <character char_type="range_value" from="2" from_unit="mm" name="some_measurement" src="d0_s9" to="12" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s10">
      <text>proximal bracts scalelike or bristlelike, shorter than inflorescences.</text>
