        <character is_modifier="false" name="shape" src="d0_s4" value="short-oblong" value_original="short-oblong" />
        <character is_modifier="false" name="shape" src="d0_s4" value="elongate" value_original="elongate" />
        <character char_type="range_value" from="7" from_unit="mm" name="length" src="d0_s4" to="20" to_unit="mm" />
        <character char_type="range_value" from="4.5" from_unit="mm" name="width" src="d0_s4" to="8" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>proximal spikes often distant, spreading or pendent, very long-pendunculate;</text>
      <biological_entity id="o6684" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure" />
      <biological_entity constraint="proximal" id="o6685" name="spike" name_original="spikes" src="d0_s5" type="structure">
