        <character is_modifier="false" name="orientation" src="d0_s5" value="pendent" value_original="pendent" />
        <character is_modifier="false" modifier="very" name="architecture" src="d0_s5" value="long-pendunculate" value_original="long-pendunculate" />
      </biological_entity>
    </statement>
    <statement id="d0_s6">
      <text>distal spikes erect, pedunculate;</text>
      <biological_entity id="o6686" name="inflorescence" name_original="inflorescences" src="d0_s6" type="structure" />
      <biological_entity constraint="distal" id="o6687" name="spike" name_original="spikes" src="d0_s6" type="structure">
