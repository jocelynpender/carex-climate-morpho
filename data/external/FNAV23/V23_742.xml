        <character is_modifier="false" name="architecture" src="d0_s7" value="pistillate" value_original="pistillate" />
      </biological_entity>
    </statement>
    <statement id="d0_s8">
      <text>terminal spike gynecandrous, 1/4 or less staminate.</text>
      <biological_entity id="o27211" name="inflorescence" name_original="inflorescences" src="d0_s8" type="structure" />
