        <character is_modifier="true" name="position_relational" src="d0_s3" value="exceeding" value_original="exceeding" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>spikes oblong or clavate, 10–25 × 6–8 mm;</text>
      <biological_entity id="o27203" name="inflorescence" name_original="inflorescences" src="d0_s4" type="structure" />
      <biological_entity id="o27204" name="spike" name_original="spikes" src="d0_s4" type="structure">
        <character is_modifier="false" name="shape" src="d0_s4" value="oblong" value_original="oblong" />
