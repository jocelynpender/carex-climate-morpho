    </statement>
    <statement id="d0_s1">
      <text>Culms to 25–60 (–100) cm, smooth.</text>
      <biological_entity id="o27198" name="culm" name_original="culms" src="d0_s1" type="structure">
        <character char_type="range_value" from="60" from_inclusive="false" from_unit="cm" name="atypical_some_measurement" src="d0_s1" to="100" to_unit="cm" />
