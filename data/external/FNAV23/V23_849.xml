        <character char_type="range_value" constraint="at maturity" from="green" name="coloration" src="d0_s14" to="golden green" />
        <character is_modifier="false" modifier="strongly" name="architecture_or_shape" src="d0_s14" value="2-ribbed" value_original="2-ribbed" />
        <character is_modifier="false" modifier="otherwise" name="architecture" src="d0_s14" value="veinless" value_original="veinless" />
        <character name="architecture" src="d0_s14" value="nearly" value_original="nearly" />
        <character is_modifier="false" name="shape" src="d0_s14" value="lanceovoid" value_original="lanceovoid" />
        <character char_type="range_value" from="2.5" from_unit="mm" name="length" src="d0_s14" to="4" to_unit="mm" />
        <character char_type="range_value" from="1" from_unit="mm" name="width" src="d0_s14" to="1.5" to_unit="mm" />
        <character is_modifier="false" name="texture" src="d0_s14" value="membranous" value_original="membranous" />
      </biological_entity>
      <biological_entity id="o29118" name="achene" name_original="achene" src="d0_s14" type="structure" />
      <biological_entity id="o29119" name="base" name_original="base" src="d0_s14" type="structure" />
      <biological_entity id="o29120" name="stipe" name_original="stipe" src="d0_s14" type="structure">
        <character is_modifier="true" name="height_or_length_or_size" src="d0_s14" value="short" value_original="short" />
      </biological_entity>
      <biological_entity id="o29121" name="apex" name_original="apex" src="d0_s14" type="structure">
        <character char_type="range_value" from="tapering" name="shape" src="d0_s14" to="flattened" />
      </biological_entity>
      <biological_entity id="o29122" name="beak" name_original="beak" src="d0_s14" type="structure">
