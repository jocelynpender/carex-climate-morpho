        <character char_type="range_value" from="3.5" from_unit="mm" name="width" notes="" src="d0_s11" to="5.5" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s12">
      <text>Terminal spike staminate or gynecandrous with a few pistillate flowers distally, 25–40 × 2.5–4 mm.</text>
      <biological_entity constraint="terminal" id="o29107" name="spike" name_original="spike" src="d0_s12" type="structure">
        <character is_modifier="false" name="architecture" src="d0_s12" value="staminate" value_original="staminate" />
        <character constraint="with flowers" constraintid="o29108" is_modifier="false" name="reproduction" src="d0_s12" value="gynecandrous" value_original="gynecandrous" />
        <character char_type="range_value" from="25" from_unit="mm" name="length" notes="" src="d0_s12" to="40" to_unit="mm" />
        <character char_type="range_value" from="2.5" from_unit="mm" name="width" notes="" src="d0_s12" to="4" to_unit="mm" />
      </biological_entity>
      <biological_entity id="o29108" name="flower" name_original="flowers" src="d0_s12" type="structure">
        <character is_modifier="true" name="quantity" src="d0_s12" value="few" value_original="few" />
        <character is_modifier="true" name="architecture" src="d0_s12" value="pistillate" value_original="pistillate" />
      </biological_entity>
    </statement>
    <statement id="d0_s13">
      <text>Pistillate scales white-hyaline with broad green midrib, ovateoblong, shorter than mature perigynia, apex cuspidate or with green awn about as long as body of scales, scabrous at tip, otherwise glabrous.</text>
      <biological_entity id="o29109" name="scale" name_original="scales" src="d0_s13" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s13" value="pistillate" value_original="pistillate" />
