  <description type="morphology">
    <statement id="d0_s0">
      <text>Plants densely cespitose, to 200 culms together;</text>
      <biological_entity id="o22776" name="whole_organism" name_original="" src="" type="structure">
        <character is_modifier="false" modifier="densely" name="growth_form" src="d0_s0" value="cespitose" value_original="cespitose" />
        <character char_type="range_value" from="0" name="quantity" src="d0_s0" to="200" />
        <character name="growth_form" value="plant" />
      </biological_entity>
      <biological_entity id="o22777" name="culm" name_original="culms" src="d0_s0" type="structure" />
    </statement>
    <statement id="d0_s1">
      <text>rhizomes appearing elongate only in old clumps.</text>
      <biological_entity id="o22778" name="rhizome" name_original="rhizomes" src="d0_s1" type="structure">
