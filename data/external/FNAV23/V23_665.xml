      <relation from="o22788" id="r2781" name="part_of" negation="false" src="d0_s4" to="o22789" />
    </statement>
    <statement id="d0_s5">
      <text>distal ligules 1.5–5.4 mm;</text>
      <biological_entity id="o22790" name="leaf" name_original="leaves" src="d0_s5" type="structure" />
      <biological_entity constraint="distal" id="o22791" name="ligule" name_original="ligules" src="d0_s5" type="structure">
        <character char_type="range_value" from="1.5" from_unit="mm" name="some_measurement" src="d0_s5" to="5.4" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s6">
      <text>blades 3–6 per fertile culm, 3.5–40 cm × 1.5–4.6 mm.</text>
      <biological_entity id="o22792" name="leaf" name_original="leaves" src="d0_s6" type="structure" />
      <biological_entity id="o22793" name="blade" name_original="blades" src="d0_s6" type="structure">
        <character char_type="range_value" constraint="per culm" constraintid="o22794" from="3" name="quantity" src="d0_s6" to="6" />
        <character char_type="range_value" from="3.5" from_unit="cm" name="length" notes="" src="d0_s6" to="40" to_unit="cm" />
        <character char_type="range_value" from="1.5" from_unit="mm" name="width" notes="" src="d0_s6" to="4.6" to_unit="mm" />
      </biological_entity>
      <biological_entity id="o22794" name="culm" name_original="culm" src="d0_s6" type="structure">
        <character is_modifier="true" name="reproduction" src="d0_s6" value="fertile" value_original="fertile" />
      </biological_entity>
    </statement>
    <statement id="d0_s7">
      <text>Inflorescences open, erect to slightly arching, pale or golden brown, 2.4–5.5 (–6.4) cm × 6.5–21 mm;</text>
      <biological_entity id="o22795" name="inflorescence" name_original="inflorescences" src="d0_s7" type="structure">
