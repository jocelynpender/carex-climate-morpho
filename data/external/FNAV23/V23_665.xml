        <character char_type="range_value" from="13" from_inclusive="false" from_unit="mm" name="atypical_some_measurement" src="d0_s8" to="18" to_unit="mm" />
        <character char_type="range_value" from="4" from_unit="mm" name="some_measurement" src="d0_s8" to="13" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s9">
      <text>2d internode 3–11 mm;</text>
      <biological_entity id="o22797" name="internode" name_original="internode" src="d0_s9" type="structure">
