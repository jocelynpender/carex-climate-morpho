        <character is_modifier="false" name="coloration" src="d0_s7" value="pale" value_original="pale" />
        <character is_modifier="false" name="coloration" src="d0_s7" value="golden brown" value_original="golden brown" />
        <character char_type="range_value" from="5.5" from_inclusive="false" from_unit="cm" name="atypical_length" src="d0_s7" to="6.4" to_unit="cm" />
        <character char_type="range_value" from="2.4" from_unit="cm" name="length" src="d0_s7" to="5.5" to_unit="cm" />
