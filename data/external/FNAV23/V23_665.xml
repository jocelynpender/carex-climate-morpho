      </biological_entity>
      <relation from="o22798" id="r2782" name="with" negation="false" src="d0_s10" to="o22799" />
    </statement>
    <statement id="d0_s11">
      <text>Spikes 4–8 (–10), distinct, ovoid, 10–22 × 4–11 mm, base rounded to tapered, apex rounded to acute.</text>
