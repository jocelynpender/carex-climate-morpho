      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Culms 50–115 cm;</text>
      <biological_entity id="o22780" name="culm" name_original="culms" src="d0_s2" type="structure">
        <character char_type="range_value" from="50" from_unit="cm" name="some_measurement" src="d0_s2" to="115" to_unit="cm" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>vegetative culms few, inconspicuous, usually fewer than 12 leaves, not strikingly 3-ranked.</text>
      <biological_entity id="o22781" name="culm" name_original="culms" src="d0_s3" type="structure">
        <character is_modifier="true" name="reproduction" src="d0_s3" value="vegetative" value_original="vegetative" />
        <character is_modifier="false" name="quantity" src="d0_s3" value="few" value_original="few" />
        <character is_modifier="false" name="prominence" src="d0_s3" value="inconspicuous" value_original="inconspicuous" />
        <character char_type="range_value" from="0" modifier="usually" name="quantity" src="d0_s3" to="12" />
        <character is_modifier="false" modifier="not strikingly" name="arrangement" notes="" src="d0_s3" value="3-ranked" value_original="3-ranked" />
      </biological_entity>
      <biological_entity id="o22782" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
    </statement>
    <statement id="d0_s4">
      <text>Leaves: sheaths glabrous, hyaline sometimes brown tinged band near collar, the summits U-shaped to truncate, ± equaling the base of the blade;</text>
