      <biological_entity id="o22784" name="sheath" name_original="sheaths" src="d0_s4" type="structure">
        <character is_modifier="false" name="pubescence" src="d0_s4" value="glabrous" value_original="glabrous" />
      </biological_entity>
      <biological_entity id="o22785" name="band" name_original="band" src="d0_s4" type="structure">
        <character is_modifier="true" name="coloration" src="d0_s4" value="hyaline" value_original="hyaline" />
        <character is_modifier="true" modifier="sometimes" name="coloration" src="d0_s4" value="brown tinged" value_original="brown tinged" />
      </biological_entity>
      <biological_entity id="o22786" name="collar" name_original="collar" src="d0_s4" type="structure" />
      <biological_entity id="o22787" name="summit" name_original="summits" src="d0_s4" type="structure">
