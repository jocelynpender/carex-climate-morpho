        <character is_modifier="false" modifier="acutely" name="shape" src="d0_s10" value="angled" value_original="angled" />
      </biological_entity>
      <biological_entity id="o15175" name="apex" name_original="apex" src="d0_s10" type="structure">
        <character constraint="to beak" constraintid="o15176" is_modifier="false" name="shape" src="d0_s10" value="tapering" value_original="tapering" />
        <character is_modifier="false" name="pubescence" notes="" src="d0_s10" value="glabrous" value_original="glabrous" />
      </biological_entity>
      <biological_entity id="o15176" name="beak" name_original="beak" src="d0_s10" type="structure" />
    </statement>
    <statement id="d0_s11">
