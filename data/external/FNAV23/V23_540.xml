        <character char_type="range_value" from="veinless sessile" name="architecture" src="d0_s10" to="stipitate" />
        <character char_type="range_value" from="veinless sessile" name="architecture" src="d0_s10" to="stipitate" />
        <character char_type="range_value" from="ovate" name="shape" src="d0_s10" to="narrowly ovate" />
        <character constraint="in cross-section" constraintid="o15172" is_modifier="false" name="shape" src="d0_s10" value="planoconvex" value_original="planoconvex" />
      </biological_entity>
      <biological_entity id="o15172" name="cross-section" name_original="cross-section" src="d0_s10" type="structure" />
      <biological_entity id="o15173" name="base" name_original="base" src="d0_s10" type="structure">
