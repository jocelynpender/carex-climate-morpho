        <character is_modifier="false" name="fusion" src="d0_s4" value="distinct" value_original="distinct" />
      </biological_entity>
      <biological_entity constraint="proximal" id="o13807" name="spike" name_original="spikes" src="d0_s4" type="structure">
        <character is_modifier="false" name="arrangement" src="d0_s4" value="separate" value_original="separate" />
