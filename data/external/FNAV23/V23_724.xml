      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>proximal long-pendunculate;</text>
      <biological_entity id="o13808" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure" />
      <biological_entity constraint="proximal" id="o13809" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure">
