      </biological_entity>
    </statement>
    <statement id="d0_s6">
      <text>lateral 3–6 spikes pistillate or, rarely, gynecandrous;</text>
      <biological_entity id="o13810" name="inflorescence" name_original="inflorescences" src="d0_s6" type="structure">
