        <character is_modifier="false" name="reproduction" src="d0_s6" value="gynecandrous" value_original="gynecandrous" />
        <character is_modifier="false" modifier="wholly" name="architecture" src="d0_s6" value="staminate" value_original="staminate" />
      </biological_entity>
    </statement>
    <statement id="d0_s7">
      <text>Pistillate scales light to dark-brown, distal margins broadly hyaline, ovate, as long and as wide as perigynia, midvein lighter colored than body, conspicuous, often raised, prominent, distally scabrous, apex rounded, acute or mucronate.</text>
