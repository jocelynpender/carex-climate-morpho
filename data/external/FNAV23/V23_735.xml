        <character constraint="than body" constraintid="o9701" is_modifier="false" name="coloration" src="d0_s7" value="lighter colored" value_original="lighter colored" />
        <character is_modifier="false" name="prominence" src="d0_s7" value="conspicuous" value_original="conspicuous" />
        <character is_modifier="false" modifier="often" name="prominence" src="d0_s7" value="raised" value_original="raised" />
