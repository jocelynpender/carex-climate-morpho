        <character is_modifier="false" name="arrangement" src="d0_s9" value="separated" value_original="separated" />
      </biological_entity>
      <biological_entity id="o27815" name="perigynium" name_original="perigynia" src="d0_s9" type="structure">
        <character char_type="range_value" from="3" is_modifier="true" name="atypical_quantity" src="d0_s9" to="4" to_inclusive="false" />
        <character char_type="range_value" from="4" is_modifier="true" name="quantity" src="d0_s9" to="13" />
      </biological_entity>
      <relation from="o27814" id="r3437" name="with" negation="false" src="d0_s9" to="o27815" />
    </statement>
    <statement id="d0_s10">
      <text>staminate spikes 8–24 × 1.3–4 mm.</text>
      <biological_entity id="o27816" name="spike" name_original="spikes" src="d0_s10" type="structure" />
      <biological_entity id="o27817" name="spike" name_original="spikes" src="d0_s10" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s10" value="staminate" value_original="staminate" />
