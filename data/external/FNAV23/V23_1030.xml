      </biological_entity>
      <relation from="o27819" id="r3438" name="with" negation="false" src="d0_s11" to="o27820" />
    </statement>
    <statement id="d0_s12">
      <text>staminate scales elliptic to ovate, 2.9–5.3 × 1.1–1.9 mm, apex obtuse or acute to acuminate.</text>
      <biological_entity id="o27823" name="scale" name_original="scales" src="d0_s12" type="structure" />
      <biological_entity id="o27824" name="scale" name_original="scales" src="d0_s12" type="structure">
