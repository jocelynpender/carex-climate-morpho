        <character is_modifier="false" name="coloration" src="d0_s11" value="dark reddish-brown" value_original="dark reddish-brown" />
        <character is_modifier="false" name="shape" notes="" src="d0_s11" value="ovate" value_original="ovate" />
        <character char_type="range_value" from="2" from_unit="mm" name="length" src="d0_s11" to="4" to_unit="mm" />
        <character char_type="range_value" from="1.3" from_unit="mm" name="width" src="d0_s11" to="2.8" to_unit="mm" />
