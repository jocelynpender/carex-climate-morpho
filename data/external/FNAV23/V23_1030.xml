        <character char_type="range_value" from="1.3" from_unit="mm" name="width" src="d0_s10" to="4" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s11">
      <text>Scales: pistillate scales dark reddish-brown, with narrow white margins, ovate, 2–4 × 1.3–2.8 mm, equaling perigynium body, apex obtuse or acute to acuminate;</text>
      <biological_entity id="o27818" name="scale" name_original="scales" src="d0_s11" type="structure" />
      <biological_entity id="o27819" name="scale" name_original="scales" src="d0_s11" type="structure">
