      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>Culms 15–30 cm, distally finely scabrous.</text>
      <biological_entity id="o16145" name="culm" name_original="culms" src="d0_s1" type="structure">
        <character char_type="range_value" from="15" from_unit="cm" name="some_measurement" src="d0_s1" to="30" to_unit="cm" />
