      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Leaves 3–4 mm wide.</text>
      <biological_entity id="o16146" name="leaf" name_original="leaves" src="d0_s2" type="structure">
        <character char_type="range_value" from="3" from_unit="mm" name="width" src="d0_s2" to="4" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>Inflorescences: proximal bracts usually shorter than or, occasionally, equaling inflorescences;</text>
      <biological_entity id="o16147" name="inflorescence" name_original="inflorescences" src="d0_s3" type="structure" />
