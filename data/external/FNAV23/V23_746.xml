        <character is_modifier="false" name="shape" src="d0_s4" value="short-oblong" value_original="short-oblong" />
        <character char_type="range_value" from="10" from_unit="mm" name="length" src="d0_s4" to="12" to_unit="mm" />
        <character char_type="range_value" from="5" from_unit="mm" name="width" src="d0_s4" to="8" to_unit="mm" />
      </biological_entity>
      <biological_entity constraint="terminal or proximal" id="o16152" name="spike" name_original="spike" src="d0_s4" type="structure">
