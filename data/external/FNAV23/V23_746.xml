      </biological_entity>
      <relation from="o16151" id="r1977" name="forming" negation="false" src="d0_s4" to="o16152" />
    </statement>
    <statement id="d0_s5">
      <text>lateral 2–3 spikes pistillate;</text>
      <biological_entity id="o16153" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure">
        <character is_modifier="false" name="position" src="d0_s5" value="lateral" value_original="lateral" />
