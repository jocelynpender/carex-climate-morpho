      </biological_entity>
    </statement>
    <statement id="d0_s9">
      <text>Proximal pistillate scales deciduous before perigyinia, apex obtuse to acute.</text>
      <biological_entity constraint="proximal" id="o14643" name="spike" name_original="spikes" src="d0_s9" type="structure" />
      <biological_entity id="o14644" name="scale" name_original="scales" src="d0_s9" type="structure">
