        <character is_modifier="false" name="architecture" src="d0_s0" value="short-rhizomatous" value_original="short-rhizomatous" />
        <character name="growth_form" value="plant" />
      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>rhizomes dark-brown, stout, sometimes inconspicuous.</text>
      <biological_entity id="o14629" name="rhizome" name_original="rhizomes" src="d0_s1" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s1" value="dark-brown" value_original="dark-brown" />
        <character is_modifier="false" name="fragility_or_size" src="d0_s1" value="stout" value_original="stout" />
        <character is_modifier="false" modifier="sometimes" name="prominence" src="d0_s1" value="inconspicuous" value_original="inconspicuous" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Culms brown at base.</text>
      <biological_entity id="o14630" name="culm" name_original="culms" src="d0_s2" type="structure">
        <character constraint="at base" constraintid="o14631" is_modifier="false" name="coloration" src="d0_s2" value="brown" value_original="brown" />
      </biological_entity>
      <biological_entity id="o14631" name="base" name_original="base" src="d0_s2" type="structure" />
    </statement>
    <statement id="d0_s3">
      <text>Leaves: basal sheaths not fibrous;</text>
      <biological_entity id="o14632" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
      <biological_entity constraint="basal" id="o14633" name="sheath" name_original="sheaths" src="d0_s3" type="structure">
        <character is_modifier="false" modifier="not" name="texture" src="d0_s3" value="fibrous" value_original="fibrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>sheath fronts membranous;</text>
      <biological_entity id="o14634" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
      <biological_entity constraint="sheath" id="o14635" name="front" name_original="fronts" src="d0_s4" type="structure">
        <character is_modifier="false" name="texture" src="d0_s4" value="membranous" value_original="membranous" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>blades V-shaped in cross-section when young, 1.5–4 mm wide, glabrous.</text>
      <biological_entity id="o14636" name="leaf" name_original="leaves" src="d0_s5" type="structure" />
      <biological_entity id="o14637" name="blade" name_original="blades" src="d0_s5" type="structure">
        <character constraint="in cross-section" constraintid="o14638" is_modifier="false" name="shape" src="d0_s5" value="v--shaped" value_original="v--shaped" />
