      </biological_entity>
      <biological_entity constraint="proximal" id="o14648" name="perigynium" name_original="perigynia" src="d0_s10" type="structure">
        <character constraint="at base" constraintid="o14649" is_modifier="false" modifier="somewhat" name="orientation" src="d0_s10" value="reflexed" value_original="reflexed" />
        <character is_modifier="false" name="architecture" src="d0_s10" value="stipitate" value_original="stipitate" />
      </biological_entity>
      <biological_entity id="o14649" name="base" name_original="base" src="d0_s10" type="structure">
        <character is_modifier="true" name="life_cycle" src="d0_s10" value="maturity" value_original="maturity" />
        <character is_modifier="true" name="architecture" src="d0_s10" value="veinless" value_original="veinless" />
        <character char_type="range_value" from="lanceolate" is_modifier="true" name="shape" src="d0_s10" to="broadly lanceolate rounded-trigonous" />
        <character char_type="range_value" from="3" from_unit="mm" is_modifier="true" name="some_measurement" src="d0_s10" to="5" to_unit="mm" />
        <character is_modifier="false" name="shape" src="d0_s10" value="rounded" value_original="rounded" />
      </biological_entity>
      <biological_entity id="o14650" name="margin" name_original="margins" src="d0_s10" type="structure">
        <character is_modifier="false" name="shape" src="d0_s10" value="rounded" value_original="rounded" />
      </biological_entity>
      <biological_entity id="o14651" name="apex" name_original="apex" src="d0_s10" type="structure">
        <character constraint="to beak" constraintid="o14652" is_modifier="false" name="shape" src="d0_s10" value="tapering" value_original="tapering" />
