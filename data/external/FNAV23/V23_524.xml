      </biological_entity>
      <relation from="o13342" id="r1645" name="with" negation="false" src="d0_s3" to="o13343" />
    </statement>
    <statement id="d0_s4">
      <text>blades V-shaped in cross-section when young, glabrous.</text>
      <biological_entity id="o13344" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
      <biological_entity id="o13345" name="blade" name_original="blades" src="d0_s4" type="structure">
