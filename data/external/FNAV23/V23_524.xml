      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>sheath fronts herbacous, not differentiated from backs, distal leaves with sheath fronts green-veined;</text>
      <biological_entity id="o13339" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
      <biological_entity constraint="sheath" id="o13340" name="front" name_original="fronts" src="d0_s3" type="structure">
