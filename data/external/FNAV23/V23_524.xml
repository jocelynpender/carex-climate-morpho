      </biological_entity>
      <biological_entity id="o13341" name="back" name_original="backs" src="d0_s3" type="structure" />
      <biological_entity constraint="distal" id="o13342" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
      <biological_entity constraint="sheath" id="o13343" name="front" name_original="fronts" src="d0_s3" type="structure">
