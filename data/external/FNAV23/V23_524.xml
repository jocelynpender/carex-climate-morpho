        <character name="growth_form" value="plant" />
      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>Culms brown at base.</text>
      <biological_entity id="o13335" name="culm" name_original="culms" src="d0_s1" type="structure">
        <character constraint="at base" constraintid="o13336" is_modifier="false" name="coloration" src="d0_s1" value="brown" value_original="brown" />
