        <character is_modifier="false" modifier="when young" name="pubescence" src="d0_s4" value="glabrous" value_original="glabrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>Inflorescences racemose with 9–37 spikes;</text>
      <biological_entity id="o13347" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure">
        <character constraint="with spikes" constraintid="o13348" is_modifier="false" name="arrangement" src="d0_s5" value="racemose" value_original="racemose" />
