      <biological_entity id="o6352" name="cross-section" name_original="cross-section" src="d0_s4" type="structure">
        <character is_modifier="false" modifier="when young" name="shape" src="d0_s4" value="filiform" value_original="filiform" />
        <character is_modifier="false" name="shape" src="d0_s4" value="channeled" value_original="channeled" />
