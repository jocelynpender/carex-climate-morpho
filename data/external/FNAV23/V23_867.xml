    </statement>
    <statement id="d0_s16">
      <text>beak bidentate, slender, 2–3 mm, teeth 1 mm.</text>
      <biological_entity id="o324" name="beak" name_original="beak" src="d0_s16" type="structure">
        <character is_modifier="false" name="shape" src="d0_s16" value="bidentate" value_original="bidentate" />
        <character is_modifier="false" name="size" src="d0_s16" value="slender" value_original="slender" />
        <character char_type="range_value" from="2" from_unit="mm" name="some_measurement" src="d0_s16" to="3" to_unit="mm" />
