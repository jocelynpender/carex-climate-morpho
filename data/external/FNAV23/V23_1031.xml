        <character char_type="range_value" from="0.9" from_unit="mm" name="some_measurement" notes="" src="d0_s15" to="1.6" to_unit="mm" />
        <character is_modifier="false" modifier="weakly" name="architecture_or_shape" src="d0_s15" value="ciliate-serrulate" value_original="ciliate-serrulate" />
      </biological_entity>
      <biological_entity id="o20439" name="apex" name_original="apex" src="d0_s15" type="structure">
        <character is_modifier="true" name="coloration" src="d0_s15" value="reddish-brown tinge" value_original="reddish-brown tinge" />
      </biological_entity>
      <biological_entity constraint="apical" id="o20440" name="tooth" name_original="teeth" src="d0_s15" type="structure">
