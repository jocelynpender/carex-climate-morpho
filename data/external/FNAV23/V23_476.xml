        <character is_modifier="false" modifier="when young" name="pubescence" src="d0_s4" value="glabrous" value_original="glabrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>Inflorescences usually paniculate, usually condensed, with 8–20+ spikes, usually more than 15 spikes;</text>
      <biological_entity id="o31358" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure">
