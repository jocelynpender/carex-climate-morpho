      </biological_entity>
      <biological_entity id="o31360" name="spike" name_original="spikes" src="d0_s5" type="structure" />
      <relation from="o31358" id="r3877" name="with" negation="false" src="d0_s5" to="o31359" />
    </statement>
    <statement id="d0_s6">
