      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>sheath fronts membranous, often dotted red, transversely rugose;</text>
      <biological_entity id="o31353" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
      <biological_entity constraint="sheath" id="o31354" name="front" name_original="fronts" src="d0_s3" type="structure">
        <character is_modifier="false" name="texture" src="d0_s3" value="membranous" value_original="membranous" />
        <character is_modifier="false" modifier="often" name="coloration" src="d0_s3" value="dotted red" value_original="dotted red" />
        <character is_modifier="false" modifier="transversely" name="relief" src="d0_s3" value="rugose" value_original="rugose" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>blades V-shaped in cross-section when young, glabrous.</text>
      <biological_entity id="o31355" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
      <biological_entity id="o31356" name="blade" name_original="blades" src="d0_s4" type="structure">
