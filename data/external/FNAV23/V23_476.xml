        <character is_modifier="false" modifier="sometimes" name="architecture" src="d0_s0" value="long-rhizomatous" value_original="long-rhizomatous" />
        <character name="growth_form" value="plant" />
      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>Culms brown at base, usually not more than 1 mm wide distally.</text>
      <biological_entity id="o31349" name="culm" name_original="culms" src="d0_s1" type="structure">
        <character constraint="at base" constraintid="o31350" is_modifier="false" name="coloration" src="d0_s1" value="brown" value_original="brown" />
        <character char_type="range_value" from="1" from_unit="mm" modifier="usually not; not; distally" name="width" notes="" src="d0_s1" upper_restricted="false" />
      </biological_entity>
      <biological_entity id="o31350" name="base" name_original="base" src="d0_s1" type="structure" />
    </statement>
    <statement id="d0_s2">
      <text>Leaves: basal sheaths fibrous;</text>
      <biological_entity id="o31351" name="leaf" name_original="leaves" src="d0_s2" type="structure" />
      <biological_entity constraint="basal" id="o31352" name="sheath" name_original="sheaths" src="d0_s2" type="structure">
