    </statement>
    <statement id="d0_s17">
      <text>Achenes stipitate, 1.9–2.5 × 1–1.5 mm, stipe 0.5–1.5 mm.</text>
      <biological_entity id="o820" name="achene" name_original="achenes" src="d0_s17" type="structure">
        <character is_modifier="false" name="architecture" src="d0_s17" value="stipitate" value_original="stipitate" />
        <character char_type="range_value" from="1.9" from_unit="mm" name="length" src="d0_s17" to="2.5" to_unit="mm" />
        <character char_type="range_value" from="1" from_unit="mm" name="width" src="d0_s17" to="1.5" to_unit="mm" />
      </biological_entity>
