        <character is_modifier="false" name="architecture" src="d0_s12" value="sessile" value_original="sessile" />
        <character is_modifier="false" modifier="commonly" name="architecture" src="d0_s12" value="pedunculate" value_original="pedunculate" />
        <character char_type="range_value" from="10" from_unit="mm" name="length" src="d0_s12" to="27" to_unit="mm" />
        <character char_type="range_value" from="2.2" from_unit="mm" name="width" src="d0_s12" to="6" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s13">
      <text>Pistillate scales white-hyaline at base and along margins, suffused with reddish-brown near apex, with broad green midrib, broadly ovate, nearly as long as perigynia, apex truncate or rounded with ciliate green awn 1 mm, otherwise glabrous.</text>
      <biological_entity id="o31812" name="scale" name_original="scales" src="d0_s13" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s13" value="pistillate" value_original="pistillate" />
        <character constraint="at base" constraintid="o31813" is_modifier="false" name="coloration" src="d0_s13" value="white-hyaline" value_original="white-hyaline" />
        <character constraint="with apex" constraintid="o31815" is_modifier="false" name="coloration" notes="" src="d0_s13" value="suffused" value_original="suffused" />
