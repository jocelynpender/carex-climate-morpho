    <statement id="d0_s14">
      <text>Perigynia golden green, sometimes red dotted, 2-ribbed and finely 8–10-veined, substipitate, loosely enveloping achene, ellipsoid, 2.5–4.2 × 1–1.8 mm, membranous, apex tapering to beak, entire surface pubescent, often densely so, except in rare glabrous form;</text>
      <biological_entity id="o31820" name="perigynium" name_original="perigynia" src="d0_s14" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s14" value="golden green" value_original="golden green" />
        <character is_modifier="false" modifier="sometimes" name="coloration" src="d0_s14" value="red dotted" value_original="red dotted" />
        <character is_modifier="false" name="architecture_or_shape" src="d0_s14" value="2-ribbed" value_original="2-ribbed" />
        <character is_modifier="false" modifier="finely" name="architecture" src="d0_s14" value="8-10-veined" value_original="8-10-veined" />
        <character is_modifier="false" name="architecture" src="d0_s14" value="substipitate" value_original="substipitate" />
        <character is_modifier="false" name="shape" src="d0_s14" value="ellipsoid" value_original="ellipsoid" />
