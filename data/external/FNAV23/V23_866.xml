    </statement>
    <statement id="d0_s16">
      <text>Achenes sessile, 2–3.1 × 1–1.7 mm. 2n = 70, 72.</text>
      <biological_entity id="o31827" name="achene" name_original="achenes" src="d0_s16" type="structure">
        <character is_modifier="false" name="architecture" src="d0_s16" value="sessile" value_original="sessile" />
        <character char_type="range_value" from="2" from_unit="mm" name="length" src="d0_s16" to="3.1" to_unit="mm" />
