        <character is_modifier="false" name="texture" src="d0_s14" value="membranous" value_original="membranous" />
      </biological_entity>
      <biological_entity id="o31821" name="achene" name_original="achene" src="d0_s14" type="structure" />
      <biological_entity id="o31822" name="apex" name_original="apex" src="d0_s14" type="structure">
        <character constraint="to beak" constraintid="o31823" is_modifier="false" name="shape" src="d0_s14" value="tapering" value_original="tapering" />
      </biological_entity>
      <biological_entity id="o31823" name="beak" name_original="beak" src="d0_s14" type="structure" />
      <biological_entity id="o31824" name="surface" name_original="surface" src="d0_s14" type="structure">
