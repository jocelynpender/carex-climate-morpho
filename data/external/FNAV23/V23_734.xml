        <character is_modifier="false" name="height_or_length_or_size" src="d0_s8" value="shorter" value_original="shorter" />
        <character is_modifier="false" name="variability" src="d0_s8" value="equaling" value_original="equaling" />
      </biological_entity>
      <biological_entity id="o17568" name="perigynium" name_original="perigynia" src="d0_s8" type="structure" />
