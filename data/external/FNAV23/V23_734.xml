      <relation from="o17564" id="r2125" name="overtopping" negation="false" src="d0_s7" to="o17565" />
    </statement>
    <statement id="d0_s8">
      <text>Pistillate scales dark-brown or black, margins hyaline, ovate, shorter or equaling, as broad as perigynia, midvein lighter colored than body, conspicuous, sometimes raised, prominent, mucronate.</text>
      <biological_entity id="o17566" name="scale" name_original="scales" src="d0_s8" type="structure">
