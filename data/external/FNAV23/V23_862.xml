      <text>Cedar swamps, rich mesic deciduous or mixed conifer-hardwood forests, mixed coniferous forests and margins, mesic meadows, often associated with calcareous soils</text>
      <biological_entity id="hab_o0" name="whole_organism" name_original="" type="structure">
        <character name="habitat" value="cedar swamps" />
        <character name="habitat" value="rich mesic deciduous" />
        <character name="habitat" value="mixed conifer-hardwood forests" />
        <character name="habitat" value="mixed coniferous forests" />
