      <relation from="o20403" id="r2461" name="with" negation="false" src="d0_s14" to="o20404" />
    </statement>
    <statement id="d0_s15">
      <text>beak minutely bidentate, less than 0.5 mm.</text>
      <biological_entity id="o20407" name="beak" name_original="beak" src="d0_s15" type="structure">
        <character is_modifier="false" modifier="minutely" name="shape" src="d0_s15" value="bidentate" value_original="bidentate" />
