      </biological_entity>
    </statement>
    <statement id="d0_s16">
      <text>Achenes substipitate, 2–2.5 × 1.4–1.5 mm.</text>
      <biological_entity id="o20408" name="achene" name_original="achenes" src="d0_s16" type="structure">
