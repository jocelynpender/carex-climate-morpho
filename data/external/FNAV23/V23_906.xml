        <character char_type="range_value" from="1.5" from_unit="mm" modifier="when young" name="width" src="d0_s3" to="2.8" to_unit="mm" />
        <character is_modifier="false" modifier="usually" name="shape_or_vernation" src="d0_s3" value="involute" value_original="involute" />
        <character is_modifier="false" modifier="proximally" name="pubescence" src="d0_s3" value="puberulent" value_original="puberulent" />
