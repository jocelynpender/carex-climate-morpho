    <statement id="d0_s4">
      <text>Inflorescences: proximal bracts sheathless or sheath not more than 2 (–5) mm;</text>
      <biological_entity id="o30841" name="inflorescence" name_original="inflorescences" src="d0_s4" type="structure" />
