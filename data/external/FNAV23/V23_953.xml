      <biological_entity id="o2380" name="stigma" name_original="stigmas" src="d0_s13" type="structure">
        <character name="quantity" src="d0_s13" value="3" value_original="3" />
      </biological_entity>
    </statement>
    <statement id="d0_s14">
      <text>Achenes trigonous, smaller than bodies of perigynia;</text>
      <biological_entity id="o2381" name="achene" name_original="achenes" src="d0_s14" type="structure">
