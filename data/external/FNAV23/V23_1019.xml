    <statement id="d0_s10">
      <text>staminate spikes 5–14 × 0.5–1.8 mm.</text>
      <biological_entity id="o29449" name="spike" name_original="spikes" src="d0_s10" type="structure" />
      <biological_entity id="o29450" name="spike" name_original="spikes" src="d0_s10" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s10" value="staminate" value_original="staminate" />
