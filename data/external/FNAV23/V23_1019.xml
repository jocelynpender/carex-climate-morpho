      <biological_entity id="o29454" name="apex" name_original="apex" src="d0_s11" type="structure">
        <character is_modifier="false" modifier="usually" name="shape" src="d0_s11" value="acuminate" value_original="acuminate" />
        <character is_modifier="false" modifier="sometimes" name="shape" src="d0_s11" value="acute" value_original="acute" />
