        <character char_type="range_value" from="pale" name="coloration" src="d0_s4" to="bright green" />
        <character constraint="than culms" constraintid="o29433" is_modifier="false" name="size_or_length" src="d0_s4" value="shorter to longer" value_original="shorter to longer" />
        <character constraint="than culms" constraintid="o29434" is_modifier="false" name="size_or_length" src="d0_s4" value="shorter to longer" value_original="shorter to longer" />
        <character char_type="range_value" from="0.8" from_unit="mm" name="width" src="d0_s4" to="2.5" to_unit="mm" />
        <character is_modifier="false" name="growth_form_or_texture" src="d0_s4" value="herbaceous" value_original="herbaceous" />
        <character is_modifier="false" modifier="abaxially" name="pubescence_or_relief" src="d0_s4" value="smooth to scabrous" value_original="smooth to scabrous" />
        <character char_type="range_value" from="papillose" modifier="adaxially" name="relief" src="d0_s4" to="scabrous" />
      </biological_entity>
      <biological_entity id="o29433" name="culm" name_original="culms" src="d0_s4" type="structure" />
      <biological_entity id="o29434" name="culm" name_original="culms" src="d0_s4" type="structure" />
    </statement>
    <statement id="d0_s5">
      <text>Inflorescences with both staminate and pistillate spikes;</text>
      <biological_entity id="o29435" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure" />
      <biological_entity id="o29436" name="spike" name_original="spikes" src="d0_s5" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s5" value="staminate" value_original="staminate" />
        <character is_modifier="true" name="architecture" src="d0_s5" value="pistillate" value_original="pistillate" />
