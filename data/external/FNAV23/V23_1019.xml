      <relation from="o29435" id="r3639" name="with" negation="false" src="d0_s5" to="o29436" />
    </statement>
    <statement id="d0_s6">
      <text>peduncles of staminate spikes 0.4–9.9 mm;</text>
      <biological_entity id="o29437" name="peduncle" name_original="peduncles" src="d0_s6" type="structure" constraint="spike" constraint_original="spike; spike">
        <character char_type="range_value" from="0.4" from_unit="mm" name="some_measurement" src="d0_s6" to="9.9" to_unit="mm" />
      </biological_entity>
      <biological_entity id="o29438" name="spike" name_original="spikes" src="d0_s6" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s6" value="staminate" value_original="staminate" />
      </biological_entity>
      <relation from="o29437" id="r3640" name="part_of" negation="false" src="d0_s6" to="o29438" />
    </statement>
    <statement id="d0_s7">
      <text>proximal nonbasal bracts leaflike, shorter than inflorescences.</text>
      <biological_entity constraint="proximal nonbasal" id="o29439" name="bract" name_original="bracts" src="d0_s7" type="structure">
        <character is_modifier="false" name="architecture_or_shape" src="d0_s7" value="leaflike" value_original="leaflike" />
        <character constraint="than inflorescences" constraintid="o29440" is_modifier="false" name="height_or_length_or_size" src="d0_s7" value="shorter" value_original="shorter" />
