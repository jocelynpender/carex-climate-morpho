      </biological_entity>
    </statement>
    <statement id="d0_s9">
      <text>cauline spikes overlapping, proximal 2 spikes usually separated by less than 7 mm, with 4–14 perigynia;</text>
      <biological_entity id="o29444" name="spike" name_original="spikes" src="d0_s9" type="structure" />
