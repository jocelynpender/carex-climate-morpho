      <biological_entity id="o29447" name="spike" name_original="spikes" src="d0_s9" type="structure">
        <character constraint="by 0-7 mm" is_modifier="false" modifier="usually" name="arrangement" src="d0_s9" value="separated" value_original="separated" />
