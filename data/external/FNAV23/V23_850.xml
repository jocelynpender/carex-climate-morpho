        <character char_type="range_value" from="2" name="quantity" src="d0_s11" to="4" />
        <character constraint="per node" constraintid="o31696" name="quantity" src="d0_s11" value="1" value_original="1" />
        <character is_modifier="false" modifier="well" name="arrangement" notes="" src="d0_s11" value="separated" value_original="separated" />
        <character is_modifier="false" name="position_or_shape" src="d0_s11" value="distal" value_original="distal" />
        <character name="quantity" src="d0_s11" value="2" value_original="2" />
        <character is_modifier="false" modifier="when young" name="orientation" notes="" src="d0_s11" value="erect" value_original="erect" />
