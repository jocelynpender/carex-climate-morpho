        <character is_modifier="true" name="life_cycle" src="d0_s2" value="maturity" value_original="maturity" />
        <character is_modifier="true" name="height_or_length_or_size" src="d0_s2" value="shorter" value_original="shorter" />
        <character is_modifier="true" name="width" src="d0_s2" value="thick" value_original="thick" />
        <character char_type="range_value" constraint="at " constraintid="o31668" from="1.5" from_unit="mm" name="some_measurement" src="d0_s2" to="3" to_unit="mm" />
      </biological_entity>
