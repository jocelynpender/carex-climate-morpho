        <character is_modifier="true" name="life_cycle" src="d0_s2" value="maturity" value_original="maturity" />
        <character is_modifier="true" name="height_or_length_or_size" src="d0_s2" value="shorter" value_original="shorter" />
        <character is_modifier="true" name="width" src="d0_s2" value="thick" value_original="thick" />
        <character is_modifier="false" name="pubescence" src="d0_s2" value="glabrous" value_original="glabrous" />
        <character constraint="at " constraintid="o31669" is_modifier="false" name="pubescence" src="d0_s2" value="pubescent" value_original="pubescent" />
      </biological_entity>
      <biological_entity id="o31669" name="angle" name_original="angles" src="d0_s2" type="structure">
