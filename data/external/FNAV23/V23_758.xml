      <biological_entity id="o3386" name="beak" name_original="beak" src="d0_s11" type="structure" />
    </statement>
    <statement id="d0_s12">
      <text>beak 0–0.5 mm, mouth entire or emarginate.</text>
      <biological_entity id="o3387" name="beak" name_original="beak" src="d0_s12" type="structure">
