        <character is_modifier="false" name="architecture" src="d0_s11" value="stipitate" value_original="stipitate" />
        <character is_modifier="false" name="architecture" src="d0_s11" value="sessile" value_original="sessile" />
        <character is_modifier="false" modifier="broadly" name="shape" src="d0_s11" value="elliptic" value_original="elliptic" />
        <character is_modifier="false" name="shape" src="d0_s11" value="ovate" value_original="ovate" />
        <character constraint="in cross-section" constraintid="o3383" is_modifier="false" name="shape" src="d0_s11" value="rounded-trigonous" value_original="rounded-trigonous" />
      </biological_entity>
      <biological_entity constraint="abaxial" id="o3381" name="face" name_original="face" src="d0_s11" type="structure" />
      <biological_entity constraint="adaxial" id="o3382" name="face" name_original="face" src="d0_s11" type="structure" />
      <biological_entity id="o3383" name="cross-section" name_original="cross-section" src="d0_s11" type="structure" />
