        <character is_modifier="false" modifier="often" name="prominence" src="d0_s7" value="raised" value_original="raised" />
        <character is_modifier="false" name="prominence" src="d0_s7" value="prominent" value_original="prominent" />
      </biological_entity>
      <biological_entity id="o29827" name="body" name_original="body" src="d0_s7" type="structure" />
      <biological_entity id="o29828" name="apex" name_original="apex" src="d0_s7" type="structure">
