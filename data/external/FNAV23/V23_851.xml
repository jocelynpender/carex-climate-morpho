        <character char_type="range_value" from="green" name="coloration" src="d0_s13" to="dark olive-green" />
        <character is_modifier="false" modifier="copiously" name="coloration" src="d0_s13" value="red dotted" value_original="red dotted" />
        <character constraint="with veins" constraintid="o28133" is_modifier="false" name="architecture_or_shape" src="d0_s13" value="2-ribbed" value_original="2-ribbed" />
        <character is_modifier="false" name="shape" src="d0_s13" value="ellipsoid-ovoid" value_original="ellipsoid-ovoid" />
        <character char_type="range_value" from="3.5" from_unit="mm" name="length" src="d0_s13" to="5" to_unit="mm" />
        <character char_type="range_value" from="1.7" from_unit="mm" name="width" src="d0_s13" to="2" to_unit="mm" />
        <character is_modifier="false" name="texture" src="d0_s13" value="membranous" value_original="membranous" />
      </biological_entity>
      <biological_entity id="o28133" name="vein" name_original="veins" src="d0_s13" type="structure">
        <character char_type="range_value" from="6" is_modifier="true" name="quantity" src="d0_s13" to="10" />
        <character is_modifier="true" modifier="almost equally" name="prominence" src="d0_s13" value="prominent" value_original="prominent" />
      </biological_entity>
      <biological_entity id="o28134" name="achene" name_original="achenes" src="d0_s13" type="structure" />
      <biological_entity id="o28135" name="base" name_original="base" src="d0_s13" type="structure">
        <character is_modifier="false" name="shape" src="d0_s13" value="acute" value_original="acute" />
        <character is_modifier="false" name="shape" src="d0_s13" value="with short stipe" value_original="with short stipe" />
      </biological_entity>
      <biological_entity id="o28136" name="stipe" name_original="stipe" src="d0_s13" type="structure">
