        <character name="habitat" value="near streams" />
        <character name="habitat" value="swampy areas" />
        <character name="habitat" value="forests" modifier="upland" constraint="on sandy soils , wooded bluffs , often associated with calcareous soils" />
        <character name="habitat" value="sandy soils" modifier="on" />
        <character name="habitat" value="wooded bluffs" />
