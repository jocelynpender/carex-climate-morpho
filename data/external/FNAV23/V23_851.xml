      <relation from="o28135" id="r3468" name="with" negation="false" src="d0_s13" to="o28136" />
    </statement>
    <statement id="d0_s14">
      <text>beak minutely bidentate, 0.4–0.7 mm.</text>
      <biological_entity id="o28139" name="beak" name_original="beak" src="d0_s14" type="structure">
        <character is_modifier="false" modifier="minutely" name="shape" src="d0_s14" value="bidentate" value_original="bidentate" />
