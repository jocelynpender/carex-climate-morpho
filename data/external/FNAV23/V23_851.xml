      </biological_entity>
    </statement>
    <statement id="d0_s15">
      <text>Achenes distinctly stipitate, 1.7–2.5 × 1–1.5 mm, stipe to 0.7 mm.</text>
      <biological_entity id="o28140" name="achene" name_original="achenes" src="d0_s15" type="structure">
