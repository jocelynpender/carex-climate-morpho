        <character is_modifier="false" modifier="less often" name="pubescence" src="d0_s13" value="pubescent" value_original="pubescent" />
      </biological_entity>
      <biological_entity id="o28138" name="beak" name_original="beak" src="d0_s13" type="structure" />
