        <character constraint="than mature perigynia" constraintid="o24367" is_modifier="false" name="height_or_length_or_size" src="d0_s12" value="shorter" value_original="shorter" />
      </biological_entity>
      <biological_entity id="o24365" name="margin" name_original="margins" src="d0_s12" type="structure">
