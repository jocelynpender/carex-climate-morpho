        <character is_modifier="false" modifier="adaxially" name="pubescence_or_relief" src="d0_s4" value="scabrous" value_original="scabrous" />
      </biological_entity>
      <biological_entity id="o8446" name="culm" name_original="culms" src="d0_s4" type="structure" />
      <biological_entity id="o8447" name="culm" name_original="culms" src="d0_s4" type="structure" />
    </statement>
    <statement id="d0_s5">
      <text>Inflorescences with both staminate and pistillate spikes;</text>
      <biological_entity id="o8448" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure" />
      <biological_entity id="o8449" name="spike" name_original="spikes" src="d0_s5" type="structure">
