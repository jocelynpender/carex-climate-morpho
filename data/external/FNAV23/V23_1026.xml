      </biological_entity>
      <biological_entity constraint="apical" id="o21397" name="tooth" name_original="teeth" src="d0_s15" type="structure">
        <character is_modifier="true" name="coloration" src="d0_s15" value="reddish tinge" value_original="reddish tinge" />
        <character char_type="range_value" from="0.4" from_unit="mm" is_modifier="true" name="some_measurement" src="d0_s15" to="1.3" to_unit="mm" />
        <character is_modifier="true" modifier="weakly" name="architecture_or_shape" src="d0_s15" value="ciliate-serrulate" value_original="ciliate-serrulate" />
