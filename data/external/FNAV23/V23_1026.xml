        <character char_type="range_value" from="1.5" from_unit="mm" name="width" src="d0_s14" to="2.2" to_unit="mm" />
        <character is_modifier="false" name="width" src="d0_s14" value="wide" value_original="wide" />
      </biological_entity>
    </statement>
    <statement id="d0_s15">
      <text>beak straight, pale green, occasionally with reddish tinge, 0.4–1.3 mm, weakly ciliate-serrulate, apical teeth 0.1–0.7 mm.</text>
      <biological_entity id="o21396" name="beak" name_original="beak" src="d0_s15" type="structure">
