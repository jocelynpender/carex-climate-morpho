      <biological_entity id="o9968" name="culm" name_original="culms" src="d0_s1" type="structure">
        <character is_modifier="true" name="reproduction" src="d0_s1" value="vegetative" value_original="vegetative" />
        <character constraint="with leaves" constraintid="o9969" is_modifier="false" name="prominence" src="d0_s1" value="inconspicuous" value_original="inconspicuous" />
