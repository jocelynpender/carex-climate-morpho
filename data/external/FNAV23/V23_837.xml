        <character is_modifier="false" name="coloration" src="d0_s2" value="purple-red" value_original="purple-red" />
        <character char_type="range_value" constraint="at base" constraintid="o669" from="4.8" from_unit="cm" name="some_measurement" src="d0_s2" to="8.1" to_unit="cm" />
