      </biological_entity>
      <biological_entity id="o669" name="base" name_original="base" src="d0_s2" type="structure" />
    </statement>
    <statement id="d0_s3">
      <text>vegetative shoots 28–49 cm, 0.5–1.3 times as tall as culms.</text>
