      </biological_entity>
      <biological_entity id="o11244" name="awn" name_original="awn" src="d0_s13" type="structure">
        <character char_type="range_value" from="0" from_unit="mm" name="some_measurement" src="d0_s13" to="1.5" to_unit="mm" />
