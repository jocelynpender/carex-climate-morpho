      </biological_entity>
      <biological_entity id="o27669" name="base" name_original="base" src="d0_s15" type="structure" />
      <biological_entity id="o27670" name="achene" name_original="achene" src="d0_s15" type="structure" />
      <biological_entity id="o27671" name="base" name_original="base" src="d0_s15" type="structure" />
      <biological_entity id="o27672" name="stipe" name_original="stipe" src="d0_s15" type="structure">
        <character name="some_measurement" src="d0_s15" unit="mm" value="0.5" value_original="0.5" />
      </biological_entity>
      <biological_entity id="o27673" name="apex" name_original="apex" src="d0_s15" type="structure">
        <character constraint="to beak" constraintid="o27674" is_modifier="false" name="shape" src="d0_s15" value="tapering" value_original="tapering" />
