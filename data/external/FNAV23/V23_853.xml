        <character char_type="range_value" constraint="on back" constraintid="o3406" from="maroon" name="coloration" src="d0_s3" to="green" />
        <character is_modifier="false" name="coloration" notes="" src="d0_s3" value="pale brown-hyaline" value_original="pale brown-hyaline" />
        <character is_modifier="false" name="coloration" src="d0_s3" value="red dotted" value_original="red dotted" />
        <character constraint="near apex" constraintid="o3407" is_modifier="false" modifier="sometimes" name="pubescence" src="d0_s3" value="pubescent" value_original="pubescent" />
      </biological_entity>
      <biological_entity id="o3406" name="back" name_original="back" src="d0_s3" type="structure" />
      <biological_entity id="o3407" name="apex" name_original="apex" src="d0_s3" type="structure" />
    </statement>
    <statement id="d0_s4">
      <text>blades flat, 3–9 mm wide, glabrous, minutely scabrous on margins distally.</text>
      <biological_entity id="o3408" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
      <biological_entity id="o3409" name="blade" name_original="blades" src="d0_s4" type="structure">
