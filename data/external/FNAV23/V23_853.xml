        <character is_modifier="false" name="arrangement" src="d0_s10" value="apart" value_original="apart" />
        <character is_modifier="false" name="arrangement_or_course_or_shape" src="d0_s10" value="linear" value_original="linear" />
        <character char_type="range_value" from="10" from_unit="mm" name="length" src="d0_s10" to="70" to_unit="mm" />
        <character char_type="range_value" from="2" from_unit="mm" name="width" src="d0_s10" to="3.5" to_unit="mm" />
      </biological_entity>
      <biological_entity id="o3426" name="node" name_original="node" src="d0_s10" type="structure" />
      <biological_entity id="o3427" name="perigynium" name_original="perigynia" src="d0_s10" type="structure">
        <character char_type="range_value" from="10" is_modifier="true" name="quantity" src="d0_s10" to="45" />
        <character is_modifier="false" name="fixation" src="d0_s10" value="attached" value_original="attached" />
      </biological_entity>
    </statement>
    <statement id="d0_s11">
      <text>Terminal spike gynecandrous, 10–60 × 1–3 mm.</text>
      <biological_entity constraint="terminal" id="o3428" name="spike" name_original="spike" src="d0_s11" type="structure">
        <character is_modifier="false" name="reproduction" src="d0_s11" value="gynecandrous" value_original="gynecandrous" />
