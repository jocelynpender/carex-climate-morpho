        <character is_modifier="false" name="shape" src="d0_s12" value="rounded" value_original="rounded" />
        <character constraint="with awn" constraintid="o3433" is_modifier="false" name="architecture_or_shape" src="d0_s12" value="cuspidate" value_original="cuspidate" />
