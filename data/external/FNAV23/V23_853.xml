        <character constraint="than leaves" constraintid="o3400" is_modifier="false" name="length_or_size" src="d0_s2" value="much longer" value_original="much longer" />
      </biological_entity>
      <biological_entity id="o3400" name="leaf" name_original="leaves" src="d0_s2" type="structure" />
