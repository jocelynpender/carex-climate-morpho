        <character is_modifier="false" modifier="well" name="arrangement" notes="" src="d0_s10" value="separated" value_original="separated" />
        <character constraint="at maturity" is_modifier="false" name="orientation" src="d0_s10" value="drooping" value_original="drooping" />
        <character constraint="with perigynia" constraintid="o3427" is_modifier="false" name="architecture" src="d0_s10" value="pistillate" value_original="pistillate" />
