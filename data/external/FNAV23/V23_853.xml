        <character char_type="range_value" from="3" from_unit="mm" name="width" src="d0_s4" to="9" to_unit="mm" />
        <character is_modifier="false" name="pubescence" src="d0_s4" value="glabrous" value_original="glabrous" />
        <character constraint="on margins" constraintid="o3410" is_modifier="false" modifier="minutely" name="pubescence_or_relief" src="d0_s4" value="scabrous" value_original="scabrous" />
