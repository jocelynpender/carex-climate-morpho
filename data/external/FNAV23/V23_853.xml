      <biological_entity id="o3403" name="sheath" name_original="sheaths" src="d0_s3" type="structure">
        <character is_modifier="false" name="pubescence" src="d0_s3" value="glabrous" value_original="glabrous" />
        <character is_modifier="false" name="pubescence" src="d0_s3" value="short-pubescent" value_original="short-pubescent" />
      </biological_entity>
      <biological_entity constraint="basal" id="o3404" name="sheath" name_original="sheaths" src="d0_s3" type="structure">
        <character is_modifier="false" name="quantity" src="d0_s3" value="few" value_original="few" />
        <character is_modifier="false" name="architecture" src="d0_s3" value="bladeless" value_original="bladeless" />
