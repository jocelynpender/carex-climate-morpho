  <description type="morphology">
    <statement id="d0_s0">
      <text>Plants without conspicuous rhizomes.</text>
      <biological_entity id="o21495" name="whole_organism" name_original="" src="" type="structure">
        <character name="growth_form" value="plant" />
      </biological_entity>
      <biological_entity id="o21496" name="rhizome" name_original="rhizomes" src="d0_s0" type="structure">
        <character is_modifier="true" name="prominence" src="d0_s0" value="conspicuous" value_original="conspicuous" />
      </biological_entity>
      <relation from="o21495" id="r2597" name="without" negation="false" src="d0_s0" to="o21496" />
    </statement>
    <statement id="d0_s1">
      <text>Culms 20–90 cm, 1.7–2.2 mm wide basally, 0.7–1.1 mm wide distally.</text>
      <biological_entity id="o21497" name="culm" name_original="culms" src="d0_s1" type="structure">
        <character char_type="range_value" from="20" from_unit="cm" name="some_measurement" src="d0_s1" to="90" to_unit="cm" />
        <character char_type="range_value" from="1.7" from_unit="mm" modifier="basally" name="width" src="d0_s1" to="2.2" to_unit="mm" />
        <character char_type="range_value" from="0.7" from_unit="mm" modifier="distally" name="width" src="d0_s1" to="1.1" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Leaves: sheaths tight, green, sometimes white spotted, fronts hyaline, sometimes transversely rugose, mouth yellowish and thickened;</text>
      <biological_entity id="o21498" name="leaf" name_original="leaves" src="d0_s2" type="structure" />
      <biological_entity id="o21499" name="sheath" name_original="sheaths" src="d0_s2" type="structure">
        <character is_modifier="false" name="arrangement_or_density" src="d0_s2" value="tight" value_original="tight" />
        <character is_modifier="false" name="coloration" src="d0_s2" value="green" value_original="green" />
        <character is_modifier="false" modifier="sometimes" name="coloration" src="d0_s2" value="white spotted" value_original="white spotted" />
      </biological_entity>
      <biological_entity id="o21500" name="front" name_original="fronts" src="d0_s2" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s2" value="hyaline" value_original="hyaline" />
        <character is_modifier="false" modifier="sometimes transversely" name="relief" src="d0_s2" value="rugose" value_original="rugose" />
      </biological_entity>
      <biological_entity id="o21501" name="mouth" name_original="mouth" src="d0_s2" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s2" value="yellowish" value_original="yellowish" />
        <character is_modifier="false" name="size_or_width" src="d0_s2" value="thickened" value_original="thickened" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>ligules to 3 mm, usually wider than long;</text>
      <biological_entity id="o21502" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
      <biological_entity id="o21503" name="ligule" name_original="ligules" src="d0_s3" type="structure">
        <character char_type="range_value" from="0" from_unit="mm" name="some_measurement" src="d0_s3" to="3" to_unit="mm" />
        <character is_modifier="false" name="width" src="d0_s3" value="usually wider than long" value_original="usually wider than long" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>widest leaf-blades 2–4 mm wide, papillose adaxially.</text>
      <biological_entity id="o21504" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
      <biological_entity constraint="widest" id="o21505" name="leaf-blade" name_original="leaf-blades" src="d0_s4" type="structure">
        <character char_type="range_value" from="2" from_unit="mm" name="width" src="d0_s4" to="4" to_unit="mm" />
        <character is_modifier="false" modifier="adaxially" name="relief" src="d0_s4" value="papillose" value_original="papillose" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>Inflorescences with 3–10 spikes, 1.5–4 cm × 8–12 mm;</text>
      <biological_entity id="o21506" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure">
        <character char_type="range_value" from="1.5" from_unit="cm" name="length" notes="" src="d0_s5" to="4" to_unit="cm" />
        <character char_type="range_value" from="8" from_unit="mm" name="width" notes="" src="d0_s5" to="12" to_unit="mm" />
      </biological_entity>
      <biological_entity id="o21507" name="spike" name_original="spikes" src="d0_s5" type="structure">
        <character char_type="range_value" from="3" is_modifier="true" name="quantity" src="d0_s5" to="10" />
      </biological_entity>
      <relation from="o21506" id="r2598" name="with" negation="false" src="d0_s5" to="o21507" />
    </statement>
    <statement id="d0_s6">
      <text>proximal internodes equaling or shorter than proximal spikes;</text>
      <biological_entity constraint="proximal" id="o21508" name="internode" name_original="internodes" src="d0_s6" type="structure">
        <character is_modifier="false" name="variability" src="d0_s6" value="equaling" value_original="equaling" />
        <character constraint="than proximal spikes" constraintid="o21509" is_modifier="false" name="height_or_length_or_size" src="d0_s6" value="shorter" value_original="shorter" />
      </biological_entity>
      <biological_entity constraint="proximal" id="o21509" name="spike" name_original="spikes" src="d0_s6" type="structure" />
    </statement>
    <statement id="d0_s7">
      <text>proximal bracts less than 2 cm;</text>
      <biological_entity constraint="proximal" id="o21510" name="bract" name_original="bracts" src="d0_s7" type="structure">
        <character char_type="range_value" from="0" from_unit="cm" name="some_measurement" src="d0_s7" to="2" to_unit="cm" />
      </biological_entity>
    </statement>
    <statement id="d0_s8">
      <text>spikes with 8–20 ascending or spreading perigynia.</text>
      <biological_entity id="o21511" name="spike" name_original="spikes" src="d0_s8" type="structure" />
      <biological_entity id="o21512" name="perigynium" name_original="perigynia" src="d0_s8" type="structure">
        <character char_type="range_value" from="8" is_modifier="true" name="quantity" src="d0_s8" to="20" />
        <character is_modifier="true" name="orientation" src="d0_s8" value="ascending" value_original="ascending" />
        <character is_modifier="true" name="orientation" src="d0_s8" value="spreading" value_original="spreading" />
      </biological_entity>
      <relation from="o21511" id="r2599" name="with" negation="false" src="d0_s8" to="o21512" />
    </statement>
    <statement id="d0_s9">
      <text>Pistillate scales hyaline with green, 3-veined center, ovate to ovate-circular, body narrower than and 2/3 to length of perigynium, apex acute or with awn 0.5–1.5 (–2) mm.</text>
      <biological_entity id="o21513" name="scale" name_original="scales" src="d0_s9" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s9" value="pistillate" value_original="pistillate" />
        <character is_modifier="false" name="coloration" src="d0_s9" value="hyaline with green" value_original="hyaline with green" />
      </biological_entity>
      <biological_entity id="o21514" name="center" name_original="center" src="d0_s9" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s9" value="3-veined" value_original="3-veined" />
        <character char_type="range_value" from="ovate" name="shape" src="d0_s9" to="ovate-circular" />
      </biological_entity>
      <biological_entity id="o21515" name="body" name_original="body" src="d0_s9" type="structure">
        <character constraint="to " constraintid="o21516" name="quantity" src="d0_s9" value="2/3" value_original="2/3" />
      </biological_entity>
      <biological_entity id="o21516" name="perigynium" name_original="perigynium" src="d0_s9" type="structure" />
      <biological_entity id="o21517" name="apex" name_original="apex" src="d0_s9" type="structure">
        <character is_modifier="false" name="length" src="d0_s9" value="acute" value_original="acute" />
        <character is_modifier="false" name="length" src="d0_s9" value="with awn" value_original="with awn" />
      </biological_entity>
      <biological_entity id="o21518" name="awn" name_original="awn" src="d0_s9" type="structure">
        <character char_type="range_value" from="1.5" from_inclusive="false" from_unit="mm" name="atypical_some_measurement" src="d0_s9" to="2" to_unit="mm" />
        <character char_type="range_value" from="0.5" from_unit="mm" name="some_measurement" src="d0_s9" to="1.5" to_unit="mm" />
      </biological_entity>
      <relation from="o21517" id="r2600" name="with" negation="false" src="d0_s9" to="o21518" />
    </statement>
    <statement id="d0_s10">
      <text>Anthers 1.5–2.3 mm.</text>
      <biological_entity id="o21519" name="anther" name_original="anthers" src="d0_s10" type="structure">
        <character char_type="range_value" from="1.5" from_unit="mm" name="some_measurement" src="d0_s10" to="2.3" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s11">
      <text>Perigynia pale green to yellowbrown, 9–15-veined abaxially, margins serrulate distally;</text>
      <biological_entity id="o21520" name="perigynium" name_original="perigynia" src="d0_s11" type="structure">
        <character char_type="range_value" from="pale green" name="coloration" src="d0_s11" to="yellowbrown" />
        <character is_modifier="false" modifier="abaxially" name="architecture" src="d0_s11" value="9-15-veined" value_original="9-15-veined" />
      </biological_entity>
      <biological_entity id="o21521" name="margin" name_original="margins" src="d0_s11" type="structure">
        <character is_modifier="false" modifier="distally" name="architecture_or_shape" src="d0_s11" value="serrulate" value_original="serrulate" />
      </biological_entity>
    </statement>
    <statement id="d0_s12">
      <text>beak 0.2–1.2mm, apical teeth 0.2–0.5 mm.</text>
      <biological_entity id="o21522" name="beak" name_original="beak" src="d0_s12" type="structure">
        <character char_type="range_value" from="0.2" from_unit="mm" name="some_measurement" src="d0_s12" to="1.2" to_unit="mm" />
      </biological_entity>
      <biological_entity constraint="apical" id="o21523" name="tooth" name_original="teeth" src="d0_s12" type="structure">
        <character char_type="range_value" from="0.2" from_unit="mm" name="some_measurement" src="d0_s12" to="0.5" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s13">
      <text>Achenes suborbiculate, 1.9–2.2 × 1.9–2.1 mm.</text>
      <biological_entity id="o21524" name="achene" name_original="achenes" src="d0_s13" type="structure">
        <character is_modifier="false" name="shape" src="d0_s13" value="suborbiculate" value_original="suborbiculate" />
        <character char_type="range_value" from="1.9" from_unit="mm" name="length" src="d0_s13" to="2.2" to_unit="mm" />
        <character char_type="range_value" from="1.9" from_unit="mm" name="width" src="d0_s13" to="2.1" to_unit="mm" />
      </biological_entity>
    </statement>
