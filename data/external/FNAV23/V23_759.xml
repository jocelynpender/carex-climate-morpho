        <character is_modifier="false" name="shape_or_vernation" src="d0_s1" value="involute" value_original="involute" />
        <character is_modifier="false" name="pubescence_or_relief" src="d0_s1" value="scabrid" value_original="scabrid" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Inflorescences: proximal bracts 2–6 cm, shorter than inflorescences;</text>
      <biological_entity id="o26736" name="inflorescence" name_original="inflorescences" src="d0_s2" type="structure" />
