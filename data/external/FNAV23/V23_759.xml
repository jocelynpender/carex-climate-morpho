    </statement>
    <statement id="d0_s4">
      <text>terminal spikes 7–35 × 1.5–2.5 (–3) mm.</text>
      <biological_entity id="o26742" name="inflorescence" name_original="inflorescences" src="d0_s4" type="structure" />
