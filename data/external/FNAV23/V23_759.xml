        <character char_type="range_value" from="6" from_unit="mm" name="length" src="d0_s3" to="20" to_unit="mm" />
        <character char_type="range_value" from="4" from_unit="mm" name="width" src="d0_s3" to="8" to_unit="mm" />
      </biological_entity>
      <biological_entity id="o26741" name="perigynium" name_original="perigynia" src="d0_s3" type="structure">
