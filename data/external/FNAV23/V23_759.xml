      <relation from="o26732" id="r3315" name="remains at" negation="false" src="d0_s0" to="o26733" />
    </statement>
    <statement id="d0_s1">
      <text>Leaf-blades 1–2.5 mm wide, margins involute, scabrid.</text>
      <biological_entity id="o26734" name="leaf-blade" name_original="leaf-blades" src="d0_s1" type="structure">
        <character char_type="range_value" from="1" from_unit="mm" name="width" src="d0_s1" to="2.5" to_unit="mm" />
      </biological_entity>
