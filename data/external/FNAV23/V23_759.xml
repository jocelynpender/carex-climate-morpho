      </biological_entity>
      <biological_entity id="o26732" name="leaf" name_original="leaf" src="d0_s0" type="structure">
        <character is_modifier="true" name="life_cycle" src="d0_s0" value="dead" value_original="dead" />
      </biological_entity>
