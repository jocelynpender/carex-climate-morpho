      </biological_entity>
      <biological_entity id="o26738" name="inflorescence" name_original="inflorescences" src="d0_s2" type="structure" />
    </statement>
    <statement id="d0_s3">
      <text>lateral spikes sometimes androgynous, 6–20 × 4–8 mm, with 8–30 perigynia;</text>
      <biological_entity id="o26739" name="inflorescence" name_original="inflorescences" src="d0_s3" type="structure" />
