        <character is_modifier="false" modifier="often" name="coloration" src="d0_s11" value="brown tinged" value_original="brown tinged" />
        <character is_modifier="false" name="shape" src="d0_s11" value="ovate" value_original="ovate" />
        <character is_modifier="false" name="size" src="d0_s11" value="subequal" value_original="subequal" />
