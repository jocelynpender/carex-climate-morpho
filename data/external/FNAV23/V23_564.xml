        <character is_modifier="false" modifier="scarcely" name="shape" src="d0_s10" value="clavate" value_original="clavate" />
      </biological_entity>
    </statement>
    <statement id="d0_s11">
      <text>Pistillate scales white-hyaline with green, 3-veined center, often brown tinged, ovate, subequal to, not concealing perigynia, apex obtuse to acute.</text>
