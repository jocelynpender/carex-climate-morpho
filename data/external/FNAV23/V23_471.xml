  <description type="morphology">
    <statement id="d0_s0">
      <text>Plants densely cespitose, short-rhizomatous.</text>
      <biological_entity id="o6912" name="whole_organism" name_original="" src="" type="structure">
        <character is_modifier="false" modifier="densely" name="growth_form" src="d0_s0" value="cespitose" value_original="cespitose" />
        <character is_modifier="false" name="architecture" src="d0_s0" value="short-rhizomatous" value_original="short-rhizomatous" />
        <character name="growth_form" value="plant" />
      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>Culms brown to dark-brown at base, not more than 1 mm wide distally.</text>
      <biological_entity id="o6913" name="culm" name_original="culms" src="d0_s1" type="structure">
        <character char_type="range_value" constraint="at base" constraintid="o6914" from="brown" name="coloration" src="d0_s1" to="dark-brown" />
        <character char_type="range_value" from="1" from_unit="mm" modifier="not; distally" name="width" notes="" src="d0_s1" upper_restricted="false" />
      </biological_entity>
      <biological_entity id="o6914" name="base" name_original="base" src="d0_s1" type="structure" />
    </statement>
    <statement id="d0_s2">
      <text>Leaves: basal sheaths not fibrous;</text>
      <biological_entity id="o6915" name="leaf" name_original="leaves" src="d0_s2" type="structure" />
      <biological_entity constraint="basal" id="o6916" name="sheath" name_original="sheaths" src="d0_s2" type="structure">
        <character is_modifier="false" modifier="not" name="texture" src="d0_s2" value="fibrous" value_original="fibrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>sheath fronts dotted red or copper, sometimes weakly transversely rugose, membranous;</text>
      <biological_entity id="o6917" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
      <biological_entity constraint="sheath" id="o6918" name="front" name_original="fronts" src="d0_s3" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s3" value="dotted red" value_original="dotted red" />
        <character is_modifier="false" name="coloration" src="d0_s3" value="copper" value_original="copper" />
        <character is_modifier="false" modifier="sometimes weakly transversely" name="relief" src="d0_s3" value="rugose" value_original="rugose" />
        <character is_modifier="false" name="texture" src="d0_s3" value="membranous" value_original="membranous" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>blades V-shaped in cross-section when young, glabrous.</text>
