        <character constraint="in cross-section" constraintid="o6932" is_modifier="false" name="shape" src="d0_s10" value="planoconvex" value_original="planoconvex" />
        <character is_modifier="false" name="architecture" notes="" src="d0_s10" value="thick-walled" value_original="thick-walled" />
