    </statement>
    <statement id="d0_s6">
      <text>proximal bracts awl-shaped or bristlelike, others scalelike or absent, sheathless;</text>
      <biological_entity constraint="proximal" id="o6924" name="bract" name_original="bracts" src="d0_s6" type="structure">
        <character is_modifier="false" name="shape" src="d0_s6" value="awl--shaped" value_original="awl--shaped" />
        <character is_modifier="false" name="architecture" src="d0_s6" value="bristlelike" value_original="bristlelike" />
      </biological_entity>
      <biological_entity id="o6925" name="other" name_original="others" src="d0_s6" type="structure">
