      <biological_entity id="o10137" name="perigynium" name_original="perigynia" src="d0_s9" type="structure">
        <character char_type="range_value" from="4" is_modifier="true" name="quantity" src="d0_s9" to="9" />
      </biological_entity>
      <relation from="o10136" id="r1228" name="with" negation="false" src="d0_s9" to="o10137" />
    </statement>
