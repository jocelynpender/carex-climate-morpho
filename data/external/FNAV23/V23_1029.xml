    <statement id="d0_s8">
      <text>Spikes: proximal pistillate spikes 1–4 (basal spikes 0–1);</text>
      <biological_entity id="o10132" name="spike" name_original="spikes" src="d0_s8" type="structure" />
      <biological_entity constraint="proximal" id="o10133" name="spike" name_original="spikes" src="d0_s8" type="structure" />
