        <character char_type="range_value" from="25" from_unit="mm" name="some_measurement" src="d0_s1" to="75" to_unit="mm" />
        <character is_modifier="false" name="size" src="d0_s1" value="slender" value_original="slender" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
