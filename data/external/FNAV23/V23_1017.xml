      <relation from="o19226" id="r2316" name="with" negation="false" src="d0_s9" to="o19227" />
    </statement>
    <statement id="d0_s10">
      <text>staminate spikes 4–15 × 0.7–1.3 mm.</text>
      <biological_entity id="o19228" name="spike" name_original="spikes" src="d0_s10" type="structure" />
      <biological_entity id="o19229" name="spike" name_original="spikes" src="d0_s10" type="structure">
