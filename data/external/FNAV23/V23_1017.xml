  <description type="morphology">
    <statement id="d0_s0">
      <text>Plants loosely cespitose;</text>
      <biological_entity id="o19210" name="whole_organism" name_original="" src="" type="structure">
        <character is_modifier="false" modifier="loosely" name="growth_form" src="d0_s0" value="cespitose" value_original="cespitose" />
        <character name="growth_form" value="plant" />
      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>rhizomes ascending to erect, reddish to reddish-brown, 0–10 (–20) mm, slender.</text>
      <biological_entity id="o19211" name="rhizome" name_original="rhizomes" src="d0_s1" type="structure">
        <character char_type="range_value" from="ascending" name="orientation" src="d0_s1" to="erect" />
        <character char_type="range_value" from="reddish" name="coloration" src="d0_s1" to="reddish-brown" />
        <character char_type="range_value" from="10" from_inclusive="false" from_unit="mm" name="atypical_some_measurement" src="d0_s1" to="20" to_unit="mm" />
        <character char_type="range_value" from="0" from_unit="mm" name="some_measurement" src="d0_s1" to="10" to_unit="mm" />
        <character is_modifier="false" name="size" src="d0_s1" value="slender" value_original="slender" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Culms 5–40 cm, weakly scabrous distally;</text>
      <biological_entity id="o19212" name="culm" name_original="culms" src="d0_s2" type="structure">
        <character char_type="range_value" from="5" from_unit="cm" name="some_measurement" src="d0_s2" to="40" to_unit="cm" />
        <character is_modifier="false" modifier="weakly; distally" name="pubescence_or_relief" src="d0_s2" value="scabrous" value_original="scabrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>bases not fibrous.</text>
      <biological_entity id="o19213" name="base" name_original="bases" src="d0_s3" type="structure">
        <character is_modifier="false" modifier="not" name="texture" src="d0_s3" value="fibrous" value_original="fibrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>Leaf-blades green, equaling or exceeding culms, 0.7–1.5 mm wide, herbaceous, smooth to papillose abaxially, weakly scabrous adaxially.</text>
      <biological_entity id="o19214" name="leaf-blade" name_original="leaf-blades" src="d0_s4" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s4" value="green" value_original="green" />
        <character is_modifier="false" name="variability" src="d0_s4" value="equaling" value_original="equaling" />
        <character name="variability" src="d0_s4" value="exceeding culms" value_original="exceeding culms" />
        <character char_type="range_value" from="0.7" from_unit="mm" name="width" src="d0_s4" to="1.5" to_unit="mm" />
        <character is_modifier="false" name="growth_form_or_texture" src="d0_s4" value="herbaceous" value_original="herbaceous" />
        <character char_type="range_value" from="smooth" name="relief" src="d0_s4" to="papillose abaxially" />
        <character is_modifier="false" modifier="weakly; adaxially" name="pubescence_or_relief" src="d0_s4" value="scabrous" value_original="scabrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>Inflorescences with both staminate and proximal spikes;</text>
      <biological_entity id="o19215" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure" />
      <biological_entity constraint="proximal" id="o19216" name="spike" name_original="spikes" src="d0_s5" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s5" value="staminate" value_original="staminate" />
      </biological_entity>
      <relation from="o19215" id="r2314" name="with" negation="false" src="d0_s5" to="o19216" />
    </statement>
    <statement id="d0_s6">
      <text>peduncles of staminate spikes 1.9–5.9 mm;</text>
      <biological_entity id="o19217" name="peduncle" name_original="peduncles" src="d0_s6" type="structure" constraint="spike" constraint_original="spike; spike">
        <character char_type="range_value" from="1.9" from_unit="mm" name="some_measurement" src="d0_s6" to="5.9" to_unit="mm" />
      </biological_entity>
      <biological_entity id="o19218" name="spike" name_original="spikes" src="d0_s6" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s6" value="staminate" value_original="staminate" />
      </biological_entity>
      <relation from="o19217" id="r2315" name="part_of" negation="false" src="d0_s6" to="o19218" />
    </statement>
    <statement id="d0_s7">
      <text>proximal nonbasal bracts leaflike, equaling or shorter than inflorescences.</text>
      <biological_entity constraint="proximal nonbasal" id="o19219" name="bract" name_original="bracts" src="d0_s7" type="structure">
        <character is_modifier="false" name="architecture_or_shape" src="d0_s7" value="leaflike" value_original="leaflike" />
        <character is_modifier="false" name="variability" src="d0_s7" value="equaling" value_original="equaling" />
        <character constraint="than inflorescences" constraintid="o19220" is_modifier="false" name="height_or_length_or_size" src="d0_s7" value="shorter" value_original="shorter" />
      </biological_entity>
      <biological_entity id="o19220" name="inflorescence" name_original="inflorescences" src="d0_s7" type="structure" />
    </statement>
    <statement id="d0_s8">
      <text>Spikes: proximal pistillate spikes 2–3 (basal spikes 0);</text>
      <biological_entity id="o19221" name="spike" name_original="spikes" src="d0_s8" type="structure" />
      <biological_entity constraint="proximal" id="o19222" name="spike" name_original="spikes" src="d0_s8" type="structure" />
      <biological_entity id="o19223" name="spike" name_original="spikes" src="d0_s8" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s8" value="pistillate" value_original="pistillate" />
        <character char_type="range_value" from="2" name="quantity" src="d0_s8" to="3" />
      </biological_entity>
    </statement>
    <statement id="d0_s9">
