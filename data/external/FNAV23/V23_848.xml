        <character is_modifier="true" name="fragility" src="d0_s12" value="strong" value_original="strong" />
      </biological_entity>
      <biological_entity id="o17849" name="base" name_original="base" src="d0_s12" type="structure">
        <character is_modifier="false" name="shape" src="d0_s12" value="tapering" value_original="tapering" />
        <character is_modifier="false" name="shape" src="d0_s12" value="rounded" value_original="rounded" />
      </biological_entity>
