    </statement>
    <statement id="d0_s6">
      <text>proximal internodes 5–10 mm, as long as proximal spikes;</text>
      <biological_entity constraint="proximal" id="o23810" name="internode" name_original="internodes" src="d0_s6" type="structure">
        <character char_type="range_value" from="5" from_unit="mm" name="some_measurement" src="d0_s6" to="10" to_unit="mm" />
