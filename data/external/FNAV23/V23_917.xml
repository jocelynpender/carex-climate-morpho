        <character is_modifier="false" name="relief" src="d0_s2" value="papillose" value_original="papillose" />
        <character is_modifier="false" modifier="distally" name="shape" src="d0_s2" value="scabrous-angled" value_original="scabrous-angled" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>Leaves: basal sheaths brown, bladeless, rapidly disintegrating into fibers;</text>
      <biological_entity id="o28657" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
      <biological_entity constraint="basal" id="o28658" name="sheath" name_original="sheaths" src="d0_s3" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s3" value="brown" value_original="brown" />
