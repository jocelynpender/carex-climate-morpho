        <character char_type="range_value" from="1.5" from_unit="mm" name="width" src="d0_s5" to="3.2" to_unit="mm" />
        <character is_modifier="false" modifier="abaxially" name="relief" src="d0_s5" value="papillose" value_original="papillose" />
      </biological_entity>
    </statement>
    <statement id="d0_s6">
      <text>Inflorescences 2.5–12 cm;</text>
      <biological_entity id="o28664" name="inflorescence" name_original="inflorescences" src="d0_s6" type="structure">
