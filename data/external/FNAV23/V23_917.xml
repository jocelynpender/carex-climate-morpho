    <statement id="d0_s2">
      <text>Culms central, slender, trigonous, 25–60 cm, papillose and scabrous-angled distally.</text>
      <biological_entity id="o28656" name="culm" name_original="culms" src="d0_s2" type="structure">
        <character is_modifier="false" name="position" src="d0_s2" value="central" value_original="central" />
