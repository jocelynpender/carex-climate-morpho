      </biological_entity>
    </statement>
    <statement id="d0_s7">
      <text>rachis beyond proximal pistillate spikes sharp-angled, often finely scabrous;</text>
      <biological_entity constraint="proximal" id="o28666" name="spike" name_original="spikes" src="d0_s7" type="structure">
