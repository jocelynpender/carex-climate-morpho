      <biological_entity id="o28659" name="fiber" name_original="fibers" src="d0_s3" type="structure" />
    </statement>
    <statement id="d0_s4">
      <text>ligules 0.7–1.5 mm, wider than long;</text>
      <biological_entity id="o28660" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
      <biological_entity id="o28661" name="ligule" name_original="ligules" src="d0_s4" type="structure">
