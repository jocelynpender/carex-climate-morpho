      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>blades somewhat glaucous, M-shaped, 1.5–3.2 mm wide, papillose abaxially.</text>
      <biological_entity id="o28662" name="leaf" name_original="leaves" src="d0_s5" type="structure" />
