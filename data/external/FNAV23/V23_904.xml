        <character is_modifier="false" name="shape" src="d0_s9" value="obovoid" value_original="obovoid" />
        <character char_type="range_value" from="3" from_unit="mm" name="length" src="d0_s9" to="4.5" to_unit="mm" />
        <character char_type="range_value" from="1.5" from_unit="mm" name="width" src="d0_s9" to="1.9" to_unit="mm" />
        <character is_modifier="false" name="pubescence" src="d0_s9" value="puberulent" value_original="puberulent" />
      </biological_entity>
      <biological_entity id="o15404" name="face" name_original="faces" src="d0_s9" type="structure">
        <character char_type="range_value" from="0.1" from_unit="mm" name="width" src="d0_s9" upper_restricted="false" />
        <character constraint="than marginal veins" constraintid="o15405" is_modifier="false" name="width" notes="alterIDs:o15404" src="d0_s9" value="distinctly narrower" value_original="distinctly narrower" />
      </biological_entity>
      <biological_entity constraint="marginal" id="o15405" name="vein" name_original="veins" src="d0_s9" type="structure" />
      <relation from="o15403" id="r1887" name="on" negation="false" src="d0_s9" to="o15404" />
    </statement>
    <statement id="d0_s10">
      <text>beak distinct, straight or slightly bent, 0.3–0.6 mm.</text>
      <biological_entity id="o15406" name="beak" name_original="beak" src="d0_s10" type="structure">
        <character is_modifier="false" name="fusion" src="d0_s10" value="distinct" value_original="distinct" />
