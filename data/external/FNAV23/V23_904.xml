      </biological_entity>
    </statement>
    <statement id="d0_s11">
      <text>Achenes subsessile, usually with 2 faces concave, 2.4–2.5 × 1.5–1.7 mm, apex obtuse to truncate.</text>
      <biological_entity id="o15407" name="achene" name_original="achenes" src="d0_s11" type="structure">
