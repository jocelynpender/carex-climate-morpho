        <character is_modifier="false" modifier="obscurely" name="shape" src="d0_s11" value="bidentate" value_original="bidentate" />
        <character is_modifier="false" name="architecture_or_shape" src="d0_s11" value="serrulate" value_original="serrulate" />
      </biological_entity>
    </statement>
    <statement id="d0_s12">
      <text>Achenes nearly filling body of perigynia.</text>
