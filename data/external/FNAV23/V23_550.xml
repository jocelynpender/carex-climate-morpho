        <character char_type="range_value" from="5" from_unit="mm" name="width" src="d0_s6" to="12" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s7">
      <text>proximal bracts usually scalelike, shorter than spikes.</text>
      <biological_entity constraint="proximal" id="o28417" name="bract" name_original="bracts" src="d0_s7" type="structure">
        <character is_modifier="false" modifier="usually" name="shape" src="d0_s7" value="scale-like" value_original="scalelike" />
