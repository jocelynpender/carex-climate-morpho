        <character is_modifier="false" modifier="individually" name="fusion" src="d0_s8" value="distinct" value_original="distinct" />
      </biological_entity>
      <biological_entity constraint="proximal lateral" id="o28420" name="spike" name_original="spikes" src="d0_s8" type="structure">
        <character is_modifier="false" name="architecture" src="d0_s8" value="pistillate" value_original="pistillate" />
        <character is_modifier="false" name="arrangement_or_course_or_shape" src="d0_s8" value="oblong-linear" value_original="oblong-linear" />
