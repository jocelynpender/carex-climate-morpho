        <character char_type="range_value" from="flat" name="shape" src="d0_s5" to="channeled" />
        <character char_type="range_value" from="5" from_unit="cm" name="length" src="d0_s5" to="15" to_unit="cm" />
        <character char_type="range_value" from="1" from_unit="mm" name="width" src="d0_s5" to="2" to_unit="mm" />
        <character constraint="than culms" constraintid="o28415" is_modifier="false" name="height_or_length_or_size" src="d0_s5" value="usually shorter" value_original="usually shorter" />
      </biological_entity>
      <biological_entity id="o28415" name="culm" name_original="culms" src="d0_s5" type="structure" />
    </statement>
    <statement id="d0_s6">
      <text>Inflorescences erect, 1–2 cm × 5–12 mm;</text>
      <biological_entity id="o28416" name="inflorescence" name_original="inflorescences" src="d0_s6" type="structure">
        <character is_modifier="false" name="orientation" src="d0_s6" value="erect" value_original="erect" />
