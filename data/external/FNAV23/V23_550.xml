        <character char_type="range_value" from="5" is_modifier="true" name="quantity" src="d0_s8" to="10" />
      </biological_entity>
      <relation from="o28420" id="r3503" name="containing" negation="false" src="d0_s8" to="o28421" />
    </statement>
    <statement id="d0_s9">
