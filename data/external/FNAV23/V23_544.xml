        <character char_type="range_value" from="reddish-brown" modifier="distally" name="coloration" src="d0_s9" to="blackish" />
        <character is_modifier="false" modifier="finely; abaxially" name="architecture" notes="" src="d0_s9" value="5-9-veined" value_original="5-9-veined" />
        <character is_modifier="false" modifier="faintly" name="architecture" src="d0_s9" value="veined" value_original="veined" />
        <character is_modifier="false" modifier="essentially; adaxially" name="architecture" src="d0_s9" value="veinless" value_original="veinless" />
        <character is_modifier="false" name="shape" src="d0_s9" value="flattened" value_original="flattened" />
        <character char_type="range_value" from="not inflated" name="shape" src="d0_s9" to="narrowly broadly elliptic" />
        <character char_type="range_value" from="not inflated" name="shape" src="d0_s9" to="narrowly broadly elliptic" />
        <character char_type="range_value" from="3.3" from_unit="mm" name="length" src="d0_s9" to="4.6" to_unit="mm" />
        <character char_type="range_value" from="1.1" from_unit="mm" name="width" src="d0_s9" to="1.9" to_unit="mm" />
        <character is_modifier="false" name="texture" src="d0_s9" value="membranous" value_original="membranous" />
        <character is_modifier="false" name="reflectance" src="d0_s9" value="shiny" value_original="shiny" />
      </biological_entity>
      <biological_entity id="o3179" name="beak" name_original="beak" src="d0_s9" type="structure" />
      <relation from="o3178" id="r381" name="on" negation="false" src="d0_s9" to="o3179" />
