        <character is_modifier="false" modifier="more or less" name="coloration_or_reflectance" src="d0_s7" value="translucent" value_original="translucent" />
      </biological_entity>
      <biological_entity id="o3173" name="margin" name_original="margins" src="d0_s7" type="structure">
