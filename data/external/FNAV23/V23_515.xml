        <character char_type="range_value" from="3" from_unit="mm" name="some_measurement" src="d0_s3" to="8" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>widest leaf-blades 5–10 mm wide.</text>
      <biological_entity id="o24149" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
      <biological_entity constraint="widest" id="o24150" name="leaf-blade" name_original="leaf-blades" src="d0_s4" type="structure">
