        <character is_modifier="true" name="prominence" src="d0_s2" value="prominent" value_original="prominent" />
      </biological_entity>
      <biological_entity id="o24145" name="back" name_original="backs" src="d0_s2" type="structure" />
      <biological_entity id="o24146" name="front" name_original="fronts" src="d0_s2" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s2" value="hyaline" value_original="hyaline" />
        <character is_modifier="false" modifier="transversely" name="relief" src="d0_s2" value="rugose" value_original="rugose" />
      </biological_entity>
      <relation from="o24143" id="r2963" name="with" negation="false" src="d0_s2" to="o24144" />
      <relation from="o24144" id="r2964" name="on" negation="false" src="d0_s2" to="o24145" />
    </statement>
    <statement id="d0_s3">
      <text>ligules 3–8 mm, as long as wide;</text>
      <biological_entity id="o24147" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
