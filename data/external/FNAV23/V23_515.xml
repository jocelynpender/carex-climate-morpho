    </statement>
    <statement id="d0_s6">
      <text>proximal internodes usually 20+ mm, more than 2 times as long as proximal spikes;</text>
      <biological_entity constraint="proximal" id="o24153" name="internode" name_original="internodes" src="d0_s6" type="structure">
        <character char_type="range_value" from="20" from_unit="mm" name="some_measurement" src="d0_s6" upper_restricted="false" />
