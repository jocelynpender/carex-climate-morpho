        <character is_modifier="true" name="prominence" src="d0_s0" value="conspicuous" value_original="conspicuous" />
      </biological_entity>
      <relation from="o24139" id="r2962" name="without" negation="false" src="d0_s0" to="o24140" />
    </statement>
    <statement id="d0_s1">
      <text>Culms 30–100 cm, 3–6 mm wide basally, 0.7–1.1 mm wide distally.</text>
