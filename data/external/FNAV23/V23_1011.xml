        <character name="growth_form" value="plant" />
      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>rhizomes ascending to erect, brown to reddish-brown, 0–10 mm, stout.</text>
      <biological_entity id="o14447" name="rhizome" name_original="rhizomes" src="d0_s1" type="structure">
        <character char_type="range_value" from="ascending" name="orientation" src="d0_s1" to="erect" />
        <character char_type="range_value" from="brown" name="coloration" src="d0_s1" to="reddish-brown" />
        <character char_type="range_value" from="0" from_unit="mm" name="some_measurement" src="d0_s1" to="10" to_unit="mm" />
        <character is_modifier="false" name="fragility_or_size" src="d0_s1" value="stout" value_original="stout" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Culms 4–16 cm, scabrous distally;</text>
      <biological_entity id="o14448" name="culm" name_original="culms" src="d0_s2" type="structure">
        <character char_type="range_value" from="4" from_unit="cm" name="some_measurement" src="d0_s2" to="16" to_unit="cm" />
        <character is_modifier="false" modifier="distally" name="pubescence_or_relief" src="d0_s2" value="scabrous" value_original="scabrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>bases (remnants of old leaves) fibrous.</text>
      <biological_entity id="o14449" name="base" name_original="bases" src="d0_s3" type="structure">
        <character is_modifier="false" name="texture" src="d0_s3" value="fibrous" value_original="fibrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>Leaf-blades pale to bright green, equaling or exceeding stems, 0.5–4.3 mm wide, herbaceous or coriaceous, glabrous abaxially, strongly scabrous to papillose adaxially.</text>
      <biological_entity id="o14450" name="leaf-blade" name_original="leaf-blades" src="d0_s4" type="structure">
        <character char_type="range_value" from="pale" name="coloration" src="d0_s4" to="bright green" />
        <character is_modifier="false" name="variability" src="d0_s4" value="equaling" value_original="equaling" />
        <character name="variability" src="d0_s4" value="exceeding stems" value_original="exceeding stems" />
        <character char_type="range_value" from="0.5" from_unit="mm" name="width" src="d0_s4" to="4.3" to_unit="mm" />
        <character is_modifier="false" name="texture" src="d0_s4" value="herbaceous" value_original="herbaceous" />
        <character is_modifier="false" name="texture" src="d0_s4" value="coriaceous" value_original="coriaceous" />
        <character is_modifier="false" modifier="abaxially" name="pubescence" src="d0_s4" value="glabrous" value_original="glabrous" />
        <character char_type="range_value" from="strongly scabrous" modifier="adaxially" name="relief" src="d0_s4" to="papillose" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>Inflorescences with both staminate and pistillate scales;</text>
      <biological_entity id="o14451" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure" />
      <biological_entity id="o14452" name="scale" name_original="scales" src="d0_s5" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s5" value="staminate" value_original="staminate" />
        <character is_modifier="true" name="architecture" src="d0_s5" value="pistillate" value_original="pistillate" />
      </biological_entity>
      <relation from="o14451" id="r1772" name="with" negation="false" src="d0_s5" to="o14452" />
    </statement>
    <statement id="d0_s6">
      <text>peduncles of basal pistillate spikes erect, short-to-elongate, stout to slender;</text>
      <biological_entity id="o14453" name="peduncle" name_original="peduncles" src="d0_s6" type="structure" constraint="spike" constraint_original="spike; spike">
        <character is_modifier="false" name="orientation" src="d0_s6" value="erect" value_original="erect" />
