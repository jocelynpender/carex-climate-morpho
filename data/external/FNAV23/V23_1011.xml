        <character char_type="range_value" from="2" name="quantity" src="d0_s9" to="3" />
      </biological_entity>
    </statement>
    <statement id="d0_s10">
      <text>cauline spikes usually overlapping with staminate spikes, with 3–10 (–15) perigynia;</text>
      <biological_entity id="o14462" name="spike" name_original="spikes" src="d0_s10" type="structure" />
