    <statement id="d0_s11">
      <text>staminate spikes 4.5–11.3 × 1.1–3 mm.</text>
      <biological_entity id="o14466" name="spike" name_original="spikes" src="d0_s11" type="structure" />
      <biological_entity id="o14467" name="spike" name_original="spikes" src="d0_s11" type="structure">
