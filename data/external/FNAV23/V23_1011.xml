        <character is_modifier="true" name="architecture" src="d0_s7" value="staminate" value_original="staminate" />
      </biological_entity>
      <relation from="o14455" id="r1774" name="part_of" negation="false" src="d0_s7" to="o14456" />
    </statement>
    <statement id="d0_s8">
      <text>proximal nonbasal bracts scalelike, shorter than inflorescences.</text>
      <biological_entity constraint="proximal nonbasal" id="o14457" name="bract" name_original="bracts" src="d0_s8" type="structure">
