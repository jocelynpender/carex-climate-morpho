    </statement>
    <statement id="d0_s7">
      <text>peduncles of staminate spikes 0.8–15 mm;</text>
      <biological_entity id="o14455" name="peduncle" name_original="peduncles" src="d0_s7" type="structure" constraint="spike" constraint_original="spike; spike">
        <character char_type="range_value" from="0.8" from_unit="mm" name="some_measurement" src="d0_s7" to="15" to_unit="mm" />
