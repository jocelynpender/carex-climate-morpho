        <character char_type="range_value" from="stout" name="size" src="d0_s6" to="slender" />
      </biological_entity>
      <biological_entity constraint="basal" id="o14454" name="spike" name_original="spikes" src="d0_s6" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s6" value="pistillate" value_original="pistillate" />
      </biological_entity>
