    </statement>
    <statement id="d0_s15">
      <text>beak minutely bidentate, 0.3–1 mm, ciliate between apical teeth.</text>
      <biological_entity id="o29002" name="beak" name_original="beak" src="d0_s15" type="structure">
        <character is_modifier="false" modifier="minutely" name="shape" src="d0_s15" value="bidentate" value_original="bidentate" />
        <character char_type="range_value" from="0.3" from_unit="mm" name="some_measurement" src="d0_s15" to="1" to_unit="mm" />
        <character constraint="between apical teeth" constraintid="o29003" is_modifier="false" name="architecture_or_pubescence_or_shape" src="d0_s15" value="ciliate" value_original="ciliate" />
