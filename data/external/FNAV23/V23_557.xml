        <character is_modifier="true" name="size" src="d0_s0" value="small" value_original="small" />
        <character is_modifier="true" name="size" src="d0_s0" value="medium-sized" value_original="medium-sized" />
      </biological_entity>
      <relation from="o25756" id="r3173" name="in" negation="false" src="d0_s0" to="o25757" />
    </statement>
    <statement id="d0_s1">
      <text>rhizomes short.</text>
      <biological_entity id="o25758" name="rhizome" name_original="rhizomes" src="d0_s1" type="structure">
        <character is_modifier="false" name="height_or_length_or_size" src="d0_s1" value="short" value_original="short" />
