        <character char_type="range_value" from="1" from_unit="mm" name="width" src="d0_s1" to="1.5" to_inclusive="false" to_unit="mm" />
        <character char_type="range_value" from="1.5" from_unit="mm" name="width" src="d0_s1" to="2.5" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Inflorescences erect or, sometimes, nodding, 1.5–5 (–7) cm;</text>
      <biological_entity id="o9124" name="inflorescence" name_original="inflorescences" src="d0_s2" type="structure">
