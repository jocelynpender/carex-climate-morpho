        <character name="shape" src="d0_s2" value="zigzag to rounded , prolonged 0-3 mm beyond collar" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>distal ligules 1.5–4 mm;</text>
      <biological_entity id="o32307" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
