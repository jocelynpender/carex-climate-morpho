      </biological_entity>
    </statement>
    <statement id="d0_s6">
      <text>blades 2–4 per fertile culm, 10–28 (–40) cm × 2–4 mm.</text>
      <biological_entity id="o15802" name="leaf" name_original="leaves" src="d0_s6" type="structure" />
