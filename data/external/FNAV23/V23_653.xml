      </biological_entity>
      <biological_entity constraint="tallest" id="o15806" name="culm" name_original="culms" src="d0_s7" type="structure" />
    </statement>
    <statement id="d0_s8">
      <text>proximal internode 1.5–6 (–10) mm;</text>
      <biological_entity constraint="proximal" id="o15807" name="internode" name_original="internode" src="d0_s8" type="structure">
