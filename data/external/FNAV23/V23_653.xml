        <character char_type="range_value" from="28" from_inclusive="false" from_unit="cm" name="atypical_length" notes="" src="d0_s6" to="40" to_unit="cm" />
        <character char_type="range_value" from="10" from_unit="cm" name="length" notes="" src="d0_s6" to="28" to_unit="cm" />
