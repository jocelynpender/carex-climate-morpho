        <character constraint="beyond collar" constraintid="o15799" is_modifier="false" modifier="at; shortly" name="length" src="d0_s4" value="prolonged" value_original="prolonged" />
        <character is_modifier="false" name="relief" notes="" src="d0_s4" value="papillose" value_original="papillose" />
      </biological_entity>
      <biological_entity id="o15799" name="collar" name_original="collar" src="d0_s4" type="structure" />
      <relation from="o15796" id="r1928" name="with" negation="false" src="d0_s4" to="o15797" />
    </statement>
    <statement id="d0_s5">
      <text>distal ligules 1.5–3.5 mm;</text>
