        <character is_modifier="false" name="relief" src="d0_s4" value="cross-corrugated" value_original="cross-corrugated" />
      </biological_entity>
      <biological_entity constraint="adaxial" id="o15797" name="area" name_original="area" src="d0_s4" type="structure">
