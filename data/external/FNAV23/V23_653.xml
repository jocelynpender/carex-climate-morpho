      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Culms typically (3–) 5–9 per clump, 25–85 (–150) cm;</text>
      <biological_entity id="o15790" name="culm" name_original="culms" src="d0_s2" type="structure">
        <character char_type="range_value" from="3" name="atypical_quantity" src="d0_s2" to="5" to_inclusive="false" />
        <character char_type="range_value" constraint="per clump" constraintid="o15791" from="5" name="quantity" src="d0_s2" to="9" />
        <character char_type="range_value" from="85" from_inclusive="false" from_unit="cm" name="atypical_some_measurement" notes="" src="d0_s2" to="150" to_unit="cm" />
        <character char_type="range_value" from="25" from_unit="cm" name="some_measurement" notes="" src="d0_s2" to="85" to_unit="cm" />
      </biological_entity>
      <biological_entity id="o15791" name="clump" name_original="clump" src="d0_s2" type="structure" />
    </statement>
    <statement id="d0_s3">
      <text>vegetative culms inconspicuous, usually fewer than 10 leaves clustered at apex, not strikingly 3-ranked.</text>
      <biological_entity id="o15792" name="culm" name_original="culms" src="d0_s3" type="structure">
        <character is_modifier="true" name="reproduction" src="d0_s3" value="vegetative" value_original="vegetative" />
