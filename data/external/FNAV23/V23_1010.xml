    <statement id="d0_s9">
      <text>Spikes: proximal pistillate spikes 2–5 (basal spikes 1–3);</text>
      <biological_entity id="o7310" name="spike" name_original="spikes" src="d0_s9" type="structure" />
      <biological_entity constraint="proximal" id="o7311" name="spike" name_original="spikes" src="d0_s9" type="structure" />
