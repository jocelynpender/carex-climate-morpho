        <character char_type="range_value" from="1" from_unit="mm" name="width" src="d0_s4" to="2.3" to_unit="mm" />
        <character is_modifier="false" name="growth_form_or_texture" src="d0_s4" value="herbaceous" value_original="herbaceous" />
        <character is_modifier="false" modifier="abaxially" name="pubescence" src="d0_s4" value="glabrous" value_original="glabrous" />
        <character char_type="range_value" from="strongly scabrous" modifier="adaxially" name="relief" src="d0_s4" to="papillose" />
      </biological_entity>
      <biological_entity id="o7301" name="culm" name_original="culms" src="d0_s4" type="structure" />
      <relation from="o7300" id="r865" modifier="greatly" name="exceeding" negation="false" src="d0_s4" to="o7301" />
    </statement>
    <statement id="d0_s5">
      <text>Inflorescences with both staminate and proximal spikes;</text>
      <biological_entity id="o7302" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure" />
      <biological_entity constraint="proximal" id="o7303" name="spike" name_original="spikes" src="d0_s5" type="structure">
