        <character is_modifier="true" name="architecture" src="d0_s9" value="pistillate" value_original="pistillate" />
        <character char_type="range_value" from="2" name="quantity" src="d0_s9" to="5" />
      </biological_entity>
    </statement>
    <statement id="d0_s10">
      <text>cauline spikes overlapping with staminate spikes, with (1–) 4–12 perigynia;</text>
      <biological_entity id="o7313" name="spike" name_original="spikes" src="d0_s10" type="structure" />
