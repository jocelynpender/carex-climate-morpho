        <character is_modifier="false" modifier="distally" name="pubescence_or_relief" src="d0_s2" value="scabrous" value_original="scabrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>bases (remnants of old leaves) weakly to strongly fibrous.</text>
      <biological_entity id="o7299" name="base" name_original="bases" src="d0_s3" type="structure">
        <character is_modifier="false" modifier="weakly to strongly" name="texture" src="d0_s3" value="fibrous" value_original="fibrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>Leaf-blades pale green, greatly exceeding culms, 1–2.3 (–3.8) mm wide, herbaceous, glabrous abaxially, strongly scabrous to papillose adaxially.</text>
      <biological_entity id="o7300" name="leaf-blade" name_original="leaf-blades" src="d0_s4" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s4" value="pale green" value_original="pale green" />
