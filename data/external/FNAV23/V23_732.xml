        <character char_type="range_value" from="5" from_unit="mm" name="width" src="d0_s4" to="10" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>proximal spikes sometimes spreading, long-pendunculate;</text>
      <biological_entity id="o25323" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure" />
      <biological_entity constraint="proximal" id="o25324" name="spike" name_original="spikes" src="d0_s5" type="structure">
