      <biological_entity id="o25321" name="inflorescence" name_original="inflorescences" src="d0_s4" type="structure" />
      <biological_entity id="o25322" name="spike" name_original="spikes" src="d0_s4" type="structure">
        <character is_modifier="false" name="arrangement" src="d0_s4" value="separate" value_original="separate" />
