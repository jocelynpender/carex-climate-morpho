      </biological_entity>
    </statement>
    <statement id="d0_s7">
      <text>lateral 2–4 spikes pistillate, contiguous, of similar length;</text>
      <biological_entity id="o25327" name="inflorescence" name_original="inflorescences" src="d0_s7" type="structure">
