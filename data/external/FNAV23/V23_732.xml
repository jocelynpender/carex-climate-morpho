      </biological_entity>
    </statement>
    <statement id="d0_s6">
      <text>distal spikes erect, short-pendunculate;</text>
      <biological_entity id="o25325" name="inflorescence" name_original="inflorescences" src="d0_s6" type="structure" />
      <biological_entity constraint="distal" id="o25326" name="spike" name_original="spikes" src="d0_s6" type="structure">
