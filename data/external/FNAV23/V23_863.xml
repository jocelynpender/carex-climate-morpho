      <biological_entity id="o20262" name="midrib" name_original="midrib" src="d0_s13" type="structure">
        <character is_modifier="true" name="coloration" src="d0_s13" value="green" value_original="green" />
      </biological_entity>
      <biological_entity id="o20263" name="perigynium" name_original="perigynia" src="d0_s13" type="structure">
        <character is_modifier="true" name="life_cycle" src="d0_s13" value="mature" value_original="mature" />
      </biological_entity>
