      </biological_entity>
      <biological_entity id="o20266" name="achene" name_original="achene" src="d0_s14" type="structure" />
      <biological_entity id="o20267" name="base" name_original="base" src="d0_s14" type="structure" />
      <biological_entity id="o20268" name="stipe" name_original="stipe" src="d0_s14" type="structure">
        <character name="some_measurement" src="d0_s14" unit="mm" value="1" value_original="1" />
      </biological_entity>
      <biological_entity id="o20269" name="apex" name_original="apex" src="d0_s14" type="structure">
        <character constraint="to beak" constraintid="o20270" is_modifier="false" modifier="gradually" name="shape" src="d0_s14" value="tapering" value_original="tapering" />
        <character constraint="toward apex" constraintid="o20271" is_modifier="false" name="pubescence" notes="" src="d0_s14" value="pubescent" value_original="pubescent" />
      </biological_entity>
      <biological_entity id="o20270" name="beak" name_original="beak" src="d0_s14" type="structure" />
