      <relation from="o20267" id="r2439" name="with" negation="false" src="d0_s14" to="o20268" />
    </statement>
    <statement id="d0_s15">
      <text>beak truncate or minutely toothed, 1 mm.</text>
      <biological_entity id="o20272" name="beak" name_original="beak" src="d0_s15" type="structure">
        <character is_modifier="false" name="shape" src="d0_s15" value="truncate" value_original="truncate" />
