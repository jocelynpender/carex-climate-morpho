        <character is_modifier="false" modifier="conspicuously" name="architecture" src="d0_s14" value="6-10-veined" value_original="6-10-veined" />
        <character char_type="range_value" from="4" from_unit="mm" name="length" src="d0_s14" to="4.8" to_unit="mm" />
