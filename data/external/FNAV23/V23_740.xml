        <character name="growth_form" value="plant" />
      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>Culms 25–55 cm, proximally scabrous.</text>
      <biological_entity id="o11365" name="culm" name_original="culms" src="d0_s1" type="structure">
        <character char_type="range_value" from="25" from_unit="cm" name="some_measurement" src="d0_s1" to="55" to_unit="cm" />
        <character is_modifier="false" modifier="proximally" name="pubescence_or_relief" src="d0_s1" value="scabrous" value_original="scabrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Leaves 3–6 mm wide.</text>
      <biological_entity id="o11366" name="leaf" name_original="leaves" src="d0_s2" type="structure">
        <character char_type="range_value" from="3" from_unit="mm" name="width" src="d0_s2" to="6" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>Inflorescences: proximal bracts shorter than or exceeding inflorescences;</text>
      <biological_entity id="o11367" name="inflorescence" name_original="inflorescences" src="d0_s3" type="structure" />
      <biological_entity constraint="proximal" id="o11368" name="bract" name_original="bracts" src="d0_s3" type="structure">
        <character constraint="than or exceeding inflorescences" constraintid="o11369" is_modifier="false" name="height_or_length_or_size" src="d0_s3" value="shorter" value_original="shorter" />
      </biological_entity>
      <biological_entity id="o11369" name="inflorescence" name_original="inflorescences" src="d0_s3" type="structure">
        <character is_modifier="true" name="position_relational" src="d0_s3" value="exceeding" value_original="exceeding" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>spikes pendent, separate, long-pendunculate, elongate, cylindric or clavate, 12–25 × 4–5.5 mm;</text>
      <biological_entity id="o11370" name="inflorescence" name_original="inflorescences" src="d0_s4" type="structure" />
      <biological_entity id="o11371" name="spike" name_original="spikes" src="d0_s4" type="structure">
