        <character is_modifier="false" name="reproduction" src="d0_s5" value="gynecandrous" value_original="gynecandrous" />
        <character name="reproduction" src="d0_s5" value="," value_original="," />
        <character is_modifier="false" name="architecture" src="d0_s5" value="pistillate" value_original="pistillate" />
      </biological_entity>
