        <character constraint="at base" constraintid="o32038" is_modifier="false" modifier="very rarely" name="coloration" src="d0_s2" value="brown" value_original="brown" />
        <character char_type="range_value" from="15" from_unit="cm" name="some_measurement" notes="" src="d0_s2" to="80" to_unit="cm" />
