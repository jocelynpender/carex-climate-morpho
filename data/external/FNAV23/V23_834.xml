        <character char_type="range_value" from="2" from_unit="mm" name="width" src="d0_s5" to="3.6" to_unit="mm" />
        <character is_modifier="false" modifier="abaxially" name="architecture_or_pubescence_or_relief" src="d0_s5" value="smooth" value_original="smooth" />
      </biological_entity>
    </statement>
    <statement id="d0_s6">
      <text>Inflorescences (0.88–) 0.94–0.99 of culm height;</text>
      <biological_entity id="o1871" name="inflorescence" name_original="inflorescences" src="d0_s6" type="structure">
        <character char_type="range_value" from="0.88" name="atypical_quantity" src="d0_s6" to="0.94" to_inclusive="false" />
