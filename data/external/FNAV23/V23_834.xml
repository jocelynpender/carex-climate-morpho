      </biological_entity>
      <biological_entity id="o1872" name="culm" name_original="culm" src="d0_s6" type="structure" />
    </statement>
    <statement id="d0_s7">
      <text>peduncles of lateral spikes barely scaberulous or smooth;</text>
      <biological_entity id="o1873" name="peduncle" name_original="peduncles" src="d0_s7" type="structure" constraint="spike" constraint_original="spike; spike">
