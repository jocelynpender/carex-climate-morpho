        <character is_modifier="false" name="architecture_or_pubescence_or_relief" src="d0_s7" value="smooth" value_original="smooth" />
      </biological_entity>
      <biological_entity constraint="lateral" id="o1874" name="spike" name_original="spikes" src="d0_s7" type="structure" />
      <relation from="o1873" id="r228" name="part_of" negation="false" src="d0_s7" to="o1874" />
    </statement>
    <statement id="d0_s8">
