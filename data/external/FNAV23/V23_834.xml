        <character is_modifier="false" name="pubescence" src="d0_s4" value="glabrous" value_original="glabrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>blades green, widest blades 2–3.6 mm wide, smooth abaxially.</text>
      <biological_entity id="o1868" name="leaf" name_original="leaves" src="d0_s5" type="structure" />
      <biological_entity id="o1869" name="blade" name_original="blades" src="d0_s5" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s5" value="green" value_original="green" />
