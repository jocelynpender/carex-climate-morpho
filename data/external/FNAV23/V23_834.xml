        <character constraint="at base" constraintid="o1863" is_modifier="false" name="height" src="d0_s2" value="high" value_original="high" />
        <character char_type="range_value" from="6" from_unit="cm" name="some_measurement" notes="" src="d0_s2" to="34" to_unit="cm" />
      </biological_entity>
      <biological_entity id="o1863" name="base" name_original="base" src="d0_s2" type="structure" />
    </statement>
    <statement id="d0_s3">
      <text>longest vegetative shoots (0.8–) 1.4–4.4 times as long as longest culms.</text>
      <biological_entity constraint="longest" id="o1864" name="shoot" name_original="shoots" src="d0_s3" type="structure">
        <character is_modifier="true" name="reproduction" src="d0_s3" value="vegetative" value_original="vegetative" />
        <character constraint="culm" constraintid="o1865" is_modifier="false" name="length_or_length" src="d0_s3" value="(0.8-)1.4-4.4 times as long as longest culms" />
      </biological_entity>
      <biological_entity id="o1865" name="culm" name_original="culms" src="d0_s3" type="structure" />
    </statement>
    <statement id="d0_s4">
