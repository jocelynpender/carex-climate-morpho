        <character char_type="range_value" from="1.6" from_unit="mm" name="thickness" src="d0_s1" to="2.2" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Culms strongly purple-red to (3.7–) 4.7–11.7 cm high at base, 6–34 cm;</text>
