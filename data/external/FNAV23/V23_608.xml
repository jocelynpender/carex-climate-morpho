      </biological_entity>
      <biological_entity id="o11106" name="summit" name_original="summits" src="d0_s2" type="structure">
        <character is_modifier="false" modifier="usually" name="shape" src="d0_s2" value="u--shaped" value_original="u--shaped" />
        <character is_modifier="false" modifier="occasionally" name="length" src="d0_s2" value="prolonged" value_original="prolonged" />
