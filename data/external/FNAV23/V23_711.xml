        <character is_modifier="false" modifier="adaxially" name="relief" src="d0_s2" value="papillose" value_original="papillose" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>Inflorescences: peduncle of proximal spike to 11 cm;</text>
      <biological_entity id="o22381" name="inflorescence" name_original="inflorescences" src="d0_s3" type="structure" />
      <biological_entity id="o22382" name="peduncle" name_original="peduncle" src="d0_s3" type="structure">
        <character char_type="range_value" from="0" from_unit="cm" name="some_measurement" src="d0_s3" to="11" to_unit="cm" />
      </biological_entity>
      <biological_entity constraint="proximal" id="o22383" name="spike" name_original="spike" src="d0_s3" type="structure" />
      <relation from="o22382" id="r2714" name="part_of" negation="false" src="d0_s3" to="o22383" />
    </statement>
    <statement id="d0_s4">
      <text>proximal bract longer than inflorescence, 3–10 mm wide.</text>
      <biological_entity id="o22384" name="inflorescence" name_original="inflorescences" src="d0_s4" type="structure" />
