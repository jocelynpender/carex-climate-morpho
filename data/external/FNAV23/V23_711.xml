        <character char_type="range_value" from="4.5" from_unit="cm" name="length" src="d0_s6" to="11.5" to_unit="cm" />
        <character char_type="range_value" from="4" from_unit="mm" name="width" src="d0_s6" to="7" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s7">
