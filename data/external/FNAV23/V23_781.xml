        <character is_modifier="false" name="pubescence" src="d0_s10" value="glabrous" value_original="glabrous" />
      </biological_entity>
      <relation from="o30350" id="r3754" name="with" negation="false" src="d0_s10" to="o30351" />
    </statement>
    <statement id="d0_s11">
      <text>beak 0–1.8 (–2.2) mm, orifice entire or bidentate.</text>
      <biological_entity id="o30355" name="beak" name_original="beak" src="d0_s11" type="structure">
        <character char_type="range_value" from="1.8" from_inclusive="false" from_unit="mm" name="atypical_some_measurement" src="d0_s11" to="2.2" to_unit="mm" />
        <character char_type="range_value" from="0" from_unit="mm" name="some_measurement" src="d0_s11" to="1.8" to_unit="mm" />
