        <character constraint="than midvein" constraintid="o30338" is_modifier="false" name="prominence" src="d0_s4" value="more prominent" value_original="more prominent" />
      </biological_entity>
      <biological_entity id="o30338" name="midvein" name_original="midvein" src="d0_s4" type="structure" />
