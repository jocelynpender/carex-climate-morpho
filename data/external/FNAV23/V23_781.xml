      <biological_entity constraint="adaxial" id="o30335" name="side" name_original="side" src="d0_s4" type="structure">
        <character constraint="than midvein" constraintid="o30338" is_modifier="false" name="prominence" src="d0_s4" value="more prominent" value_original="more prominent" />
        <character is_modifier="false" modifier="not" name="shape" src="d0_s4" value="septate-nodulose" value_original="septate-nodulose" />
      </biological_entity>
      <biological_entity id="o30336" name="blade" name_original="blades" src="d0_s4" type="structure" />
