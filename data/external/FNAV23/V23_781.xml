        <character char_type="range_value" from="0" from_unit="mm" name="width" src="d0_s4" to="5" to_unit="mm" />
        <character constraint="on abaxial surface" constraintid="o30340" is_modifier="false" name="relief" src="d0_s4" value="papillose" value_original="papillose" />
        <character is_modifier="false" name="pubescence" notes="" src="d0_s4" value="glabrous" value_original="glabrous" />
      </biological_entity>
      <biological_entity constraint="abaxial" id="o30340" name="surface" name_original="surface" src="d0_s4" type="structure" />
