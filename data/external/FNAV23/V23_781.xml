  <description type="morphology">
    <statement id="d0_s0">
      <text>Plants loosely cespitose or, usually, colonial, rhizomatous.</text>
      <biological_entity id="o30325" name="whole_organism" name_original="" src="" type="structure">
        <character is_modifier="false" modifier="loosely" name="growth_form" src="d0_s0" value="cespitose" value_original="cespitose" />
        <character name="growth_form" src="d0_s0" value="," value_original="," />
        <character is_modifier="false" name="growth_form" src="d0_s0" value="colonial" value_original="colonial" />
        <character is_modifier="false" name="architecture" src="d0_s0" value="rhizomatous" value_original="rhizomatous" />
        <character name="growth_form" value="plant" />
      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>Culms brown or purple at base.</text>
      <biological_entity id="o30326" name="culm" name_original="culms" src="d0_s1" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s1" value="brown" value_original="brown" />
        <character constraint="at base" constraintid="o30327" is_modifier="false" name="coloration" src="d0_s1" value="purple" value_original="purple" />
      </biological_entity>
      <biological_entity id="o30327" name="base" name_original="base" src="d0_s1" type="structure" />
    </statement>
    <statement id="d0_s2">
      <text>Leaves: basal sheaths sometimes fibrous;</text>
      <biological_entity id="o30328" name="leaf" name_original="leaves" src="d0_s2" type="structure" />
      <biological_entity constraint="basal" id="o30329" name="sheath" name_original="sheaths" src="d0_s2" type="structure">
        <character is_modifier="false" modifier="sometimes" name="texture" src="d0_s2" value="fibrous" value_original="fibrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>sheath fronts membranous;</text>
      <biological_entity id="o30330" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
      <biological_entity constraint="sheath" id="o30331" name="front" name_original="fronts" src="d0_s3" type="structure">
        <character is_modifier="false" name="texture" src="d0_s3" value="membranous" value_original="membranous" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>blades M-shaped in cross-section when young, adaxial side of blades with 2 lateral-veins more prominent than midvein, not septate-nodulose, widest leaf-blade usually less than 5 mm wide, papillose on abaxial surface, glabrous.</text>
      <biological_entity id="o30332" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
