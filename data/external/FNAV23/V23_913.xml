        <character is_modifier="false" name="architecture_or_pubescence_or_relief" src="d0_s2" value="smooth" value_original="smooth" />
        <character is_modifier="false" modifier="somewhat; distally" name="shape" src="d0_s2" value="scabrous-angled" value_original="scabrous-angled" />
      </biological_entity>
