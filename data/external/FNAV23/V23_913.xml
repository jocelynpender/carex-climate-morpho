        <character is_modifier="false" name="shape" src="d0_s5" value="m--shaped" value_original="m--shaped" />
        <character char_type="range_value" from="5.5" from_unit="mm" name="width" src="d0_s5" to="8.5" to_inclusive="false" to_unit="mm" />
