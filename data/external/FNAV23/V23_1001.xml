      </biological_entity>
      <relation from="o6056" id="r723" name="with" negation="false" src="d0_s5" to="o6057" />
    </statement>
    <statement id="d0_s6">
      <text>peduncles of basal spikes arching, elongate, slender;</text>
      <biological_entity id="o6058" name="peduncle" name_original="peduncles" src="d0_s6" type="structure" constraint="spike" constraint_original="spike; spike">
        <character is_modifier="false" name="orientation" src="d0_s6" value="arching" value_original="arching" />
