      <relation from="o6060" id="r725" name="part_of" negation="false" src="d0_s7" to="o6061" />
    </statement>
    <statement id="d0_s8">
      <text>proximal cauline bracts leaflike, usually shorter than (occasionally slightly exceeding) inflorescences.</text>
