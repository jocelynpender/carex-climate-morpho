        <character char_type="range_value" from="1.4" from_unit="mm" name="width" src="d0_s4" to="4.5" to_unit="mm" />
        <character is_modifier="false" name="growth_form_or_texture" src="d0_s4" value="herbaceous" value_original="herbaceous" />
        <character is_modifier="false" modifier="abaxially" name="pubescence" src="d0_s4" value="glabrous" value_original="glabrous" />
        <character char_type="range_value" from="strongly scabrous" modifier="adaxially" name="relief" src="d0_s4" to="papillose" />
      </biological_entity>
      <biological_entity id="o6055" name="stem" name_original="stems" src="d0_s4" type="structure" />
    </statement>
    <statement id="d0_s5">
      <text>Inflorescences usually with both staminate and pistillate spikes;</text>
      <biological_entity id="o6056" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure" />
      <biological_entity id="o6057" name="spike" name_original="spikes" src="d0_s5" type="structure">
