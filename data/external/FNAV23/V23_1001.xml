        <character name="growth_form" value="plant" />
      </biological_entity>
    </statement>
    <statement id="d0_s1">
      <text>rhizomes arching to ascending, dark reddish-brown to purplish brown, 0–12 mm, slender or stout.</text>
      <biological_entity id="o6051" name="rhizome" name_original="rhizomes" src="d0_s1" type="structure">
        <character char_type="range_value" from="arching" name="orientation" src="d0_s1" to="ascending" />
