      </biological_entity>
    </statement>
    <statement id="d0_s11">
      <text>beak 0.2–0.3 mm, shallowly bidentate, smooth or serrulate.</text>
      <biological_entity id="o12641" name="beak" name_original="beak" src="d0_s11" type="structure">
