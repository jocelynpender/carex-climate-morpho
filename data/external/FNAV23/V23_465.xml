      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>blades not epistomic, to 100 cm × 15 mm.</text>
      <biological_entity id="o1805" name="leaf" name_original="leaves" src="d0_s5" type="structure" />
      <biological_entity id="o1806" name="blade" name_original="blades" src="d0_s5" type="structure">
