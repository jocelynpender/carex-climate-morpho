        <character char_type="range_value" from="0" from_unit="mm" name="length" src="d0_s12" to="2" to_unit="mm" />
        <character char_type="range_value" from="0" from_unit="mm" name="width" src="d0_s12" to="1.5" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s13">
      <text>stalk to 0.3 mm;</text>
      <biological_entity id="o1817" name="stalk" name_original="stalk" src="d0_s13" type="structure" />
    </statement>
    <statement id="d0_s14">
      <text>persistent style base cylindric.</text>
      <biological_entity constraint="style" id="o1818" name="base" name_original="base" src="d0_s14" type="structure">
