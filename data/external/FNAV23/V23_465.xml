        <character is_modifier="false" name="pubescence_or_relief" src="d0_s1" value="scabrous" value_original="scabrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Leaves: sheaths usually all with blades, green;</text>
      <biological_entity id="o1795" name="leaf" name_original="leaves" src="d0_s2" type="structure" />
