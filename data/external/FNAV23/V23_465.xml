      </biological_entity>
      <biological_entity id="o1801" name="extension" name_original="extension" src="d0_s3" type="structure" />
      <relation from="o1800" id="r225" name="forming" negation="true" src="d0_s3" to="o1801" />
    </statement>
    <statement id="d0_s4">
      <text>ligules acute, 10 mm, free limb to 0.8 mm;</text>
      <biological_entity id="o1802" name="leaf" name_original="leaves" src="d0_s4" type="structure" />
      <biological_entity id="o1803" name="ligule" name_original="ligules" src="d0_s4" type="structure">
