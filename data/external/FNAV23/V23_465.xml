      </biological_entity>
      <relation from="o1807" id="r226" name="with" negation="false" src="d0_s6" to="o1808" />
    </statement>
    <statement id="d0_s7">
      <text>proximal internode to 10 mm.</text>
      <biological_entity constraint="proximal" id="o1809" name="internode" name_original="internode" src="d0_s7" type="structure">
        <character char_type="range_value" from="0" from_unit="mm" name="some_measurement" src="d0_s7" to="10" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s8">
      <text>Scales hyaline.</text>
      <biological_entity id="o1810" name="scale" name_original="scales" src="d0_s8" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s8" value="hyaline" value_original="hyaline" />
      </biological_entity>
    </statement>
    <statement id="d0_s9">
      <text>Perigynia pale-brown with redbrown veins, 15-veined abaxially, 7-veined adaxially, to 6 × 2 mm, base distended proximally, cordate;</text>
