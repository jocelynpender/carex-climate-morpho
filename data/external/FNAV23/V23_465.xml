      <relation from="o1792" id="r223" name="part_of" negation="false" src="d0_s0" to="o1793" />
    </statement>
    <statement id="d0_s1">
      <text>Culms easily compressed, to 120 cm × 7 mm, scabrous.</text>
      <biological_entity id="o1794" name="culm" name_original="culms" src="d0_s1" type="structure">
        <character is_modifier="false" modifier="easily" name="shape" src="d0_s1" value="compressed" value_original="compressed" />
