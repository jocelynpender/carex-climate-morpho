      <relation from="o1796" id="r224" name="with" negation="false" src="d0_s2" to="o1797" />
    </statement>
    <statement id="d0_s3">
      <text>fronts rugose, indistinctly linearly veined, apex colorless, fragile, convex, not forming extension, erose;</text>
      <biological_entity id="o1798" name="leaf" name_original="leaves" src="d0_s3" type="structure" />
      <biological_entity id="o1799" name="front" name_original="fronts" src="d0_s3" type="structure">
        <character is_modifier="false" name="relief" src="d0_s3" value="rugose" value_original="rugose" />
        <character is_modifier="false" modifier="indistinctly linearly" name="architecture" src="d0_s3" value="veined" value_original="veined" />
      </biological_entity>
      <biological_entity id="o1800" name="apex" name_original="apex" src="d0_s3" type="structure">
        <character is_modifier="false" name="coloration" src="d0_s3" value="colorless" value_original="colorless" />
