        <character is_modifier="false" name="prominence" src="d0_s7" value="raised" value_original="raised" />
        <character is_modifier="false" name="prominence" src="d0_s7" value="prominent" value_original="prominent" />
        <character is_modifier="false" name="shape" src="d0_s7" value="short-mucronate" value_original="short-mucronate" />
      </biological_entity>
      <biological_entity id="o22927" name="body" name_original="body" src="d0_s7" type="structure" />
      <relation from="o22926" id="r2799" name="as" negation="false" src="d0_s7" to="o22927" />
    </statement>
    <statement id="d0_s8">
      <text>Perigynia ascending, chestnut, veinless, ovate, 3–3.5 × 2–2.5 mm, apex abruptly beaked, papillose;</text>
      <biological_entity id="o22928" name="perigynium" name_original="perigynia" src="d0_s8" type="structure">
        <character is_modifier="false" name="orientation" src="d0_s8" value="ascending" value_original="ascending" />
        <character is_modifier="false" name="coloration" src="d0_s8" value="chestnut" value_original="chestnut" />
        <character is_modifier="false" name="architecture" src="d0_s8" value="veinless" value_original="veinless" />
        <character is_modifier="false" name="shape" src="d0_s8" value="ovate" value_original="ovate" />
        <character char_type="range_value" from="3" from_unit="mm" name="length" src="d0_s8" to="3.5" to_unit="mm" />
        <character char_type="range_value" from="2" from_unit="mm" name="width" src="d0_s8" to="2.5" to_unit="mm" />
