      <biological_entity id="o10306" name="scale" name_original="scales" src="d0_s11" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s11" value="pistillate" value_original="pistillate" />
        <character char_type="range_value" from="pale" name="coloration" src="d0_s11" to="dark reddish-brown" />
