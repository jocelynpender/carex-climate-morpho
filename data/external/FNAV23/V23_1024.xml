      </biological_entity>
      <relation from="o10292" id="r1248" name="part_of" negation="false" src="d0_s6" to="o10293" />
    </statement>
    <statement id="d0_s7">
      <text>proximal cauline bracts leakflike, shorter than inflorescences.</text>
      <biological_entity constraint="proximal cauline" id="o10294" name="bract" name_original="bracts" src="d0_s7" type="structure">
        <character is_modifier="false" name="shape" src="d0_s7" value="leakflike" value_original="leakflike" />
        <character constraint="than inflorescences" constraintid="o10295" is_modifier="false" name="height_or_length_or_size" src="d0_s7" value="shorter" value_original="shorter" />
