    </statement>
    <statement id="d0_s10">
      <text>staminate spikes (3–) 6.1–9.3 × 1.1–1.7 mm.</text>
      <biological_entity id="o10303" name="spike" name_original="spikes" src="d0_s10" type="structure" />
      <biological_entity id="o10304" name="spike" name_original="spikes" src="d0_s10" type="structure">
