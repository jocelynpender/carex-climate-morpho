      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>bases (remnants of old leaves) weakly fibrous.</text>
      <biological_entity id="o10287" name="base" name_original="bases" src="d0_s3" type="structure">
        <character is_modifier="false" modifier="weakly" name="texture" src="d0_s3" value="fibrous" value_original="fibrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s4">
      <text>Leaf-blades green, occasionally glaucous, exceeding culms, 1.7–3 mm wide, herbaceous, papillose to scabrous abaxially, papillose to scabrous adaxially.</text>
