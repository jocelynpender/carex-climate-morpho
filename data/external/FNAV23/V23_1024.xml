        <character char_type="range_value" from="2.7" from_unit="mm" name="length" src="d0_s11" to="4.2" to_unit="mm" />
        <character char_type="range_value" from="1.2" from_unit="mm" name="width" src="d0_s11" to="2" to_unit="mm" />
