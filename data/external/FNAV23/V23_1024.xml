        <character char_type="range_value" from="1.7" from_unit="mm" name="width" src="d0_s4" to="3" to_unit="mm" />
        <character is_modifier="false" name="growth_form_or_texture" src="d0_s4" value="herbaceous" value_original="herbaceous" />
        <character char_type="range_value" from="papillose" modifier="adaxially" name="relief" src="d0_s4" to="scabrous abaxially" />
      </biological_entity>
      <biological_entity id="o10289" name="culm" name_original="culms" src="d0_s4" type="structure" />
      <relation from="o10288" id="r1246" name="exceeding" negation="false" src="d0_s4" to="o10289" />
    </statement>
    <statement id="d0_s5">
      <text>Inflorescences with both staminate and pistillate spikes;</text>
      <biological_entity id="o10290" name="inflorescence" name_original="inflorescences" src="d0_s5" type="structure" />
