    </statement>
    <statement id="d0_s8">
      <text>Spikes: proximal pistillate spikes 2–3 (basal spikes 0);</text>
      <biological_entity id="o10296" name="spike" name_original="spikes" src="d0_s8" type="structure" />
      <biological_entity constraint="proximal" id="o10297" name="spike" name_original="spikes" src="d0_s8" type="structure" />
      <biological_entity id="o10298" name="spike" name_original="spikes" src="d0_s8" type="structure">
        <character is_modifier="true" name="architecture" src="d0_s8" value="pistillate" value_original="pistillate" />
        <character char_type="range_value" from="2" name="quantity" src="d0_s8" to="3" />
      </biological_entity>
    </statement>
    <statement id="d0_s9">
      <text>cauline spikes overlapping, proximal 2 separated by less than 7 mm, with 4–8 perigynia;</text>
      <biological_entity id="o10299" name="spike" name_original="spikes" src="d0_s9" type="structure" />
