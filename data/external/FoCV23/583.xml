  <TaxonIdentification Status="ACCEPTED">
    <genus_name>Scirpus</genus_name>
    <species_name>hainanensis</species_name>
    <species_authority>S. M. Huang</species_authority>
    <place_of_publication>
      <publication_title>Fl. Hainan.</publication_title>
      <place_in_publication>4: 538. 1977</place_in_publication>
    </place_of_publication>
