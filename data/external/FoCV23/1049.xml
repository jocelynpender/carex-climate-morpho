        <character name="shape" value="trigonous" />
        <character name="external_texture_or_feature" value="leathery" />
        <character name="architecture_or_feature" value="veined" modifier="faintly" />
        <character name="height_or_length" value="shorter" modifier="slightly" />
        <character name="size" value="contracted" modifier="abruptly" constraint="into beak" constraintid="o1702" />
      </structure>
      <relation id="r382" name="equaling" from="o1700" to="o1701" negation="false" />
      <structure id="o1702" name="beak">
        <character name="height_or_length" value="short" />
      </structure>
      <structure id="o1703" name="orifice">
        <character name="shape" value="2-toothed" />
      </structure>
    </statement>
    <statement id="1049.txt-9">
      <text>Nutlets obovate, trigonous, 2–2.3 mm;</text>
      <structure id="o1704" name="nutlet">
        <character name="shape" value="obovate" />
        <character name="shape" value="trigonous" />
