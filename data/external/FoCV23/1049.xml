      </structure>
      <structure id="o1696" name="peduncle">
        <character name="height_or_length" value="short" modifier="rather" />
      </structure>
    </statement>
    <statement id="1049.txt-7">
      <text>Female glumes purple, ovate-lanceolate, 5–6 mm, margins broadly white hyaline, apex acuminate.</text>
      <structure id="o1697" name="glume" constraint="female">
        <character name="coloration" value="purple" />
        <character name="shape" value="ovate-lanceolate" />
        <character char_type="range_value" name="size" from="5" from_unit="mm" to="6" to_unit="mm" />
      </structure>
      <structure id="o1698" name="margin">
        <character name="coloration" value="hyaline" modifier="broadly;white" />
      </structure>
      <structure id="o1699" name="apex">
        <character name="shape" value="acuminate" />
      </structure>
    </statement>
    <statement id="1049.txt-8">
      <text>Utricles yellow-green, purple above, equaling or slightly shorter than glume, elliptic-obovate, trigonous, leathery, faintly veined, apex abruptly contracted into a short beak, orifice 2-toothed.</text>
      <structure id="o1700" name="utricle">
        <character name="coloration" value="yellow-green" />
        <character name="coloration" value="purple" />
      </structure>
      <structure id="o1701" name="apex">
