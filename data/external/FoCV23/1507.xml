    </statement>
    <statement id="1507.txt-4">
      <text>Spikes 3–5;</text>
      <structure id="o15687" name="spike">
        <character char_type="range_value" name="count" from="3" to="5" />
      </structure>
    </statement>
    <statement id="1507.txt-5">
      <text>terminal spike male, clavate, 1–1.5 cm;</text>
      <structure id="o15688" name="spike" constraint="terminal">
        <character name="feature_or_reproduction" value="male" />
        <character name="shape" value="clavate" />
        <character char_type="range_value" name="size" from="1" from_unit="cm" to="1.5" to_unit="cm" />
      </structure>
    </statement>
    <statement id="1507.txt-6">
      <text>remaining spikes female, oblong, 5–15 mm, lower spikes with peduncles 5–10 mm, upper sessile.</text>
      <structure id="o15689" name="spike" constraint="remaining">
        <character name="reproduction" value="female" />
        <character name="shape" value="oblong" />
