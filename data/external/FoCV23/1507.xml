      </structure>
    </statement>
    <statement id="1507.txt-8">
      <text>Utricles purple-red punctate above, equaling glume, broadly obovate or suborbicular, planoconvex, 2–2.2 × 1.8–2 mm, membranous, densely minutely papillose, nerveless, base shortly stipitate, apex rounded, with very short beak, orifice emarginate.</text>
      <structure id="o15695" name="utricle">
        <character name="coloration" value="purple-red" />
        <character name="pattern" value="punctate" />
        <character name="shape" value="obovate" modifier="broadly" />
        <character name="shape" value="suborbicular" />
        <character name="shape" value="planoconvex" />
        <character char_type="range_value" name="length" from="2" from_unit="mm" to="2.2" to_unit="mm" />
        <character char_type="range_value" name="width" from="1.8" from_unit="mm" to="2" to_unit="mm" />
        <character name="feature" value="membranous" />
        <character name="relief" value="papillose" modifier="densely minutely" />
        <character name="feature" value="nerveless" />
      </structure>
      <structure id="o15696" name="glume" />
      <relation id="r2996" name="equaling" from="o15695" to="o15696" negation="false" />
      <structure id="o15697" name="base">
