        <character name="arrangement_or_shape" value="linear" />
        <character char_type="range_value" name="width" from="2" from_unit="mm" to="4" to_unit="mm" />
      </structure>
      <structure id="o15685" name="margin">
        <character name="shape_or_vernation" value="revolute" />
      </structure>
    </statement>
    <statement id="1507.txt-3">
      <text>Involucral bracts glumelike, lowest one setaceous, sheathless.</text>
      <structure id="o15686" name="bract" constraint="involucral">
        <character name="feature" value="glumelike" />
        <character name="feature" value="lowest" />
        <character name="feature_or_shape" value="setaceous" />
        <character name="feature" value="sheathless" />
