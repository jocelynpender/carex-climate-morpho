      </structure>
      <structure id="o15691" name="peduncle">
        <character char_type="range_value" name="size" from="5" from_unit="mm" to="10" to_unit="mm" />
      </structure>
      <relation id="r2995" name="with" from="o15690" to="o15691" negation="false" />
    </statement>
    <statement id="1507.txt-7">
      <text>Female glumes blackish purple-red, ovate, 2–2.2 × 1.8–2 mm, 1-veined, margins broadly white hyaline, apex rounded.</text>
      <structure id="o15692" name="glume" constraint="female">
        <character name="coloration" value="purple-red" modifier="blackish" />
        <character name="shape" value="ovate" />
        <character char_type="range_value" name="length" from="2" from_unit="mm" to="2.2" to_unit="mm" />
        <character char_type="range_value" name="width" from="1.8" from_unit="mm" to="2" to_unit="mm" />
        <character name="architecture_or_feature" value="1-veined" />
      </structure>
      <structure id="o15693" name="margin">
        <character name="coloration" value="hyaline" modifier="broadly;white" />
      </structure>
      <structure id="o15694" name="apex">
