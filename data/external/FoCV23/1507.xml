  <description>
    <statement id="1507.txt-0">
      <text>Rhizome short, obliquely ascending with curving stolons.</text>
      <structure id="o15676" name="rhizome">
        <character name="height_or_length" value="short" />
        <character name="orientation" value="ascending" modifier="obliquely" constraint="with stolons" constraintid="o15677" />
      </structure>
      <structure id="o15677" name="stolon">
        <character name="course_or_orientation" value="curving" />
      </structure>
    </statement>
    <statement id="1507.txt-1">
      <text>Culms 40–50 cm tall, trigonous, smooth, rigid, clothed at base with bladed sheaths but sterile culm with purple-red bladeless sheaths.</text>
      <structure id="o15678" name="culm">
        <character char_type="range_value" name="size" from="40" from_unit="cm" to="50" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="shape" value="trigonous" />
        <character name="architecture_or_external_texture_or_relief" value="smooth" />
        <character name="internal_texture" value="rigid" />
      </structure>
      <structure id="o15679" name="sheath">
        <character name="type" value="sterile culm" />
      </structure>
      <relation id="r2993" name="clothed at base with" from="o15678" to="o15679" negation="false" />
      <structure id="o15680" name="culm" constraint="sterile" />
      <structure id="o15681" name="sheath" constraint="bladeless">
        <character name="coloration" value="purple-red" />
      </structure>
      <relation id="r2994" name="with" from="o15679" to="o15681" negation="false" />
    </statement>
    <statement id="1507.txt-2">
      <text>Leaves shorter than culm, blades gray-green, linear, 2–4 mm wide, margins revolute.</text>
      <structure id="o15682" name="leaf">
        <character name="height_or_length" value="shorter" constraint="than culm" constraintid="o15683" />
      </structure>
      <structure id="o15683" name="culm" />
      <structure id="o15684" name="blade">
