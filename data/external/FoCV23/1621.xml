        <character name="relief" value="scabrid" />
      </structure>
    </statement>
    <statement id="1621.txt-4">
      <text>Inflorescence 1-spiked, terminal, androgynous, pale brown, ovate-elliptic, 1–1.3 × ca. 0.5 cm;</text>
    </statement>
    <statement id="1621.txt-5">
      <text>male part short, 3–6-flowered;</text>
    </statement>
    <statement id="1621.txt-6">
