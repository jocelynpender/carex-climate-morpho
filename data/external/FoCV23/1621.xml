      <relation id="r3567" name="split into" from="o18724" to="o18726" negation="true" />
    </statement>
    <statement id="1621.txt-3">
      <text>blades slightly shorter to slightly longer than culm, linear, flat, ca. 1 mm wide, smooth, margins scabrid.</text>
      <structure id="o18727" name="blade">
        <character name="arrangement_or_shape" value="linear" />
        <character name="prominence_or_shape" value="flat" />
        <character name="width" value="1" unit="mm" />
