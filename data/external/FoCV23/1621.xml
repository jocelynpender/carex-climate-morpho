        <character name="shape" value="compressed trigonous" />
        <character name="architecture" value="2-leaved" modifier="in lower 1/3" />
      </structure>
    </statement>
    <statement id="1621.txt-2">
      <text>Basal sheaths with blades, pale brown, not split into fibers;</text>
      <structure id="o18724" name="sheath" constraint="basal">
        <character name="coloration" value="brown" modifier="pale" />
      </structure>
      <structure id="o18725" name="blade" />
