  <description>
    <statement id="968.txt-0">
      <text>Basal sheaths reddish brown.</text>
      <structure id="o44562" name="sheath" constraint="basal">
        <character name="coloration" value="brown" modifier="reddish" />
      </structure>
    </statement>
    <statement id="968.txt-1">
      <text>Inflorescences 1–3 × 0.2–0.3 cm.</text>
      <structure id="o44563" name="inflorescence">
        <character char_type="range_value" name="length" from="1" from_unit="cm" to="3" to_unit="cm" />
        <character char_type="range_value" name="width" from="0.2" from_unit="cm" to="0.3" to_unit="cm" />
      </structure>
    </statement>
    <statement id="968.txt-2">
      <text>Glumes yellowish brown or reddish brown to brown, 2.5–4 × 1.2–2 mm.</text>
      <structure id="o44564" name="glume">
        <character char_type="range_value" name="coloration" from="yellowish brown" to="brown" />
        <character char_type="range_value" name="coloration" from="reddish brown" to="brown" />
        <character name="coloration" value="brown" modifier="yellowish" />
        <character name="coloration" value="brown" modifier="reddish" />
        <character name="coloration" value="brown" />
        <character char_type="range_value" name="length" from="2.5" from_unit="mm" to="4" to_unit="mm" />
        <character char_type="range_value" name="width" from="1.2" from_unit="mm" to="2" to_unit="mm" />
      </structure>
    </statement>
    <statement id="968.txt-3">
      <text>Prophylls oblong, 2.5–3.5 × 1–1.3 mm.</text>
      <structure id="o44565" name="prophyll">
        <character name="shape" value="oblong" />
        <character char_type="range_value" name="length" from="2.5" from_unit="mm" to="3.5" to_unit="mm" />
        <character char_type="range_value" name="width" from="1" from_unit="mm" to="1.3" to_unit="mm" />
      </structure>
    </statement>
    <statement id="968.txt-4">
      <text>Nutlets 2.5–3 (including beak )× 0.8–1 mm.</text>
      <structure id="o44566" name="nutlet">
        <character char_type="range_value" name="length" from="2.5" from_unit="mm" to="3" to_unit="mm" />
        <character char_type="range_value" name="width" from="0.8" from_unit="mm" to="1" to_unit="mm" />
      </structure>
    </statement>
