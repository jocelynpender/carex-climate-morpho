      </structure>
      <structure id="o16925" name="orifice">
        <character name="shape" value="entire" />
      </structure>
    </statement>
    <statement id="1554.txt-9">
      <text>Nutlets elliptic or obovate-elliptic, 1.7–2.3 mm;</text>
      <structure id="o16926" name="nutlet">
        <character name="arrangement_or_shape" value="elliptic" />
        <character name="arrangement_or_shape" value="obovate-elliptic" />
        <character char_type="range_value" name="size" from="1.7" from_unit="mm" to="2.3" to_unit="mm" />
      </structure>
    </statement>
    <statement id="1554.txt-10">
