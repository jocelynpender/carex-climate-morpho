    </statement>
    <statement id="1232.txt-4">
      <text>Spikes numerous, arising single or binate from involucral bract axil;</text>
      <structure id="o7156" name="axil" constraint="involucral bract" />
    </statement>
    <statement id="1232.txt-5">
      <text>upper 1–3 male, 1–3 cm;</text>
