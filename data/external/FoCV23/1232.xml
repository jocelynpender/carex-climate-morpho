    <statement id="1232.txt-0">
      <text>Rhizome long creeping.</text>
      <structure id="o7144" name="rhizome">
        <character name="habit" value="creeping" />
      </structure>
    </statement>
    <statement id="1232.txt-1">
      <text>Culms 40–60 cm tall, trigonous, smooth, clothed at base with purple-reddish sheaths disintegrating into fibers.</text>
      <structure id="o7145" name="culm">
        <character char_type="range_value" name="size" from="40" from_unit="cm" to="60" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="shape" value="trigonous" />
        <character name="architecture_or_external_texture_or_relief" value="smooth" />
      </structure>
      <structure id="o7146" name="base" />
      <relation id="r1288" name="clothed at" from="o7145" to="o7146" negation="false" />
      <structure id="o7147" name="sheath">
        <character name="coloration" value="purple-reddish" />
        <character name="dehiscence" value="disintegrating" constraint="into fibers" constraintid="o7148" />
      </structure>
      <relation id="r1289" name="with" from="o7146" to="o7147" negation="false" />
      <structure id="o7148" name="fiber" />
    </statement>
    <statement id="1232.txt-2">
      <text>Leaves slightly shorter than culm, blades linear, 2–3 mm wide, leathery, stiff.</text>
      <structure id="o7149" name="leaf">
        <character name="height_or_length" value="shorter" modifier="slightly" constraint="than culm" constraintid="o7150" />
      </structure>
      <structure id="o7150" name="culm" />
      <structure id="o7151" name="blade">
        <character name="arrangement_or_shape" value="linear" />
        <character char_type="range_value" name="width" from="2" from_unit="mm" to="3" to_unit="mm" />
        <character name="external_texture_or_feature" value="leathery" />
        <character name="external_texture" value="stiff" />
      </structure>
    </statement>
    <statement id="1232.txt-3">
      <text>Lower involucral bracts leaflike, shorter than inflorescence, sheathing, 3–4 mm, purple-reddish, upper blades setaceous.</text>
      <structure id="o7152" name="bract" constraint="lower involucral">
        <character name="architecture" value="leaflike" />
        <character name="height_or_length" value="shorter" constraint="than inflorescence" constraintid="o7153" />
        <character name="architecture_or_feature_or_shape" value="sheathing" />
        <character char_type="range_value" name="size" from="3" from_unit="mm" to="4" to_unit="mm" />
        <character name="coloration" value="purple-reddish" />
      </structure>
      <structure id="o7153" name="inflorescence" />
      <structure id="o7154" name="blade" constraint="upper">
        <character name="feature_or_shape" value="setaceous" />
