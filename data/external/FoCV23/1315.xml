      <structure id="o9676" name="spike">
        <character name="count" value="several" />
      </structure>
    </statement>
    <statement id="1315.txt-4">
      <text>terminal spike male, linear-clavate, ca. 2 cm, with peduncles;</text>
      <structure id="o9677" name="spike" constraint="terminal">
