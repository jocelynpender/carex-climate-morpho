        <character name="external_texture_or_relief" value="scabrous" constraint="on surface" constraintid="o9674" />
        <character name="architecture_or_feature" value="sheathed" />
      </structure>
      <structure id="o9673" name="apex" />
      <structure id="o9674" name="surface" constraint="upper" />
      <structure id="o9675" name="sheath" constraint="lower">
