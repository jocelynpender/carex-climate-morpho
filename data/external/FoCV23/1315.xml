        <character name="architecture_or_external_texture_or_relief" value="smooth" />
      </structure>
    </statement>
    <statement id="1315.txt-2">
      <text>Leaves shorter than culm, blades ca. 2 mm wide, flat, slightly revolute at apex, scabrous on upper surface, sheathed, lower sheaths dark brown.</text>
      <structure id="o9670" name="leaf">
