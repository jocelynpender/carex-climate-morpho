  <description>
    <statement id="1668.txt-0">
      <text>Rhizome slender, long creeping.</text>
      <structure id="o19809" name="rhizome">
        <character name="size" value="slender" />
        <character name="habit" value="creeping" />
      </structure>
    </statement>
    <statement id="1668.txt-1">
      <text>Culms 3–10 cm tall, acutely trigonous, smooth, clothed with brown sheaths at base.</text>
      <structure id="o19810" name="culm">
        <character char_type="range_value" name="size" from="3" from_unit="cm" to="10" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="shape" value="trigonous" modifier="acutely" />
        <character name="architecture_or_external_texture_or_relief" value="smooth" />
      </structure>
      <structure id="o19811" name="sheath">
        <character name="coloration" value="brown" />
      </structure>
      <relation id="r3732" name="clothed with" from="o19810" to="o19811" negation="false" />
      <structure id="o19812" name="base" />
      <relation id="r3733" name="at" from="o19811" to="o19812" negation="false" />
    </statement>
    <statement id="1668.txt-2">
      <text>Leaves shorter than or subequaling culm, blades linear, 1–2 mm wide, flat, or involute on margins, stiff, apex acute.</text>
      <structure id="o19813" name="leaf">
        <character name="height_or_length" value="shorter" constraint="than or subequaling culm" constraintid="o19814" />
      </structure>
      <structure id="o19814" name="culm" />
      <structure id="o19815" name="blade">
        <character name="arrangement_or_shape" value="linear" />
        <character char_type="range_value" name="width" from="1" from_unit="mm" to="2" to_unit="mm" />
        <character name="shape" value="flat" />
        <character name="shape" value="involute" constraint="on margins" constraintid="o19816" />
        <character name="external_texture" value="stiff" />
      </structure>
      <structure id="o19816" name="margin" />
      <structure id="o19817" name="apex">
        <character name="shape" value="acute" />
      </structure>
    </statement>
    <statement id="1668.txt-3">
      <text>Involucral bracts glumelike.</text>
      <structure id="o19818" name="bract" constraint="involucral">
        <character name="feature" value="glumelike" />
      </structure>
    </statement>
    <statement id="1668.txt-4">
      <text>Inflorescence capitate, globose or ovate-oblong, 6–12 × 6–8 mm;</text>
      <structure id="o19819" name="inflorescence">
        <character name="shape" value="capitate" />
        <character name="shape" value="globose" />
        <character name="shape" value="ovate-oblong" />
        <character char_type="range_value" name="length" from="6" from_unit="mm" to="12" to_unit="mm" />
        <character char_type="range_value" name="width" from="6" from_unit="mm" to="8" to_unit="mm" />
      </structure>
    </statement>
    <statement id="1668.txt-5">
      <text>spikes 3–5, androgynous, oblong, 5–6 mm, sessile.</text>
      <structure id="o19820" name="spike">
        <character char_type="range_value" name="count" from="3" to="5" />
        <character name="feature_or_reproduction" value="androgynous" />
        <character name="shape" value="oblong" />
