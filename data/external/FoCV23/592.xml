        <character name="shape" value="inflated" modifier="apically slightly" />
      </structure>
      <structure id="o32722" name="vein">
        <character name="coloration" value="reddish" />
        <character name="width" value="fine" />
        <character name="dehiscence_or_orientation" value="transverse" />
      </structure>
      <structure id="o32723" name="apex">
        <character name="shape" value="obtuse" />
      </structure>
    </statement>
    <statement id="592.txt-5">
      <text>Involucral bracts grayish black, glumelike, ovate-lanceolate, 3–9-veined, apex acuminate.</text>
      <structure id="o32724" name="bract" constraint="involucral">
        <character name="coloration" value="black" modifier="grayish" />
