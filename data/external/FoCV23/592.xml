      </structure>
    </statement>
    <statement id="592.txt-7">
      <text>Spikelet oblong-cylindric and 1.5–2.5 cm in flower, obovoid, fairly lax, and 2.5–4 cm in fruit.</text>
      <structure id="o32728" name="spikelet">
        <character name="shape" value="oblong-cylindric" />
        <character char_type="range_value" name="size" from="1.5" from_unit="cm" to="2.5" to_unit="cm" constraint="in flower" constraintid="o32729" />
