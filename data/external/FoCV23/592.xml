      </structure>
      <structure id="o32725" name="apex">
        <character name="shape" value="acuminate" />
      </structure>
    </statement>
    <statement id="592.txt-6">
      <text>Inflorescences terminal with only 1 spikelet.</text>
      <structure id="o32726" name="inflorescence">
        <character name="position" value="terminal" constraint="with spikelet" constraintid="o32727" />
      </structure>
      <structure id="o32727" name="spikelet">
