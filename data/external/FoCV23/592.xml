      </structure>
    </statement>
    <statement id="592.txt-3">
      <text>Cauline leaves 1 or 2, reduced to bladeless leaf sheaths.</text>
      <structure id="o32719" name="leaf" constraint="cauline">
        <character name="count" value="1" />
        <character name="count" value="2" />
      </structure>
      <structure id="o32720" name="sheath" constraint="bladeless leaf" />
      <relation id="r6874" name="reduced to" from="o32719" to="o32720" negation="false" />
    </statement>
    <statement id="592.txt-4">
      <text>Leaf sheaths grayish black with reddish fine transverse veins, cylindric, 6.5–7.5 cm, apically slightly inflated, apex obtuse.</text>
      <structure id="o32721" name="sheath" constraint="leaf">
        <character name="coloration" value="black" modifier="grayish" constraint="with veins" constraintid="o32722" />
        <character name="shape" value="cylindric" />
