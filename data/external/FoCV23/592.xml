  <description>
    <statement id="592.txt-0">
      <text>Rhizomes creeping.</text>
      <structure id="o32715" name="rhizome">
        <character name="habit" value="creeping" />
      </structure>
    </statement>
    <statement id="592.txt-1">
      <text>Culms erect, 30–60 cm tall, slender, terete.</text>
      <structure id="o32716" name="culm">
        <character name="orientation" value="erect" />
        <character char_type="range_value" name="size" from="30" from_unit="cm" to="60" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="size" value="slender" />
        <character name="shape" value="terete" />
      </structure>
    </statement>
    <statement id="592.txt-2">
      <text>Basal leaves linear, ca. 1 mm wide, subterete, apex obtuse.</text>
      <structure id="o32717" name="leaf" constraint="basal">
        <character name="arrangement_or_shape" value="linear" />
        <character name="width" value="1" unit="mm" />
