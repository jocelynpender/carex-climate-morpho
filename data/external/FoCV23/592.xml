      </structure>
      <structure id="o32729" name="flower" />
      <structure id="o32730" name="fruit" />
    </statement>
    <statement id="592.txt-8">
      <text>Glumes brownish gray, ovate-lanceolate, ca. 7 mm, thinly membranous, 1-veined, margin broadly whitish hyaline, apex obtuse.</text>
      <structure id="o32731" name="glume">
        <character name="coloration" value="gray" modifier="brownish" />
