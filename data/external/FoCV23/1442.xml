        <character name="shape" value="rhomboid-oblong" />
        <character name="shape" value="inflated trigonous" />
        <character char_type="range_value" name="size" from="8" from_unit="mm" to="9" to_unit="mm" />
        <character name="architecture_or_feature" value="veined" />
        <character name="external_texture" value="glabrous" />
      </structure>
      <structure id="o13526" name="base">
        <character name="architecture" value="stipitate" modifier="shortly" />
      </structure>
      <structure id="o13527" name="apex">
        <character name="size" value="contracted" modifier="abruptly" constraint="into beak" constraintid="o13528" />
      </structure>
      <structure id="o13528" name="beak">
        <character name="external_texture_or_relief" value="scabrous" />
        <character name="length" value="long" />
      </structure>
      <structure id="o13529" name="orifice">
        <character name="shape" value="2-toothed" />
      </structure>
    </statement>
    <statement id="1442.txt-10">
      <text>Nutlets pale, obovate or oblong, ca. 3.2 mm, faces concave at base, base shortly stipitate, apex abruptly contracted into a short beak;</text>
      <structure id="o13530" name="nutlet">
