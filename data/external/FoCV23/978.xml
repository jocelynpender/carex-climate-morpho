      <place_in_publication>2, 4: 50. 1903</place_in_publication>
    </place_of_publication>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <species_name>不丹嵩草 bu dan song cao</species_name>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <genus_name>Kobresia</genus_name>
    <species_name>prainii</species_name>
    <variety_name>elliptica</variety_name>
    <variety_authority>Y. C. Yang</variety_authority>
