      <structure id="o44070" name="flower" constraint="female">
        <character name="count" value="1" />
        <character name="reproduction" value="bisexual" modifier="occasionally" in_brackets="true" constraint="with flower, flowers" constraintid="o44071  o44072" />
      </structure>
      <structure id="o44071" name="flower" in_brackets="true" constraint="basal female">
        <character name="count" value="1" in_brackets="true" />
      </structure>
      <structure id="o44072" name="flower" in_brackets="true" constraint="distal male">
        <character char_type="range_value" name="count" from="2" to="5" in_brackets="true" />
        <character name="count" value="1" in_brackets="true" />
      </structure>
    </statement>
    <statement id="952.txt-4">
      <text>Glumes ovate-oblong, 3.3–5.1 × 1.7–2.1 mm.</text>
      <structure id="o44073" name="glume">
        <character name="shape" value="ovate-oblong" />
        <character char_type="range_value" name="length" from="3.3" from_unit="mm" to="5.1" to_unit="mm" />
