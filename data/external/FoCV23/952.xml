  <description>
    <statement id="952.txt-0">
      <text>Culms trigonous, or subterete in lower part, 30–75 cm tall, slender, 1–1.5 (–1 .8 )mm in diam.</text>
      <structure id="o44063" name="culm">
        <character name="shape" value="trigonous" />
        <character name="shape" value="subterete" constraint="in part" constraintid="o44064" />
        <character char_type="range_value" name="size" from="30" from_unit="cm" to="75" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="size" value="slender" />
        <character char_type="range_value" name="atypical_size" from="1.5" to="1.8" from_unit="mm" to_unit="mm" from_inclusive="false" />
        <character char_type="range_value" name="size" from="1" from_unit="mm" to="1.5" to_unit="mm" modifier="in diam" />
      </structure>
      <structure id="o44064" name="part" constraint="lower" />
    </statement>
    <statement id="952.txt-1">
      <text>Leaf blade flat, 1.5–2 (–2 .5 )mm wide.</text>
      <structure id="o44065" name="blade" constraint="leaf">
        <character name="prominence_or_shape" value="flat" />
        <character char_type="range_value" name="width" from="2" to="2.5" from_unit="mm" to_unit="mm" from_inclusive="false" />
        <character char_type="range_value" name="width" from="1.5" from_unit="mm" to="2" to_unit="mm" />
      </structure>
    </statement>
    <statement id="952.txt-2">
      <text>Inflorescence narrowly paniculate, compact or slightly loose in lower part, 1.2–3.5 × 0.7–0.9 cm.</text>
      <structure id="o44066" name="inflorescence">
        <character name="architecture" value="paniculate" modifier="narrowly" />
        <character name="architecture" value="compact" />
        <character name="architecture" value="loose" modifier="slightly" constraint="in part" constraintid="o44067" />
        <character char_type="range_value" name="length" from="1.2" from_unit="cm" to="3.5" to_unit="cm" />
        <character char_type="range_value" name="width" from="0.7" from_unit="cm" to="0.9" to_unit="cm" />
      </structure>
      <structure id="o44067" name="part" constraint="lower" />
    </statement>
    <statement id="952.txt-3">
      <text>Lower spikelets of inflorescence branches usually unisexual with 1 female flower only (occasionally bisexual with 1 basal female flower and 2–5 distal male flowers ).</text>
      <structure id="o44068" name="spikelet" constraint="lower">
