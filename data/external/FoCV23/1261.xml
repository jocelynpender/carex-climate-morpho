      </structure>
    </statement>
    <statement id="1261.txt-1">
      <text>Culms 40–50 cm tall, smooth, clothed at base with yellow-brown sheaths.</text>
      <structure id="o8000" name="culm">
        <character char_type="range_value" name="size" from="40" from_unit="cm" to="50" to_unit="cm" />
