      </structure>
      <relation id="r1466" name="with" from="o8001" to="o8002" negation="false" />
    </statement>
    <statement id="1261.txt-2">
      <text>Leaves shorter than culm, blades gray-green, linear, 1–2 mm wide, flat, slightly soft, apex acuminate.</text>
