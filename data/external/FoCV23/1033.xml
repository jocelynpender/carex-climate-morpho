<treatment>
  <number>33.</number>
  <TaxonIdentification Status="ACCEPTED">
    <genus_name>Carex</genus_name>
    <species_name>oligostachya</species_name>
    <species_authority>Nees</species_authority>
    <place_of_publication>
      <publication_title>Hooker’s J. Bot. Kew Gard. Misc.</publication_title>
      <place_in_publication>6: 29. 1854</place_in_publication>
    </place_of_publication>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <species_name>少穗薹草 shao sui tai cao</species_name>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <genus_name>Carex</genus_name>
    <species_name>rhizomatosa</species_name>
    <species_authority>Steudel.</species_authority>
