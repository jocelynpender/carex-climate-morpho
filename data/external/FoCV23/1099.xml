        <character name="coloration" value="brownish" />
        <character name="shape" value="ovate" />
        <character char_type="range_value" name="length" from="2" from_unit="mm" to="3" to_unit="mm" />
        <character char_type="range_value" name="width" from="1.2" from_unit="mm" to="1.5" to_unit="mm" />
        <character name="feature" value="membranous" />
        <character name="architecture_or_feature" value="3-veined" />
        <character name="architecture" value="excurrent" constraint="beyond apex" constraintid="o3147" />
      </structure>
      <structure id="o3146" name="costa" />
      <structure id="o3147" name="apex" />
      <structure id="o3148" name="awn">
        <character name="orientation" value="erect" />
        <character name="size" value="1.5" unit="mm" />
      </structure>
      <relation id="r640" name="into" from="o3147" to="o3148" negation="false" />
    </statement>
    <statement id="1099.txt-9">
      <text>Utricles glaucous green, longer than body of glume but surpassed by awn, obovate to broadly obovate, 2.8–3 × 1.5–2 mm, herbaceous, glabrous, strongly 2-costate and bearing 4 or 5 veins reaching halfway up utricle, base cuneate-attenuate, apex rounded-contracted into a very short beak, orifice scarcely 2-toothed.</text>
      <structure id="o3149" name="utricle">
        <character name="external_texture" value="glaucous" />
        <character name="coloration" value="green" />
        <character name="length" value="longer" constraint="than body" constraintid="o3150" />
        <character char_type="range_value" name="shape" from="obovate" to="broadly obovate" />
        <character name="shape" value="obovate" />
        <character name="shape" value="obovate" modifier="broadly" />
        <character char_type="range_value" name="length" from="2.8" from_unit="mm" to="3" to_unit="mm" />
        <character char_type="range_value" name="width" from="1.5" from_unit="mm" to="2" to_unit="mm" />
        <character name="external_texture_or_feature_or_internal_texture" value="herbaceous" />
        <character name="external_texture" value="glabrous" />
        <character name="architecture" value="2-costate" modifier="strongly" />
