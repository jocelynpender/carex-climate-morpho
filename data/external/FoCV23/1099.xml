  <description>
    <statement id="1099.txt-0">
      <text>Rhizome short, covered with brown scales not disintegrated into fibers.</text>
      <structure id="o3127" name="rhizome">
        <character name="height_or_length" value="short" />
      </structure>
      <structure id="o3128" name="scale">
        <character name="coloration" value="brown" />
      </structure>
      <relation id="r634" name="covered with" from="o3127" to="o3128" negation="false" />
