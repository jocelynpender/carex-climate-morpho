        <character name="count" value="4" />
        <character name="count" value="5" />
      </structure>
      <relation id="r643" name="bearing" from="o3149" to="o3153" negation="false" />
      <structure id="o3154" name="utricle">
        <character name="position" value="halfway" />
      </structure>
      <relation id="r644" name="reaching" from="o3149" to="o3154" negation="false" />
      <structure id="o3155" name="apex">
        <character name="shape" value="cuneate-attenuate" />
        <character name="size" value="rounded-contracted" constraint="into beak" constraintid="o3156" />
