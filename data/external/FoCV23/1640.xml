        <character name="width" value="1.2" unit="mm" />
      </structure>
      <structure id="o19158" name="base">
        <character name="arrangement_or_shape" value="cuneate" modifier="broadly" />
        <character name="architecture" value="stipitate" modifier="shortly" />
      </structure>
    </statement>
    <statement id="1640.txt-10">
      <text>style base slightly thickened;</text>
      <structure id="o19159" name="base" constraint="style">
        <character name="width" value="thickened" modifier="slightly" />
      </structure>
    </statement>
    <statement id="1640.txt-11">
