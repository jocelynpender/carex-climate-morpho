        <character name="external_texture_or_relief" value="scabrous" />
        <character name="length" value="long" />
      </structure>
      <structure id="o20431" name="orifice">
        <character name="shape" value="2-toothed" />
      </structure>
    </statement>
    <statement id="1695.txt-8">
      <text>Nutlets somewhat tightly enveloped, ovate-elliptic, biconvex, base shortly stipitate, apex rounded;</text>
      <structure id="o20432" name="nutlet">
        <character name="feature" value="enveloped" modifier="somewhat tightly" />
