      <relation id="r1432" name="with" from="o7803" to="o7804" negation="false" />
    </statement>
    <statement id="1254.txt-7">
      <text>Female glumes brown, oblong, 4–6 mm, glabrous, margins white hyaline, green 1-veined costa excurrent into a mucro at apex.</text>
