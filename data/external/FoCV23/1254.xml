        <character name="prominence_or_shape" value="flat" />
        <character name="external_texture" value="stiff" />
      </structure>
    </statement>
    <statement id="1254.txt-3">
      <text>Involucral bracts reduced to shortly bladed, sheathless.</text>
      <structure id="o7800" name="bract" constraint="involucral">
        <character name="size" value="reduced" />
        <character name="feature" value="sheathless" />
      </structure>
    </statement>
    <statement id="1254.txt-4">
      <text>Spikes 4 or 5, lowest one subradical;</text>
      <structure id="o7801" name="spike">
        <character name="count" value="4" />
        <character name="count" value="5" />
        <character name="feature" value="lowest" />
        <character name="feature" value="subradical" />
      </structure>
    </statement>
    <statement id="1254.txt-5">
      <text>upper 1–3 spikes male;</text>
      <structure id="o7802" name="spike">
