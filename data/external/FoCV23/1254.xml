    </statement>
    <statement id="1254.txt-1">
      <text>Culms tufted, 8–13 cm tall, rigid, clothed at base with brown bladeless sheaths.</text>
      <structure id="o7794" name="culm">
        <character name="external_texture_or_habit" value="tufted" />
        <character char_type="range_value" name="size" from="8" from_unit="cm" to="13" to_unit="cm" />
