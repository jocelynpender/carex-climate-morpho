      <structure id="o7795" name="base" />
      <relation id="r1430" name="clothed at" from="o7794" to="o7795" negation="false" />
      <structure id="o7796" name="sheath" constraint="bladeless">
        <character name="coloration" value="brown" />
      </structure>
      <relation id="r1431" name="with" from="o7795" to="o7796" negation="false" />
    </statement>
    <statement id="1254.txt-2">
      <text>Leaves shorter than culm, blades linear, 3–4 mm wide, flat, stiff.</text>
      <structure id="o7797" name="leaf">
