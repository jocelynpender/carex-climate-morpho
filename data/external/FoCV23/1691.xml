        <character name="size" value="2.5" unit="mm" />
        <character name="feature" value="membranous" />
        <character name="internal_texture" value="spongy" />
        <character name="count" value="many" />
        <character name="architecture_or_feature" value="veined" constraint="on faces" constraintid="o20336" />
      </structure>
