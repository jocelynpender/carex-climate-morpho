      <structure id="o20330" name="upper">
        <character name="feature" value="setaceous" />
        <character name="feature" value="glumelike" />
      </structure>
    </statement>
    <statement id="1691.txt-4">
      <text>Spikes 9–22, gynaecandrous, ovate to oblong-ovate, 5–12 × ca. 3 mm, upper spikes approximate, lower ones remote.</text>
      <structure id="o20331" name="spike">
        <character char_type="range_value" name="count" from="9" to="22" />
        <character name="feature" value="gynaecandrous" />
        <character char_type="range_value" name="shape" from="ovate" to="oblong-ovate" />
