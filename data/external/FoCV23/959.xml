    </place_of_publication>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <species_name>蕨状嵩草 jue zhuang song cao</species_name>
  </TaxonIdentification>
  <TaxonIdentification Status="BASIONYM">
    <genus_name>Hemicarex</genus_name>
    <species_name>filicina</species_name>
    <species_authority>C. B. Clarke</species_authority>
    <place_of_publication>
      <publication_title>J. Linn. Soc., Bot.</publication_title>
      <place_in_publication>20: 384. 1883</place_in_publication>
    </place_of_publication>
