  <TaxonIdentification Status="SYNONYM">
    <species_name>锥囊薹草 zhui nang tai cao</species_name>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <genus_name>Carex</genus_name>
    <species_name>aristata</species_name>
    <species_authority>R. Brown</species_authority>
