        <character name="count" value="4" />
        <character name="arrangement" value="remote" modifier="rather" />
      </structure>
    </statement>
    <statement id="1336.txt-5">
      <text>terminal spike male, linear, 1–2 cm, shortly pedunculate;</text>
      <structure id="o10315" name="spike" constraint="terminal">
        <character name="feature_or_reproduction" value="male" />
        <character name="arrangement_or_shape" value="linear" />
        <character char_type="range_value" name="size" from="1" from_unit="cm" to="2" to_unit="cm" />
        <character name="architecture" value="pedunculate" modifier="shortly" />
      </structure>
    </statement>
    <statement id="1336.txt-6">
      <text>lateral spikes female, oblong or oblong-cylindric, 1–3 cm, densely many flowered, lowest long pedunculate, upper ones rather shortly pedunculate.</text>
