        <character name="length" value="lowest" />
        <character name="architecture" value="pedunculate" />
        <character name="position" value="upper" />
        <character name="architecture" value="pedunculate" modifier="rather shortly" />
      </structure>
    </statement>
    <statement id="1336.txt-7">
      <text>Female glumes pale, lanceolate-ovate, 4–5 mm, membranous, 1–3 veined, apex with an awn.</text>
      <structure id="o10317" name="glume" constraint="female">
        <character name="coloration" value="pale" />
        <character name="shape" value="lanceolate-ovate" />
