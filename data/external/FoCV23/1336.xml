      <relation id="r1909" name="clothed with bladeless light-c-brown sheaths  at" from="o10305" to="o10306" negation="false" />
      <structure id="o10307" name="fiber" />
    </statement>
    <statement id="1336.txt-2">
      <text>Leaves shorter than culm, blades 3–4 mm wide, flat, rather stiff.</text>
      <structure id="o10308" name="leaf">
        <character name="height_or_length" value="shorter" constraint="than culm" constraintid="o10309" />
      </structure>
      <structure id="o10309" name="culm" />
      <structure id="o10310" name="blade">
        <character char_type="range_value" name="width" from="3" from_unit="mm" to="4" to_unit="mm" />
        <character name="prominence_or_shape" value="flat" />
        <character name="external_texture" value="stiff" modifier="rather" />
      </structure>
    </statement>
    <statement id="1336.txt-3">
      <text>Involucral bracts longer than culm, leaflike, lowest sheath long, upper ones rather short.</text>
      <structure id="o10311" name="bract" constraint="involucral">
        <character name="length" value="longer" constraint="than culm" constraintid="o10312" />
        <character name="architecture" value="leaflike" />
