    </statement>
    <statement id="1336.txt-1">
      <text>Culms tufted, 30–60 cm tall, triquetrous, smooth, clothed with bladeless light brown sheaths at base, few sheaths disintegrating into fibers.</text>
      <structure id="o10305" name="culm">
        <character name="external_texture_or_habit" value="tufted" />
        <character char_type="range_value" name="size" from="30" from_unit="cm" to="60" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="shape" value="triquetrous" />
        <character name="architecture_or_external_texture_or_relief" value="smooth" />
      </structure>
      <structure id="o10306" name="sheath">
        <character name="count" value="few" />
        <character name="dehiscence" value="disintegrating" constraint="into fibers" constraintid="o10307" />
