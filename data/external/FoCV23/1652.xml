        <character name="external_texture_or_relief" value="scabrous" />
      </structure>
    </statement>
    <statement id="1652.txt-3">
      <text>Involucral bracts glumelike.</text>
      <structure id="o19440" name="bract" constraint="involucral">
        <character name="feature" value="glumelike" />
      </structure>
    </statement>
    <statement id="1652.txt-4">
      <text>Spicate inflorescence oblong or oblong-ovate, 1.8–2.7 cm;</text>
      <structure id="o19441" name="inflorescence" constraint="spicate">
