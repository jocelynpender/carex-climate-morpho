      <structure id="o8237" name="spike">
        <character name="count" value="3" />
        <character name="arrangement" value="distant" />
      </structure>
    </statement>
    <statement id="1268.txt-5">
      <text>terminal spike male, linear, 48–50 × 1.5–2.5 mm, densely many flowered;</text>
      <structure id="o8238" name="spike" constraint="terminal">
        <character name="feature_or_reproduction" value="male" />
        <character name="arrangement_or_shape" value="linear" />
        <character char_type="range_value" name="length" from="48" from_unit="mm" to="50" to_unit="mm" />
        <character char_type="range_value" name="width" from="1.5" from_unit="mm" to="2.5" to_unit="mm" />
        <character name="count" value="many" modifier="densely" />
        <character name="architecture" value="flowered" />
      </structure>
    </statement>
    <statement id="1268.txt-6">
      <text>lateral spikes female, cylindric, 2.8–3.2 × 0.3–0.4 cm, slightly densely many flowered, with peduncles 3–9 cm.</text>
      <structure id="o8239" name="spike" constraint="lateral">
        <character name="reproduction" value="female" />
        <character name="shape" value="cylindric" />
        <character char_type="range_value" name="length" from="2.8" from_unit="cm" to="3.2" to_unit="cm" />
        <character char_type="range_value" name="width" from="0.3" from_unit="cm" to="0.4" to_unit="cm" />
        <character name="count" value="many" modifier="slightly densely" />
        <character name="architecture" value="flowered" />
      </structure>
      <structure id="o8240" name="peduncle">
        <character char_type="range_value" name="size" from="3" from_unit="cm" to="9" to_unit="cm" />
      </structure>
      <relation id="r1514" name="with" from="o8239" to="o8240" negation="false" />
    </statement>
    <statement id="1268.txt-7">
      <text>Female glumes brown, ovate or broadly oblong, 3.3–3.5 mm, margins white hyaline, green 1-veined costa forming a mucro, apex acute.</text>
      <structure id="o8241" name="glume" constraint="female">
        <character name="coloration" value="brown" />
        <character name="shape" value="ovate" />
        <character name="shape" value="oblong" modifier="broadly" />
        <character char_type="range_value" name="size" from="3.3" from_unit="mm" to="3.5" to_unit="mm" />
      </structure>
      <structure id="o8242" name="margin">
        <character name="coloration" value="hyaline" modifier="white" />
        <character name="coloration" value="green" />
        <character name="architecture_or_feature" value="1-veined" />
      </structure>
      <structure id="o8243" name="costa" />
      <structure id="o8244" name="mucro" />
      <relation id="r1515" name="forming" from="o8242" to="o8244" negation="false" />
      <structure id="o8245" name="apex">
        <character name="shape" value="acute" />
      </structure>
    </statement>
    <statement id="1268.txt-8">
      <text>Utricles stramineous, longer than glume, oblong-lanceolate, trigonous, 3.5–4 mm, hirsute, veins slightly conspicuous, contracted at both ends, base shortly stipitate, apical beak 1–1.3 mm, hispidulous on margins, orifice obliquely truncate, shallowly 2-lobed.</text>
      <structure id="o8246" name="utricle">
        <character name="coloration" value="stramineous" />
        <character name="length" value="longer" constraint="than glume" constraintid="o8247" />
        <character name="shape" value="oblong-lanceolate" />
        <character name="shape" value="trigonous" />
        <character char_type="range_value" name="size" from="3.5" from_unit="mm" to="4" to_unit="mm" />
        <character name="external_texture" value="hirsute" />
      </structure>
      <structure id="o8247" name="glume" />
      <structure id="o8248" name="vein">
        <character name="prominence" value="conspicuous" modifier="slightly" />
        <character name="size" value="contracted" constraint="at ends" constraintid="o8249" />
      </structure>
      <structure id="o8249" name="end" />
      <structure id="o8250" name="base">
        <character name="architecture" value="stipitate" modifier="shortly" />
      </structure>
      <structure id="o8251" name="beak" constraint="apical">
        <character char_type="range_value" name="size" from="1" from_unit="mm" to="1.3" to_unit="mm" />
