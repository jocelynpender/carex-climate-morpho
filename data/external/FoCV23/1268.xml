  <description>
    <statement id="1268.txt-0">
      <text>Rhizome oblique, woody.</text>
      <structure id="o8226" name="rhizome">
        <character name="orientation_or_shape" value="oblique" />
        <character name="internal_texture" value="woody" />
      </structure>
    </statement>
    <statement id="1268.txt-1">
      <text>Culms tufted, 25–28 cm tall, obtusely trigonous, smooth, basal sheaths purple-red, disintegrating into fibers.</text>
      <structure id="o8227" name="culm">
        <character name="external_texture_or_habit" value="tufted" />
        <character char_type="range_value" name="size" from="25" from_unit="cm" to="28" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="shape" value="trigonous" modifier="obtusely" />
        <character name="architecture_or_external_texture_or_relief" value="smooth" />
      </structure>
      <structure id="o8228" name="sheath" constraint="basal">
        <character name="coloration" value="purple-red" />
        <character name="dehiscence" value="disintegrating" constraint="into fibers" constraintid="o8229" />
      </structure>
      <structure id="o8229" name="fiber" />
    </statement>
    <statement id="1268.txt-2">
      <text>Leaves longer than culm, blades greenish, linear, ca. 2.5 mm wide, margins scabrous, apex long acuminate.</text>
      <structure id="o8230" name="leaf">
        <character name="length" value="longer" constraint="than culm" constraintid="o8231" />
      </structure>
      <structure id="o8231" name="culm" />
      <structure id="o8232" name="blade">
        <character name="coloration" value="greenish" />
        <character name="arrangement_or_shape" value="linear" />
        <character name="width" value="2.5" unit="mm" />
      </structure>
      <structure id="o8233" name="margin">
        <character name="external_texture_or_relief" value="scabrous" />
      </structure>
      <structure id="o8234" name="apex">
        <character name="shape" value="acuminate" />
      </structure>
    </statement>
    <statement id="1268.txt-3">
      <text>Involucral bracts reduced to shortly bladed, shorter than inflorescence, sheathing 1.5–2 cm.</text>
      <structure id="o8235" name="bract" constraint="involucral">
        <character name="size" value="reduced" />
        <character name="height_or_length" value="shorter" constraint="than inflorescence" constraintid="o8236" />
        <character name="architecture_or_feature_or_shape" value="sheathing" />
        <character char_type="range_value" name="size" from="1.5" from_unit="cm" to="2" to_unit="cm" />
      </structure>
      <structure id="o8236" name="inflorescence" />
