        <character name="shape" value="2-lobed" modifier="shallowly" />
      </structure>
    </statement>
    <statement id="1268.txt-9">
      <text>Nutlets tightly enveloped, elliptic, trigonous, 2–2.2 mm;</text>
      <structure id="o8254" name="nutlet">
        <character name="feature" value="enveloped" modifier="tightly" />
        <character name="arrangement_or_shape" value="elliptic" />
        <character name="shape" value="trigonous" />
        <character char_type="range_value" name="size" from="2" from_unit="mm" to="2.2" to_unit="mm" />
      </structure>
    </statement>
    <statement id="1268.txt-10">
      <text>style erect, base conspicuously thickened;</text>
      <structure id="o8255" name="style">
