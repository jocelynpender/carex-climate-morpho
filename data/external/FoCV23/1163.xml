      <structure id="o5156" name="apex">
        <character name="architecture" value="beakless" />
      </structure>
    </statement>
    <statement id="1163.txt-10">
      <text>style base thickened, conic;</text>
      <structure id="o5157" name="base" constraint="style">
        <character name="width" value="thickened" />
        <character name="shape" value="conic" />
      </structure>
    </statement>
    <statement id="1163.txt-11">
      <text>stigmas 3.</text>
    </statement>
