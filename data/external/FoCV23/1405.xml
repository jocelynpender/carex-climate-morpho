      </structure>
      <relation id="r2329" name="may_be_the_same" from="o12368" to="o12369" negation="false" />
      <relation id="r2330" name="may_be_the_same" from="o12368" to="o12371" negation="false" />
      <relation id="r2331" name="may_be_the_same" from="o12369" to="o12371" negation="false" />
    </statement>
    <statement id="1405.txt-7">
      <text>Nutlets pale castaneous, tightly enveloped, trigonous ovate, ca. 4 mm, constricted on angles at middle, faces concave at base, base very shortly stipitate, apex abruptly contracted into a cylindric beak ca. 1.5 mm, orifice inflated;</text>
      <structure id="o12373" name="nutlet">
        <character name="coloration" value="castaneous" modifier="pale" />
        <character name="feature" value="enveloped" modifier="tightly" />
        <character name="shape" value="trigonous ovate" />
        <character name="size" value="4" unit="mm" />
        <character name="size" value="constricted" constraint="on angles" constraintid="o12374" />
        <character name="shape" value="concave" constraint="at base" constraintid="o12375" />
      </structure>
      <structure id="o12374" name="angle" constraint="at middle" />
      <structure id="o12375" name="base" />
      <structure id="o12376" name="base">
        <character name="architecture" value="stipitate" modifier="very shortly" />
      </structure>
      <structure id="o12377" name="apex">
        <character name="size" value="contracted" modifier="abruptly" constraint="into beak" constraintid="o12378" />
      </structure>
      <structure id="o12378" name="beak">
        <character name="shape" value="cylindric" />
        <character name="size" value="1.5" unit="mm" />
      </structure>
      <structure id="o12379" name="orifice">
        <character name="shape" value="inflated" />
      </structure>
      <relation id="r2332" name="may_be_the_same" from="o12375" to="o12376" negation="false" />
    </statement>
    <statement id="1405.txt-8">
      <text>style persistent, base not thickened;</text>
      <structure id="o12380" name="style">
        <character name="duration" value="persistent" />
      </structure>
