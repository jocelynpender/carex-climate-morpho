      <structure id="o20478" name="margin">
        <character name="external_texture_or_relief" value="scabrous" />
      </structure>
    </statement>
    <statement id="1698.txt-3">
      <text>Involucral bracts glumelike, proximal ones sometimes setaceous.</text>
      <structure id="o20479" name="bract" constraint="involucral">
        <character name="feature" value="glumelike" />
        <character name="position" value="proximal" />
        <character name="feature_or_shape" value="setaceous" modifier="sometimes" />
