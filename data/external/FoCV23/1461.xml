      </structure>
      <structure id="o14114" name="tooth">
        <character name="orientation" value="recurved" modifier="slightly" />
      </structure>
    </statement>
    <statement id="1461.txt-9">
      <text>Nutlets usually immature;</text>
      <structure id="o14115" name="nutlet">
        <character name="life_stage" value="immature" modifier="usually" />
      </structure>
    </statement>
