      </structure>
      <relation id="r3533" name="covered with" from="o18491" to="o18492" negation="false" />
    </statement>
    <statement id="1610.txt-1">
      <text>Culms loosely tufted, 10–25 cm tall, slender, soft, slightly scabrid on angles, leafy in lower 1/4.</text>
      <structure id="o18493" name="culm">
        <character name="external_texture_or_habit" value="tufted" modifier="loosely" />
