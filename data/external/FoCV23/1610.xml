        <character name="external_texture" value="soft" />
        <character name="relief" value="scabrid" modifier="slightly" constraint="on angles" constraintid="o18494" />
        <character name="architecture" value="leafy" modifier="in lower 1/4" />
      </structure>
      <structure id="o18494" name="angle" />
    </statement>
    <statement id="1610.txt-2">
      <text>Cauline leaves shorter than culm;</text>
      <structure id="o18495" name="leaf" constraint="cauline">
        <character name="height_or_length" value="shorter" constraint="than culm" constraintid="o18496" />
      </structure>
      <structure id="o18496" name="culm" />
    </statement>
    <statement id="1610.txt-3">
      <text>blades hairlike, ca. 0.3 mm wide, flat, smooth, soft;</text>
      <structure id="o18497" name="blade">
        <character name="shape" value="hairlike" />
        <character name="width" value="0.3" unit="mm" />
        <character name="prominence_or_shape" value="flat" />
        <character name="architecture_or_external_texture_or_relief" value="smooth" />
        <character name="external_texture" value="soft" />
      </structure>
    </statement>
    <statement id="1610.txt-4">
      <text>basal sheath pale brown.</text>
      <structure id="o18498" name="sheath" constraint="basal">
        <character name="coloration" value="brown" modifier="pale" />
      </structure>
    </statement>
    <statement id="1610.txt-5">
      <text>Inflorescence 1-spiked, terminal, ovate, 6–8 mm;</text>
    </statement>
    <statement id="1610.txt-6">
      <text>male part very short, 1–3-flowered;</text>
    </statement>
    <statement id="1610.txt-7">
      <text>female part 4–6-flowered, longer than male part.</text>
      <structure id="o18499" name="inflorescence">
        <character name="position" value="terminal" />
        <character name="shape" value="ovate" />
        <character char_type="range_value" name="size" from="6" from_unit="mm" to="8" to_unit="mm" />
        <character name="feature_or_reproduction" value="male" />
        <character name="height_or_length" value="short" modifier="very" />
        <character name="architecture" value="1-3-flowered" />
        <character name="reproduction" value="female" />
        <character name="architecture" value="4-6-flowered" />
        <character name="length" value="longer than male part" />
      </structure>
    </statement>
    <statement id="1610.txt-8">
      <text>Female glumes red-brown to dark brown laterally, green at middle, elliptic-ovate, ca. 2.5 mm, 1-veined, apex acute.</text>
      <structure id="o18500" name="glume" constraint="female">
        <character char_type="range_value" name="coloration" from="red-brown" to="dark brown" />
