        <character name="shape" value="spinulose-serrate" />
      </structure>
      <structure id="o19224" name="apex">
        <character name="shape" value="attenuate" modifier="gradually" constraint="into beak" constraintid="o19225" />
      </structure>
      <structure id="o19225" name="beak">
        <character name="length" value="long" />
      </structure>
      <structure id="o19226" name="beak">
        <character name="shape" value="cleft" modifier="deeply;abaxially" />
        <character name="shape" value="cuneate-cleft" modifier="adaxially" />
        <character name="shape" value="2-toothed" constraint="at orifice" constraintid="o19227" />
      </structure>
      <structure id="o19227" name="orifice" />
