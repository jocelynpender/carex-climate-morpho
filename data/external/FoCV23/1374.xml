      <structure id="o11585" name="peduncle">
        <character char_type="range_value" name="size" from="3" from_unit="cm" to="5" to_unit="cm" />
      </structure>
    </statement>
    <statement id="1374.txt-8">
      <text>Female glumes brown, elliptic-lanceolate, 8–10 mm, 3-veined costa excurrent into a scabrous awn ca. 5 mm, apex truncate or emarginate.</text>
