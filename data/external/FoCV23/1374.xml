        <character name="shape" value="cylindric" />
        <character char_type="range_value" name="length" from="4" from_unit="cm" to="5" to_unit="cm" />
        <character name="width" value="0.8" unit="cm" />
        <character name="architecture" value="flowered" modifier="densely" />
      </structure>
      <structure id="o11583" name="flower" constraint="male">
        <character name="count" value="few" />
      </structure>
      <relation id="r2159" name="with" from="o11582" to="o11583" negation="false" />
      <structure id="o11584" name="apex" />
      <relation id="r2160" name="at" from="o11583" to="o11584" negation="false" />
    </statement>
    <statement id="1374.txt-7">
