      </structure>
      <structure id="o11579" name="spike" constraint="subtending" />
    </statement>
    <statement id="1374.txt-4">
      <text>Spikes 2 or 3;</text>
      <structure id="o11580" name="spike">
        <character name="count" value="2" />
        <character name="count" value="3" />
      </structure>
    </statement>
    <statement id="1374.txt-5">
      <text>terminal spike male, cylindric, ca. 40 × 7 mm;</text>
      <structure id="o11581" name="spike" constraint="terminal">
        <character name="feature_or_reproduction" value="male" />
        <character name="shape" value="cylindric" />
        <character name="length" value="40" unit="mm" />
        <character name="width" value="7" unit="mm" />
      </structure>
    </statement>
    <statement id="1374.txt-6">
      <text>lateral spikes female, with few male flowers at apex, distant, cylindric, 4–5 × ca. 0.8 cm, densely flowered;</text>
      <structure id="o11582" name="spike" constraint="lateral">
        <character name="reproduction" value="female" />
