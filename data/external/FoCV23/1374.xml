        <character name="external_texture_or_relief" value="scabrous" />
        <character name="size" value="5" unit="mm" />
      </structure>
      <structure id="o11589" name="apex">
        <character name="shape" value="truncate" />
        <character name="shape" value="emarginate" />
      </structure>
    </statement>
    <statement id="1374.txt-9">
      <text>Utricles obovate-elliptic, trigonous, 5–6 mm, glabrous, faces concave at base, apex abruptly contracted into a scabrous beak ca. 3 mm, orifice deeply 2-lobed.</text>
      <structure id="o11590" name="utricle">
        <character name="arrangement_or_shape" value="obovate-elliptic" />
        <character name="shape" value="trigonous" />
        <character char_type="range_value" name="size" from="5" from_unit="mm" to="6" to_unit="mm" />
        <character name="external_texture" value="glabrous" />
        <character name="shape" value="concave" constraint="at base" constraintid="o11591" />
      </structure>
      <structure id="o11591" name="base" />
      <structure id="o11592" name="apex">
