  <description>
    <statement id="1374.txt-0">
      <text>Rhizome thick, obliquely ascending.</text>
      <structure id="o11570" name="rhizome">
        <character name="width" value="thick" />
        <character name="orientation" value="ascending" modifier="obliquely" />
      </structure>
    </statement>
    <statement id="1374.txt-1">
      <text>Culms 30–40 cm tall, slender, trigonous, clothed at base with dark brown sheaths disintegrating into parallel fibers.</text>
      <structure id="o11571" name="culm">
        <character char_type="range_value" name="size" from="30" from_unit="cm" to="40" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="size" value="slender" />
        <character name="shape" value="trigonous" />
      </structure>
      <structure id="o11572" name="base" />
      <relation id="r2157" name="clothed at" from="o11571" to="o11572" negation="false" />
      <structure id="o11573" name="sheath">
        <character name="coloration" value="brown" modifier="dark" />
        <character name="dehiscence" value="disintegrating" constraint="into fibers" constraintid="o11574" />
      </structure>
      <relation id="r2158" name="with" from="o11572" to="o11573" negation="false" />
      <structure id="o11574" name="fiber">
        <character name="arrangement" value="parallel" />
      </structure>
    </statement>
    <statement id="1374.txt-2">
      <text>Leaves longer than culm, blades linear, 3–5 mm wide, leathery.</text>
      <structure id="o11575" name="leaf">
        <character name="length" value="longer" constraint="than culm" constraintid="o11576" />
      </structure>
      <structure id="o11576" name="culm" />
      <structure id="o11577" name="blade">
        <character name="arrangement_or_shape" value="linear" />
        <character char_type="range_value" name="width" from="3" from_unit="mm" to="5" to_unit="mm" />
        <character name="external_texture_or_feature" value="leathery" />
      </structure>
    </statement>
    <statement id="1374.txt-3">
      <text>Involucral bracts shortly bladed, shorter than subtending spike, sheathing 2.5–4 cm.</text>
      <structure id="o11578" name="bract" constraint="involucral">
        <character name="height_or_length" value="shorter" constraint="than subtending spike" constraintid="o11579" />
        <character name="architecture_or_feature_or_shape" value="sheathing" />
