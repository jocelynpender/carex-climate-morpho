    <statement id="1135.txt-5">
      <text>Spikes 2;</text>
      <structure id="o4268" name="spike">
        <character name="count" value="2" />
      </structure>
    </statement>
    <statement id="1135.txt-6">
      <text>terminal spike male, pale yellow, ovate-clavate, 7–10 mm, with peduncle 5–12 mm;</text>
      <structure id="o4269" name="spike" constraint="terminal">
        <character name="feature_or_reproduction" value="male" />
