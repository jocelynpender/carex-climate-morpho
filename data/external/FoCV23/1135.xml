        <character name="shape" value="trigonous" />
      </structure>
    </statement>
    <statement id="1135.txt-2">
      <text>Basal sheath dark brown, split into fibers.</text>
      <structure id="o4259" name="sheath" constraint="basal">
        <character name="coloration" value="brown" modifier="dark" />
      </structure>
      <structure id="o4260" name="fiber" />
      <relation id="r881" name="split into" from="o4259" to="o4260" negation="false" />
    </statement>
    <statement id="1135.txt-3">
      <text>Leaves longer than culm, blades 1–2 mm wide, leathery, margins scabrid, revolute, apex acuminate.</text>
      <structure id="o4261" name="leaf">
