        <character name="architecture" value="bristlelike" />
        <character name="length" value="longer" constraint="than inflorescence" constraintid="o4267" />
        <character char_type="range_value" name="size" from="2.5" from_unit="cm" to="3" to_unit="cm" />
        <character name="architecture_or_feature" value="sheathed" modifier="shortly" />
