        <character name="architecture" value="flowered" modifier="densely" />
        <character name="architecture" value="subsessile" />
      </structure>
    </statement>
    <statement id="1135.txt-8">
      <text>Female glumes pale yellow, oblong, ca. 2 × 1 mm, midvein distinct, apex obtuse.</text>
