        <character name="relief" value="scabrid" />
        <character name="shape_or_vernation" value="revolute" />
      </structure>
      <structure id="o4265" name="apex">
        <character name="shape" value="acuminate" />
      </structure>
    </statement>
    <statement id="1135.txt-4">
      <text>Involucral bract bristlelike, longer than inflorescence, 2.5–3 cm, shortly sheathed ca. 1.5 mm.</text>
