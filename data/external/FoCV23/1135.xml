      <relation id="r882" name="with" from="o4269" to="o4270" negation="false" />
    </statement>
    <statement id="1135.txt-7">
      <text>lateral spike androgynous, oblong-ovate or suborbicular, 7–12 × 5–8 mm, densely flowered, subsessile.</text>
      <structure id="o4271" name="spike" constraint="lateral">
        <character name="feature_or_reproduction" value="androgynous" />
        <character name="shape" value="oblong-ovate" />
        <character name="shape" value="suborbicular" />
