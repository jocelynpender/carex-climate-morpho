  <description>
    <statement id="1408.txt-0">
      <text>Rhizome short, woody.</text>
      <structure id="o12451" name="rhizome">
        <character name="height_or_length" value="short" />
        <character name="internal_texture" value="woody" />
      </structure>
    </statement>
    <statement id="1408.txt-1">
      <text>Culms lateral, 55–65 cm tall, trigonous, glabrous, clothed at base with bladeless sheaths.</text>
      <structure id="o12452" name="culm">
        <character name="position" value="lateral" />
        <character char_type="range_value" name="size" from="55" from_unit="cm" to="65" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="shape" value="trigonous" />
        <character name="external_texture" value="glabrous" />
      </structure>
      <structure id="o12453" name="base" />
      <relation id="r2347" name="clothed at" from="o12452" to="o12453" negation="false" />
      <structure id="o12454" name="sheath" constraint="bladeless" />
      <relation id="r2348" name="with" from="o12453" to="o12454" negation="false" />
    </statement>
    <statement id="1408.txt-2">
      <text>Leaves longer or shorter than culm, blades broadly linear, 7–8 mm wide, flat, leathery, apex acuminate.</text>
      <structure id="o12455" name="leaf" />
      <structure id="o12456" name="culm" />
      <structure id="o12457" name="blade">
        <character name="arrangement_or_shape" value="linear" modifier="broadly" />
        <character char_type="range_value" name="width" from="7" from_unit="mm" to="8" to_unit="mm" />
        <character name="prominence_or_shape" value="flat" />
        <character name="external_texture_or_feature" value="leathery" />
      </structure>
      <structure id="o12458" name="apex">
        <character name="shape" value="acuminate" />
      </structure>
    </statement>
    <statement id="1408.txt-3">
      <text>Involucral bracts shortly bladed, shorter than inflorescence, long sheathing.</text>
      <structure id="o12459" name="bract" constraint="involucral">
        <character name="height_or_length" value="shorter" constraint="than inflorescence" constraintid="o12460" />
        <character name="architecture_or_feature_or_shape" value="sheathing" />
      </structure>
      <structure id="o12460" name="inflorescence" />
    </statement>
    <statement id="1408.txt-4">
      <text>Spikes 3, distant;</text>
      <structure id="o12461" name="spike">
        <character name="count" value="3" />
        <character name="arrangement" value="distant" />
      </structure>
    </statement>
    <statement id="1408.txt-5">
      <text>terminal spike male (occasionally with 1 to few female flowers at base ), linear, 4–5 cm, with peduncles 5.5–6.5 cm, scabrous;</text>
      <structure id="o12462" name="spike" constraint="terminal">
        <character name="feature_or_reproduction" value="male" constraint="with 1 to flowers" constraintid="o12463" modifier="occasionally" />
        <character name="arrangement_or_shape" value="linear" />
        <character char_type="range_value" name="size" from="4" from_unit="cm" to="5" to_unit="cm" />
        <character name="external_texture_or_relief" value="scabrous" />
      </structure>
      <structure id="o12463" name="flower" in_brackets="true" constraint="female">
        <character name="count" value="few" in_brackets="true" />
      </structure>
      <structure id="o12464" name="base" in_brackets="true" />
