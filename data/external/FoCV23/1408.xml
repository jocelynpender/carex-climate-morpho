        <character name="architecture" value="stipitate" />
        <character name="size" value="contracted" modifier="abruptly" constraint="into beak" constraintid="o12481" />
      </structure>
      <relation id="r2355" name="base" from="o12478" to="o12480" negation="false" />
      <structure id="o12481" name="beak">
        <character name="shape" value="cylindric" />
