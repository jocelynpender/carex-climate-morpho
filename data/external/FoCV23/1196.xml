  <number>158.</number>
  <TaxonIdentification Status="ACCEPTED">
    <genus_name>Carex</genus_name>
    <species_name>crebra</species_name>
    <species_authority>V. I. Kreczetowicz</species_authority>
    <place_of_publication>
      <publication_title>Bot. Mater. Gerb. Bot. Inst. Komarova Akad. Nauk S.S.S.R.</publication_title>
