    <species_authority>C. B. Clarke</species_authority>
    <place_of_publication>
      <publication_title>Bull. Misc. Inform. Kew, Addit. Ser.</publication_title>
      <place_in_publication>8: 67. 1908</place_in_publication>
    </place_of_publication>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <species_name>康藏嵩草 kang zang song cao</species_name>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <genus_name>Kobresia</genus_name>
    <species_name>tibetica</species_name>
    <species_authority>Maximowicz</species_authority>
    <subspecies_name>littledalei</subspecies_name>
    <subspecies_authority>(C. B. Clarke) P. C. Li.</subspecies_authority>
