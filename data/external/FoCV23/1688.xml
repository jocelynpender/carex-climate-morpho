        <character name="external_texture" value="soft" />
      </structure>
      <structure id="o20240" name="margin">
        <character name="shape" value="serrulate" />
      </structure>
    </statement>
    <statement id="1688.txt-3">
      <text>Involucral bracts setaceous at base, other bracts glumelike.</text>
      <structure id="o20241" name="bract" constraint="involucral">
