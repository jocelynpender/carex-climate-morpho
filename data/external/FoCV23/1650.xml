      <structure id="o19385" name="base" />
      <relation id="r3673" name="at" from="o19384" to="o19385" negation="false" />
    </statement>
    <statement id="1650.txt-3">
      <text>Leaves equaling or longer than culm, blades linear, 2.5–5 mm wide, flat, scabrous.</text>
