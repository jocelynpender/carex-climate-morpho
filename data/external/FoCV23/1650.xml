        <character name="external_texture_or_relief" value="scabrous" />
      </structure>
    </statement>
    <statement id="1650.txt-4">
      <text>Involucral bracts glumelike.</text>
      <structure id="o19389" name="bract" constraint="involucral">
        <character name="feature" value="glumelike" />
      </structure>
    </statement>
    <statement id="1650.txt-5">
