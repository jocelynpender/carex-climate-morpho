        <character name="arrangement" value="spaced" modifier="much" />
        <character name="shape" value="cylindric" />
        <character char_type="range_value" name="length" from="25" from_unit="mm" to="36" to_unit="mm" />
        <character char_type="range_value" name="width" from="6" from_unit="mm" to="7" to_unit="mm" />
        <character name="architecture" value="flowered" modifier="densely" />
        <character name="position" value="upper" />
        <character name="count" value="3" />
        <character name="count" value="4" />
        <character name="feature_or_reproduction" value="male" constraint="with flowers" constraintid="o1575" modifier="rarely" />
