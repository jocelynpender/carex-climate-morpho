      <structure id="o1578" name="flower" constraint="male">
        <character name="count" value="few" />
      </structure>
      <relation id="r352" name="with" from="o1577" to="o1578" negation="false" modifier="usually" />
      <structure id="o1579" name="apex" />
      <relation id="r353" name="at" from="o1578" to="o1579" negation="false" />
      <relation id="r354" name="may_be_the_same" from="o1573" to="o1574" negation="false" />
    </statement>
    <statement id="1045.txt-5">
      <text>peduncles slender, with dense papillae, upper ones rather short.</text>
      <structure id="o1580" name="peduncle">
        <character name="size" value="slender" />
