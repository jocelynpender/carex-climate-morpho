      </structure>
      <relation id="r3323" name="with" from="o17297" to="o17300" negation="false" />
    </statement>
    <statement id="1567.txt-6">
      <text>Female glumes pale brown or ferruginous, elliptic-lanceolate or lanceolate, 3–3.8 mm, green 3-veined costa excurrent into a mucro.</text>
      <structure id="o17301" name="glume" constraint="female">
        <character name="coloration" value="brown" modifier="pale" />
        <character name="coloration" value="brown" modifier="pale;ferruginous" />
        <character name="shape" value="elliptic-lanceolate" />
        <character name="shape" value="lanceolate" />
        <character char_type="range_value" name="size" from="3" from_unit="mm" to="3.8" to_unit="mm" />
        <character name="coloration" value="green" />
        <character name="architecture_or_feature" value="3-veined" />
        <character name="architecture" value="excurrent" constraint="into mucro" constraintid="o17303" />
