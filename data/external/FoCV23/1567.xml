        <character name="shape" value="cylindric" />
        <character name="shape" value="clavate" />
        <character char_type="range_value" name="size" from="1.5" from_unit="cm" to="5.5" to_unit="cm" />
        <character name="orientation" value="pendent" />
      </structure>
      <structure id="o17298" name="flower" constraint="several female" />
