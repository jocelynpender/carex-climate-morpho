    <statement id="1567.txt-0">
      <text>Rhizome creeping.</text>
      <structure id="o17286" name="rhizome">
        <character name="habit" value="creeping" />
      </structure>
    </statement>
    <statement id="1567.txt-1">
      <text>Culms tufted, stout, trigonous, 60–100 cm tall.</text>
      <structure id="o17287" name="culm">
