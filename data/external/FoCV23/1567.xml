        <character name="prominence_or_shape" value="flat" />
      </structure>
      <structure id="o17291" name="margin">
        <character name="shape_or_vernation" value="revolute" modifier="slightly" />
      </structure>
    </statement>
    <statement id="1567.txt-3">
      <text>Involucral bracts leaflike, lower 3 surpassing inflorescence.</text>
      <structure id="o17292" name="bract" constraint="involucral">
