        <character name="height" value="tall" />
      </structure>
    </statement>
    <statement id="1567.txt-2">
      <text>Leaves shorter than culm, blades widely linear, 6–10 mm wide, flat, margins slightly revolute.</text>
      <structure id="o17288" name="leaf">
        <character name="height_or_length" value="shorter" constraint="than culm" constraintid="o17289" />
      </structure>
      <structure id="o17289" name="culm" />
