      <text>Fr. Jun.</text>
      <structure id="o13244" name="beak">
        <character name="external_texture_or_relief" value="scabrous" constraint="on margins" constraintid="o13245" />
        <character name="shape" value="2-lobed" modifier="deeply" constraint="at orifice" constraintid="o13246" />
      </structure>
    </statement>
  </description>
  <distribution>Meadows in valleys; 2000–3500 m. Xinjiang [C Asia].</distribution>
