        <character name="shape" value="2-toothed" />
      </structure>
    </statement>
    <statement id="1454.txt-9">
      <text>Nutlets yellowish brown, rather loosely enveloped, obovate, trigonous, ca. 4 mm, base shortly stipitate;</text>
      <structure id="o13900" name="nutlet">
        <character name="coloration" value="brown" modifier="yellowish" />
        <character name="feature" value="enveloped" modifier="rather loosely" />
