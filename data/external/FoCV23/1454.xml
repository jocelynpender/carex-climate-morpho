      <structure id="o13904" name="stigma">
        <character name="count" value="3" />
        <character name="height_or_length" value="short" modifier="rather" />
      </structure>
    </statement>
  </description>
  <distribution>Riversides, lakesides, wet places. Fujian, Jiangsu, Zhejiang [Japan].</distribution>
