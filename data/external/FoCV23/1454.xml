        <character name="external_texture" value="stiff" modifier="slightly" />
      </structure>
      <structure id="o13903" name="part" constraint="lower" />
    </statement>
    <statement id="1454.txt-11">
      <text>stigmas 3, rather short.</text>
    </statement>
