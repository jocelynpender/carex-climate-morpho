        <character name="architecture" value="pedunculate" />
      </structure>
    </statement>
    <statement id="1454.txt-6">
      <text>remaining spikes female, oblong, 2–3 × 1–1.2 cm, subdensely many flowered, sessile at upper spikes, shortly pedunculate at lowermost spike.</text>
