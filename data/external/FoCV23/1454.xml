        <character name="architecture_or_feature" value="3-veined" />
      </structure>
      <structure id="o13893" name="apex">
        <character name="shape" value="acuminate" />
        <character name="shape" value="mucronate" />
      </structure>
    </statement>
    <statement id="1454.txt-8">
      <text>Utricles yellow-brown, obliquely patent, much longer than glume, oblong-ovate, inflated trigonous, 9–12 mm, thinly leathery, glabrous, several veined, base rounded, shortly stipitate, apex gradually narrowed to a long beak, orifice slightly long 2-toothed.</text>
      <structure id="o13894" name="utricle">
        <character name="coloration" value="yellow-brown" />
        <character name="orientation" value="patent" modifier="obliquely" />
        <character name="length" value="longer" modifier="much" constraint="than glume" constraintid="o13895" />
        <character name="shape" value="oblong-ovate" />
        <character name="shape" value="inflated trigonous" />
        <character char_type="range_value" name="size" from="9" from_unit="mm" to="12" to_unit="mm" />
        <character name="external_texture_or_feature" value="leathery" modifier="thinly" />
        <character name="external_texture" value="glabrous" />
        <character name="count" value="several" />
        <character name="architecture_or_feature" value="veined" />
      </structure>
      <structure id="o13895" name="glume" />
      <structure id="o13896" name="base">
        <character name="shape" value="rounded" />
        <character name="architecture" value="stipitate" modifier="shortly" />
      </structure>
      <structure id="o13897" name="apex" />
      <structure id="o13898" name="beak">
        <character name="length" value="long" />
      </structure>
      <relation id="r2637" name="narrowed to" from="o13897" to="o13898" negation="false" modifier="gradually" />
