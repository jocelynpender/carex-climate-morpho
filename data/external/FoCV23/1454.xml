        <character name="architecture" value="stipitate" modifier="shortly" />
      </structure>
    </statement>
    <statement id="1454.txt-10">
      <text>style thickened at lower part, persistent, slightly stiff;</text>
      <structure id="o13902" name="style">
        <character name="width" value="thickened" constraint="at part" constraintid="o13903" />
