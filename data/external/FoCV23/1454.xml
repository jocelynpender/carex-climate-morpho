        <character char_type="range_value" name="length" from="2" from_unit="cm" to="3" to_unit="cm" />
        <character char_type="range_value" name="width" from="1" from_unit="cm" to="1.2" to_unit="cm" />
        <character name="count" value="many" modifier="subdensely" />
        <character name="architecture" value="flowered" />
        <character name="architecture_or_feature" value="sessile" constraint="at spikes" constraintid="o13890" />
        <character name="architecture" value="pedunculate" modifier="shortly" constraint="at spike" constraintid="o13891" />
      </structure>
      <structure id="o13890" name="spike" constraint="upper" />
      <structure id="o13891" name="spike">
        <character name="feature" value="lowermost" />
      </structure>
    </statement>
    <statement id="1454.txt-7">
      <text>Female glumes dark yellow, lanceolate-ovate, 5–6 mm, membranous, green 3-veined, apex acuminate, mucronate.</text>
      <structure id="o13892" name="glume" constraint="female">
        <character name="coloration" value="yellow" modifier="dark" />
        <character name="shape" value="lanceolate-ovate" />
        <character char_type="range_value" name="size" from="5" from_unit="mm" to="6" to_unit="mm" />
        <character name="feature" value="membranous" />
