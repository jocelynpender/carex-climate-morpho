      </structure>
      <relation id="r3063" name="with" from="o16056" to="o16057" negation="false" />
    </statement>
    <statement id="1522.txt-1">
      <text>Culms 10–25 cm tall, slender, acutely trigonous, clothed at base with sheaths disintegrating into fibers.</text>
      <structure id="o16058" name="culm">
        <character char_type="range_value" name="size" from="10" from_unit="cm" to="25" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="size" value="slender" />
        <character name="shape" value="trigonous" modifier="acutely" />
      </structure>
      <structure id="o16059" name="base" />
      <relation id="r3064" name="clothed at" from="o16058" to="o16059" negation="false" />
      <structure id="o16060" name="sheath">
        <character name="dehiscence" value="disintegrating" constraint="into fibers" constraintid="o16061" />
      </structure>
      <relation id="r3065" name="with" from="o16059" to="o16060" negation="false" />
      <structure id="o16061" name="fiber" />
    </statement>
    <statement id="1522.txt-2">
      <text>Leaves subequaling culm, blades linear, 1.5–3 mm wide, flat, margins revolute.</text>
      <structure id="o16062" name="leaf" />
      <structure id="o16063" name="culm" />
      <relation id="r3066" name="subequaling" from="o16062" to="o16063" negation="false" />
      <structure id="o16064" name="blade">
        <character name="arrangement_or_shape" value="linear" />
        <character char_type="range_value" name="width" from="1.5" from_unit="mm" to="3" to_unit="mm" />
        <character name="prominence_or_shape" value="flat" />
      </structure>
      <structure id="o16065" name="margin">
        <character name="shape_or_vernation" value="revolute" />
      </structure>
    </statement>
    <statement id="1522.txt-3">
      <text>Involucral bracts sheathless, lowest involucral bract leaflike, longer or shorter than inflorescence.</text>
      <structure id="o16066" name="bract" constraint="involucral">
        <character name="feature" value="sheathless" />
      </structure>
      <structure id="o16067" name="bract" constraint="involucral">
