    <statement id="1522.txt-0">
      <text>Rhizome with long stolons, slender, woody.</text>
      <structure id="o16056" name="rhizome">
        <character name="size" value="slender" />
        <character name="internal_texture" value="woody" />
      </structure>
      <structure id="o16057" name="stolon">
