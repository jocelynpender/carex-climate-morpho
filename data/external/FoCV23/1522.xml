        <character name="size" value="slender" />
        <character char_type="range_value" name="size" from="7" from_unit="mm" to="20" to_unit="mm" />
      </structure>
    </statement>
    <statement id="1522.txt-7">
      <text>Female glumes dark brown, oblong or oblong-lanceolate, ca. 2 mm, green 3-veined at middle, margins narrowly white hyaline, apex obtuse.</text>
      <structure id="o16075" name="glume" constraint="female">
        <character name="coloration" value="brown" modifier="dark" />
        <character name="shape" value="oblong" />
        <character name="shape" value="oblong-lanceolate" />
        <character name="size" value="2" unit="mm" />
        <character name="coloration" value="green" />
        <character name="architecture_or_feature" value="3-veined" modifier="at middle" />
      </structure>
      <structure id="o16076" name="margin">
        <character name="coloration" value="hyaline" modifier="narrowly;white" />
      </structure>
      <structure id="o16077" name="apex">
        <character name="shape" value="obtuse" />
      </structure>
    </statement>
    <statement id="1522.txt-8">
      <text>Utricles gray-green above, brownish below, minutely ferruginous papillose, longer and broader than glume, broadly ovate, planoconvex, ca. 2.5 mm, base rounded, shortly stipitate, apex abruptly contracted into a hispidulous short beak, orifice entire.</text>
      <structure id="o16078" name="utricle">
        <character name="coloration" value="gray-green" />
        <character name="coloration" value="brownish" />
        <character name="relief" value="papillose" />
        <character name="length" value="longer" />
