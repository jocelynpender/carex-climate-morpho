        <character name="position" value="upper" />
        <character name="reproduction" value="female" />
        <character name="feature_or_reproduction" value="male" modifier="in middle" />
        <character name="shape" value="oblong-ovate" />
        <character char_type="range_value" name="atypical_size" from="5" to="6" from_unit="mm" to_unit="mm" from_inclusive="false" />
        <character char_type="range_value" name="size" from="4" from_unit="mm" to="5" to_unit="mm" />
      </structure>
      <structure id="o19417" name="spike" constraint="lower" />
