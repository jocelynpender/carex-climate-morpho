      <relation id="r3678" name="may_be_the_same" from="o19416" to="o19418" negation="false" />
    </statement>
    <statement id="1651.txt-7">
      <text>Female glumes yellowish brown, oblong-ovate, margins pale hyaline, apex acute.</text>
      <structure id="o19419" name="glume" constraint="female">
        <character name="coloration" value="brown" modifier="yellowish" />
        <character name="shape" value="oblong-ovate" />
      </structure>
