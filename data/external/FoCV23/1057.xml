    </statement>
    <statement id="1057.txt-1">
      <text>Culms tufted, 50–60 cm tall, 2.5–3.5 mm wide, trigonous, clothed at base with dark purple sheaths sometimes disintegrating into fibers.</text>
      <structure id="o1920" name="culm">
        <character name="external_texture_or_habit" value="tufted" />
        <character char_type="range_value" name="size" from="50" from_unit="cm" to="60" to_unit="cm" />
        <character name="height" value="tall" />
        <character char_type="range_value" name="width" from="2.5" from_unit="mm" to="3.5" to_unit="mm" />
        <character name="shape" value="trigonous" />
      </structure>
      <structure id="o1921" name="base" />
      <relation id="r413" name="clothed at" from="o1920" to="o1921" negation="false" />
      <structure id="o1922" name="sheath">
        <character name="coloration" value="purple" modifier="dark" />
        <character name="dehiscence" value="disintegrating" modifier="sometimes" constraint="into fibers" constraintid="o1923" />
      </structure>
      <relation id="r414" name="with" from="o1921" to="o1922" negation="false" />
      <structure id="o1923" name="fiber" />
    </statement>
    <statement id="1057.txt-2">
      <text>Leaves shorter than culm, blades linear, 4–7 mm wide, flat.</text>
      <structure id="o1924" name="leaf">
