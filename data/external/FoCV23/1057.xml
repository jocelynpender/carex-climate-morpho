        <character char_type="range_value" name="width" from="4" from_unit="mm" to="7" to_unit="mm" />
        <character name="prominence_or_shape" value="flat" />
      </structure>
    </statement>
    <statement id="1057.txt-3">
      <text>Lowest 1 or 2 involucral bracts leaflike, overtopping inflorescence, sheathless, upper ones setaceous, shorter than inflorescence, sheathless.</text>
      <structure id="o1927" name="bract" constraint="involucral">
