    </statement>
    <statement id="1057.txt-5">
      <text>terminal spike gynaecandrous, cylindric, 2–3 × 0.8–0.9 cm;</text>
      <structure id="o1932" name="spike" constraint="terminal">
        <character name="feature" value="gynaecandrous" />
