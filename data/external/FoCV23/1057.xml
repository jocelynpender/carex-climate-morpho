      </structure>
    </statement>
    <statement id="1057.txt-6">
      <text>lowest peduncle 8–12 mm, upper spikes with short peduncle or sessile.</text>
      <structure id="o1933" name="peduncle">
        <character name="feature" value="lowest" />
        <character char_type="range_value" name="size" from="8" from_unit="mm" to="12" to_unit="mm" />
