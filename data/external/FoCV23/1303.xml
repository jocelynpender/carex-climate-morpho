    <place_of_publication>
      <publication_title>Acta Phytotax. Sin.</publication_title>
      <place_in_publication>37: 187. 1999</place_in_publication>
    </place_of_publication>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <species_name>双柏薹草 shuang bai tai cao</species_name>
