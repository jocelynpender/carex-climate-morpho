      </structure>
    </statement>
    <statement id="1559.txt-2">
      <text>Leaves shorter than culm, blades pale green, linear, 3.5–5 mm wide, flat, margins scabrous.</text>
      <structure id="o17044" name="leaf">
        <character name="height_or_length" value="shorter" constraint="than culm" constraintid="o17045" />
      </structure>
      <structure id="o17045" name="culm" />
      <structure id="o17046" name="blade">
