    </statement>
    <statement id="1559.txt-1">
      <text>Culms 80–100 cm tall, 1.5–2 mm thick, acutely trigonous, smooth, clothed at base with purple-red sheaths disintegrating into reticulate fibers.</text>
      <structure id="o17040" name="culm">
        <character char_type="range_value" name="size" from="80" from_unit="cm" to="100" to_unit="cm" />
        <character name="height" value="tall" />
        <character char_type="range_value" name="size" from="1.5" from_unit="mm" to="2" to_unit="mm" />
        <character name="width" value="thick" />
        <character name="shape" value="trigonous" modifier="acutely" />
        <character name="architecture_or_external_texture_or_relief" value="smooth" />
      </structure>
      <structure id="o17041" name="base" />
      <relation id="r3256" name="clothed at" from="o17040" to="o17041" negation="false" />
