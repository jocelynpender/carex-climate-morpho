        <character name="prominence_or_shape" value="flat" />
      </structure>
      <structure id="o17047" name="margin">
        <character name="external_texture_or_relief" value="scabrous" />
      </structure>
    </statement>
    <statement id="1559.txt-3">
      <text>Involucral bracts leaflike, surpassing inflorescence, sheathless.</text>
      <structure id="o17048" name="bract" constraint="involucral">
