    </statement>
    <statement id="1459.txt-1">
      <text>Culms 15–50 cm, triquetrous, rather thin, scabrous on upper part, clothed with yellowish brown and bladeless sheaths at base.</text>
      <structure id="o13986" name="culm">
        <character char_type="range_value" name="size" from="15" from_unit="cm" to="50" to_unit="cm" />
        <character name="shape" value="triquetrous" />
        <character name="width" value="thin" modifier="rather" />
        <character name="external_texture_or_relief" value="scabrous" constraint="on part" constraintid="o13987" />
      </structure>
      <structure id="o13987" name="part" constraint="upper" />
      <structure id="o13988" name="sheath" constraint="bladeless">
        <character name="coloration" value="brown" modifier="yellowish" />
      </structure>
      <relation id="r2652" name="clothed with" from="o13986" to="o13988" negation="false" />
      <structure id="o13989" name="base" />
      <relation id="r2653" name="at" from="o13988" to="o13989" negation="false" />
    </statement>
    <statement id="1459.txt-2">
      <text>Leaves equaling or slightly longer than culm, blades 2–4 mm wide, flat, sheathed.</text>
      <structure id="o13990" name="leaf">
        <character name="prominence_or_shape" value="flat" />
        <character name="architecture_or_feature" value="sheathed" />
      </structure>
      <structure id="o13991" name="mm">
        <character char_type="range_value" name="count" from="2" to="4" />
        <character name="length" value="longer" modifier="slightly" />
      </structure>
      <relation id="r2654" name="equaling" from="o13990" to="o13991" negation="false" />
    </statement>
    <statement id="1459.txt-3">
      <text>Involucral bracts leaflike, longer than spike, nearly not sheathed.</text>
      <structure id="o13992" name="bract" constraint="involucral">
        <character name="architecture" value="leaflike" />
        <character name="length" value="longer" constraint="than spike" constraintid="o13993" />
