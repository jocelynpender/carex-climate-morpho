        <character name="shape" value="linear" />
        <character name="shape" value="clavate" />
        <character char_type="range_value" name="size" from="1" from_unit="cm" to="3" to_unit="cm" />
        <character name="architecture" value="subsessile" />
      </structure>
    </statement>
    <statement id="1459.txt-6">
      <text>remaining spikes female, remote, oblong, 1.5–2.5 × 0.7–0.8 cm, densely many flowered, pedunculate, lowermost peduncle 2–3 cm.</text>
