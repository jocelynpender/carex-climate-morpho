      </structure>
      <structure id="o13993" name="spike" />
    </statement>
    <statement id="1459.txt-4">
      <text>Spikes 2–5;</text>
      <structure id="o13994" name="spike">
        <character char_type="range_value" name="count" from="2" to="5" />
      </structure>
    </statement>
    <statement id="1459.txt-5">
      <text>terminal 1–3 spikes male, approximate, linear or clavate, 1–3 cm, subsessile;</text>
      <structure id="o13995" name="spike">
