  <number>386.</number>
  <TaxonIdentification Status="ACCEPTED">
    <genus_name>Carex</genus_name>
    <species_name>drymophila</species_name>
    <species_authority>Turczaninow ex Steudel</species_authority>
    <place_of_publication>
      <publication_title>Syn. Pl. Glumac.</publication_title>
      <place_in_publication>2: 238. 1855</place_in_publication>
    </place_of_publication>
