    <statement id="1623.txt-0">
      <text>Rhizome slender, creeping.</text>
      <structure id="o18753" name="rhizome">
        <character name="size" value="slender" />
        <character name="habit" value="creeping" />
      </structure>
    </statement>
    <statement id="1623.txt-1">
      <text>Culms loosely tufted, 20–30 cm tall, smooth, leafy on lower 1/4.</text>
      <structure id="o18754" name="culm">
        <character name="external_texture_or_habit" value="tufted" modifier="loosely" />
        <character char_type="range_value" name="size" from="20" from_unit="cm" to="30" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="architecture_or_external_texture_or_relief" value="smooth" />
        <character name="architecture" value="leafy" modifier="on lower 1/4" />
      </structure>
    </statement>
    <statement id="1623.txt-2">
      <text>Basal leaf sheaths rusty brown, eventually pale brown, split into fibers.</text>
      <structure id="o18755" name="sheath" constraint="basal leaf">
        <character name="coloration" value="brown" modifier="rusty" />
        <character name="coloration" value="brown" modifier="eventually;pale" />
