  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <species_name>三阳薹草 san yang tai cao</species_name>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <genus_name>Carex</genus_name>
    <species_name>sachalinensis</species_name>
