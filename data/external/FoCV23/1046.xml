      </structure>
      <relation id="r359" name="with" from="o1599" to="o1600" negation="false" />
    </statement>
    <statement id="1046.txt-2">
      <text>Leaves shorter than culm, blades linear, 1.5–2 mm wide, flat, papillose abaxially.</text>
