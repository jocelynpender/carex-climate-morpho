    <statement id="1046.txt-1">
      <text>Culms tufted, 15–30 cm tall, slender, pendent apically, trigonous, clothed at base with dark brown bladeless sheaths.</text>
      <structure id="o1598" name="culm">
        <character name="external_texture_or_habit" value="tufted" />
        <character char_type="range_value" name="size" from="15" from_unit="cm" to="30" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="size" value="slender" />
        <character name="orientation" value="pendent" modifier="apically" />
        <character name="shape" value="trigonous" />
      </structure>
      <structure id="o1599" name="base" />
      <relation id="r358" name="clothed at" from="o1598" to="o1599" negation="false" />
      <structure id="o1600" name="sheath" constraint="bladeless">
