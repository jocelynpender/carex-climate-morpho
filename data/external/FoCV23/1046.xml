        <character name="coloration" value="purple" modifier="dark" />
        <character name="shape" value="oblong" />
        <character char_type="range_value" name="size" from="2.8" from_unit="mm" to="3.2" to_unit="mm" />
        <character name="relief" value="papillose" modifier="densely minutely" />
