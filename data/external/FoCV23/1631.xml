        <character name="shape" value="2-toothed" />
      </structure>
      <relation id="r3607" name="may_be_the_same" from="o18954" to="o18956" negation="false" />
    </statement>
    <statement id="1631.txt-7">
      <text>Nutlets pale brown, loosely enveloped, ovate or suborbicular, planoconvex, 1.3–1.5 mm, lustrous;</text>
