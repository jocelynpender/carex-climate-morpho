      </structure>
    </statement>
    <statement id="1631.txt-1">
      <text>Culms tufted, 10–70 cm tall, trigonous, erect, stiff, clothed with brown-reddish sheaths at base.</text>
      <structure id="o18939" name="culm">
        <character name="external_texture_or_habit" value="tufted" />
        <character char_type="range_value" name="size" from="10" from_unit="cm" to="70" to_unit="cm" />
