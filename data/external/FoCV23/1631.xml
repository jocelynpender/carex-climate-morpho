        <character name="shape" value="planoconvex" />
        <character char_type="range_value" name="length" from="3" from_unit="mm" to="3.5" to_unit="mm" />
        <character name="width" value="1.5" unit="mm" />
        <character name="feature" value="membranous" />
        <character name="external_texture" value="glabrous" />
        <character name="count" value="many" />
        <character name="architecture_or_feature" value="veined" constraint="on faces" constraintid="o18952" />
      </structure>
      <structure id="o18951" name="glume" />
      <structure id="o18952" name="face" />
      <structure id="o18953" name="margin" constraint="from middle" />
      <structure id="o18954" name="apex" />
      <relation id="r3605" name="to" from="o18953" to="o18954" negation="false" />
      <structure id="o18955" name="wing">
        <character name="width" value="narrow" />
        <character name="shape" value="serrulate" />
      </structure>
      <relation id="r3606" name="with" from="o18954" to="o18955" negation="false" />
      <structure id="o18956" name="apex">
        <character name="shape" value="narrowed" modifier="gradually" constraint="into beak" constraintid="o18957" />
      </structure>
      <structure id="o18957" name="beak" />
