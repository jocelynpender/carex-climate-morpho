        <character name="shape" value="2-toothed" />
      </structure>
    </statement>
    <statement id="1659.txt-8">
      <text>Nutlets brown, tightly enveloped, elliptic or ovate, planoconvex or compressed trigonous;</text>
      <structure id="o19639" name="nutlet">
        <character name="coloration" value="brown" />
        <character name="feature" value="enveloped" modifier="tightly" />
