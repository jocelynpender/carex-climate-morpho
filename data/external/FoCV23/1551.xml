        <character name="orientation" value="pendent" />
      </structure>
      <structure id="o16833" name="peduncle">
        <character name="length" value="long" />
        <character name="size" value="slender" />
      </structure>
      <relation id="r3211" name="with" from="o16832" to="o16833" negation="false" />
    </statement>
    <statement id="1551.txt-5">
      <text>Female glumes dark purple, lanceolate, 2.8–3 mm, green 3-veined costa excurrent into a scabrous long awn.</text>
      <structure id="o16834" name="glume" constraint="female">
        <character name="coloration" value="purple" modifier="dark" />
        <character name="shape" value="lanceolate" />
        <character char_type="range_value" name="size" from="2.8" from_unit="mm" to="3" to_unit="mm" />
        <character name="coloration" value="green" />
        <character name="architecture_or_feature" value="3-veined" />
        <character name="architecture" value="excurrent" constraint="into awn" constraintid="o16836" />
      </structure>
      <structure id="o16835" name="costa" />
