    </statement>
    <statement id="1551.txt-3">
      <text>Lowest involucral bract leaflike, surpassing inflorescence, sheathless, remaining bracts setaceous.</text>
      <structure id="o16828" name="bract" constraint="involucral">
        <character name="feature" value="lowest" />
        <character name="architecture" value="leaflike" />
        <character name="feature" value="sheathless" />
