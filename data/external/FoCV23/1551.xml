    <statement id="1551.txt-1">
      <text>Culms tufted, 40–60 cm tall, slender, acutely trigonous, clothed at base with brown bladeless sheaths.</text>
      <structure id="o16822" name="culm">
        <character name="external_texture_or_habit" value="tufted" />
        <character char_type="range_value" name="size" from="40" from_unit="cm" to="60" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="size" value="slender" />
        <character name="shape" value="trigonous" modifier="acutely" />
      </structure>
      <structure id="o16823" name="base" />
      <relation id="r3208" name="clothed at" from="o16822" to="o16823" negation="false" />
      <structure id="o16824" name="sheath" constraint="bladeless">
        <character name="coloration" value="brown" />
      </structure>
      <relation id="r3209" name="with" from="o16823" to="o16824" negation="false" />
    </statement>
    <statement id="1551.txt-2">
      <text>Leaves short or longer than culm, blades linear, 3–5 mm wide, flat.</text>
      <structure id="o16825" name="leaf" />
