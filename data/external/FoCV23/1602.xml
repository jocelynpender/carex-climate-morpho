      <relation id="r3508" name="equaling" from="o18334" to="o18335" negation="false" modifier="nearly" />
      <structure id="o18336" name="blade">
        <character char_type="range_value" name="width" from="2" from_unit="mm" to="3" to_unit="mm" />
        <character name="prominence_or_shape" value="flat" />
        <character name="external_texture_or_relief" value="rough" modifier="slightly" />
      </structure>
    </statement>
    <statement id="1602.txt-3">
      <text>Inflorescence 1-spiked, terminal, androgynous, pale brown, 1.5–2 cm;</text>
    </statement>
    <statement id="1602.txt-4">
      <text>male part clavate, ca. 4 mm wide, densely flowered;</text>
    </statement>
    <statement id="1602.txt-5">
      <text>female part densely 6–15-flowered.</text>
      <structure id="o18337" name="inflorescence">
