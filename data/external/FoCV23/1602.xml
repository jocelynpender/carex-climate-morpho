        <character name="coloration" value="brown" modifier="pale" />
        <character char_type="range_value" name="size" from="1.5" from_unit="cm" to="2" to_unit="cm" />
        <character name="feature_or_reproduction" value="male" />
        <character name="shape" value="clavate" />
        <character name="width" value="4" unit="mm" />
        <character name="architecture" value="flowered" modifier="densely" />
        <character name="reproduction" value="female" />
        <character name="architecture" value="6-15-flowered" modifier="densely" />
      </structure>
    </statement>
    <statement id="1602.txt-6">
      <text>Female glumes pale brown on margins, white membranous at middle, elliptic-orbicular, ca. 3 mm, with slightly distinct midvein, apex obtusely rounded.</text>
      <structure id="o18338" name="glume" constraint="female">
