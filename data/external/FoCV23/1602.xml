      <relation id="r3504" name="part_of" from="o18327" to="o18329" negation="false" />
    </statement>
    <statement id="1602.txt-1">
      <text>Culms 10–25 cm tall, erect or slightly curved, slightly rough, with pale to dark brown fibrous remains of leaf sheaths at base.</text>
      <structure id="o18330" name="culm">
        <character char_type="range_value" name="size" from="10" from_unit="cm" to="25" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="orientation" value="erect" />
        <character name="orientation" value="curved" modifier="slightly" />
        <character name="external_texture_or_relief" value="rough" modifier="slightly" />
      </structure>
      <structure id="o18331" name="remain">
        <character char_type="range_value" name="coloration" from="pale" to="dark brown" />
        <character name="coloration" value="pale" />
        <character name="coloration" value="brown" modifier="dark" />
        <character name="internal_texture" value="fibrous" />
      </structure>
      <relation id="r3505" name="with" from="o18330" to="o18331" negation="false" />
      <structure id="o18332" name="sheath" constraint="leaf" />
      <relation id="r3506" name="part_of" from="o18331" to="o18332" negation="false" />
      <structure id="o18333" name="base" />
      <relation id="r3507" name="at" from="o18331" to="o18333" negation="false" />
    </statement>
    <statement id="1602.txt-2">
      <text>Leaves nearly equaling culm, blades 2–3 mm wide, flat, slightly rough.</text>
      <structure id="o18334" name="leaf" />
