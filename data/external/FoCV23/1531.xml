        <character name="shape" value="cylindric" />
        <character char_type="range_value" name="length" from="20" from_unit="mm" to="22" to_unit="mm" />
        <character name="width" value="4" unit="mm" />
      </structure>
      <structure id="o16314" name="peduncle">
        <character char_type="range_value" name="size" from="5" from_unit="mm" to="10" to_unit="mm" />
      </structure>
      <relation id="r3106" name="with" from="o16313" to="o16314" negation="false" />
    </statement>
    <statement id="1531.txt-6">
      <text>lateral spikes female, oblong or cylindric, 12–20 × 6–7 mm, with peduncles 4–5 mm, upper subsessile.</text>
