        <character name="arrangement_or_shape" value="linear" />
        <character char_type="range_value" name="width" from="3" from_unit="mm" to="4" to_unit="mm" />
        <character name="prominence_or_shape" value="flat" />
      </structure>
      <structure id="o16309" name="margin">
        <character name="external_texture_or_relief" value="scabrous" />
      </structure>
    </statement>
    <statement id="1531.txt-3">
      <text>Involucral bracts leaflike, shorter than inflorescence, sheathless.</text>
      <structure id="o16310" name="bract" constraint="involucral">
        <character name="architecture" value="leaflike" />
        <character name="height_or_length" value="shorter" constraint="than inflorescence" constraintid="o16311" />
        <character name="feature" value="sheathless" />
      </structure>
      <structure id="o16311" name="inflorescence" />
    </statement>
    <statement id="1531.txt-4">
      <text>Spikes 3 or 4, approximate;</text>
      <structure id="o16312" name="spike">
        <character name="count" value="3" />
        <character name="count" value="4" />
        <character name="feature" value="approximate" />
      </structure>
    </statement>
    <statement id="1531.txt-5">
      <text>terminal spike male, light brown, cylindric, 20–22 × ca. 4 mm, with peduncle 5–10 mm;</text>
      <structure id="o16313" name="spike" constraint="terminal">
        <character name="feature_or_reproduction" value="male" />
