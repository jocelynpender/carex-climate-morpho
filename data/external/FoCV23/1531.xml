        <character name="shape" value="entire" />
      </structure>
    </statement>
    <statement id="1531.txt-9">
      <text>Nutlets loosely enveloped, broadly elliptic, planoconvex, ca. 1.5 mm;</text>
      <structure id="o16327" name="nutlet">
