        <character char_type="range_value" name="width" from="6" from_unit="mm" to="7" to_unit="mm" />
      </structure>
      <structure id="o16316" name="peduncle">
        <character char_type="range_value" name="size" from="4" from_unit="mm" to="5" to_unit="mm" />
      </structure>
      <relation id="r3107" name="with" from="o16315" to="o16316" negation="false" />
      <structure id="o16317" name="upper">
        <character name="architecture" value="subsessile" />
      </structure>
    </statement>
    <statement id="1531.txt-7">
      <text>Female glumes dark purple-red, oblong or lanceolate, ca. 2 mm, green 3-veined costa excurrent into a mucro.</text>
      <structure id="o16318" name="glume" constraint="female">
        <character name="coloration" value="purple-red" modifier="dark" />
        <character name="shape" value="oblong" />
