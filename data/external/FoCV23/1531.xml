  <description>
    <statement id="1531.txt-0">
      <text>Rhizome long.</text>
      <structure id="o16304" name="rhizome" />
    </statement>
    <statement id="1531.txt-1">
      <text>Culms 15–20 cm tall, acutely trigonous, slender, scabrous above.</text>
      <structure id="o16305" name="culm">
        <character char_type="range_value" name="size" from="15" from_unit="cm" to="20" to_unit="cm" />
        <character name="height" value="tall" />
        <character name="shape" value="trigonous" modifier="acutely" />
        <character name="size" value="slender" />
        <character name="external_texture_or_relief" value="scabrous" />
      </structure>
    </statement>
    <statement id="1531.txt-2">
      <text>Leaves shorter than culm, blades linear, 3–4 mm wide, flat, margins scabrous.</text>
      <structure id="o16306" name="leaf">
        <character name="height_or_length" value="shorter" constraint="than culm" constraintid="o16307" />
      </structure>
      <structure id="o16307" name="culm" />
