      </structure>
      <relation id="r1793" name="may_be_the_same" from="o9712" to="o9713" negation="false" />
    </statement>
    <statement id="1316.txt-11">
      <text>Nutlets yellow-green, obovate or broadly elliptic, obtusely trigonous, ca. 3 mm;</text>
      <structure id="o9715" name="nutlet">
        <character name="coloration" value="yellow-green" />
        <character name="shape" value="obovate" />
        <character name="shape" value="elliptic" modifier="broadly" />
        <character name="shape" value="trigonous" modifier="obtusely" />
        <character name="size" value="3" unit="mm" />
      </structure>
    </statement>
