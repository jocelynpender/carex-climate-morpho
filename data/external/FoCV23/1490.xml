        <character name="architecture_or_feature" value="sheathed" constraint="at base" constraintid="o15153" />
      </structure>
      <structure id="o15153" name="base" />
    </statement>
    <statement id="1490.txt-2">
      <text>Leaves longer than culm, 4–8 mm wide, flat, densely hirsute, rather long sheathed.</text>
      <structure id="o15154" name="leaf">
        <character name="length" value="longer" constraint="than culm" constraintid="o15155" />
