    <statement id="1490.txt-1">
      <text>Culms 50–100 cm tall, rather thick, triquetrous, densely hispidulous, dark red-brown and bladeless sheathed at base.</text>
      <structure id="o15152" name="culm">
        <character char_type="range_value" name="size" from="50" from_unit="cm" to="100" to_unit="cm" />
