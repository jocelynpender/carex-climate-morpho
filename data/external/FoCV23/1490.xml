      <structure id="o15155" name="culm" />
    </statement>
    <statement id="1490.txt-3">
      <text>Involucral bracts leaflike, longer than inflorescence, shortly sheathed, densely hirsute.</text>
      <structure id="o15156" name="bract" constraint="involucral">
