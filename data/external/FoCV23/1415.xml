      </structure>
      <structure id="o12765" name="beak">
        <character char_type="range_value" name="size" from="1" from_unit="mm" to="1.5" to_unit="mm" />
      </structure>
      <relation id="r2412" name="may_be_the_same" from="o12761" to="o12764" negation="false" />
