      <relation id="r2409" name="may_be_the_same" from="o12753" to="o12754" negation="false" />
    </statement>
    <statement id="1415.txt-8">
      <text>Nutlets yellow-brown, tightly enveloped, elliptic, 3–3.5 mm (including beak ), trigonous with angles constricted at middle, faces concave both at apex and base, base with short curved stipe, apex abruptly contracted into a beak 1–1.5 mm;</text>
      <structure id="o12758" name="nutlet">
        <character name="coloration" value="yellow-brown" />
        <character name="feature" value="enveloped" modifier="tightly" />
        <character name="arrangement_or_shape" value="elliptic" />
        <character char_type="range_value" name="size" from="3" from_unit="mm" to="3.5" to_unit="mm" />
        <character name="shape" value="trigonous" constraint="with angles" constraintid="o12760" />
        <character name="shape" value="concave" constraint="at apex, base" constraintid="o12761  o12762" />
      </structure>
      <structure id="o12759" name="beak" in_brackets="true" />
      <relation id="r2410" name="including" from="o12758" to="o12759" negation="false" in_brackets="true" />
      <structure id="o12760" name="angle">
        <character name="size" value="constricted" modifier="at middle" />
