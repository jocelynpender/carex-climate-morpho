  <number>451.</number>
  <TaxonIdentification Status="ACCEPTED">
    <genus_name>Carex</genus_name>
    <species_name>stipitinux</species_name>
    <species_authority>C. B. Clarke ex Franchet</species_authority>
    <place_of_publication>
      <publication_title>Bull. Soc. Philom. Paris, sér.</publication_title>
      <place_in_publication>8, 7: 31. 1895</place_in_publication>
    </place_of_publication>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <species_name>柄果薹草 bing guo tai cao</species_name>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <genus_name>Carex</genus_name>
    <species_name>brunnea</species_name>
    <species_authority>Thunberg</species_authority>
    <variety_name>stipitinux</variety_name>
    <variety_authority>(C. B. Clarke ex Franchet) Kükenthal.</variety_authority>
