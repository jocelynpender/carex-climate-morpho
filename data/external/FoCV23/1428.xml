        <character name="arrangement_or_shape" value="linear" modifier="broadly" />
        <character char_type="range_value" name="width" from="1" from_unit="cm" to="1.5" to_unit="cm" />
        <character name="prominence_or_shape" value="flat" />
      </structure>
      <structure id="o13159" name="base" />
      <structure id="o13160" name="margin" constraint="fibers upper">
        <character name="external_texture_or_relief" value="scabrous" modifier="slightly" />
      </structure>
      <relation id="r2502" name="clothed with dark-c-brown sheaths disintegrating  into" from="o13159" to="o13160" negation="false" />
      <structure id="o13161" name="apex">
        <character name="shape" value="acute" />
        <character name="shape" value="acuminate" />
      </structure>
    </statement>
    <statement id="1428.txt-3">
      <text>Involucral bracts shortly bladed, sheathless.</text>
      <structure id="o13162" name="bract" constraint="involucral">
        <character name="feature" value="sheathless" />
      </structure>
    </statement>
    <statement id="1428.txt-4">
      <text>Spikes 4–7, subbasal, approximate;</text>
      <structure id="o13163" name="spike">
        <character char_type="range_value" name="count" from="4" to="7" />
        <character name="position" value="subbasal" />
        <character name="feature" value="approximate" />
      </structure>
    </statement>
    <statement id="1428.txt-5">
      <text>terminal spike male, linear-cylindric, 3.5–5 cm;</text>
      <structure id="o13164" name="spike" constraint="terminal">
        <character name="feature_or_reproduction" value="male" />
        <character name="shape" value="linear-cylindric" />
        <character char_type="range_value" name="size" from="3.5" from_unit="cm" to="5" to_unit="cm" />
      </structure>
    </statement>
    <statement id="1428.txt-6">
      <text>remaining spikes female, oblong-cylindric, 3–5 cm, densely flowered, pedunculate.</text>
      <structure id="o13165" name="spike" constraint="remaining">
        <character name="reproduction" value="female" />
        <character name="shape" value="oblong-cylindric" />
