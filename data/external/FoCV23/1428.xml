        <character name="arrangement_or_shape" value="elliptic" />
        <character char_type="range_value" name="size" from="4" from_unit="mm" to="5" to_unit="mm" />
        <character name="feature" value="membranous" />
        <character name="external_texture" value="hispid" modifier="sparsely" />
        <character name="count" value="many" />
        <character name="architecture_or_feature" value="veined" />
        <character name="size" value="contracted" constraint="at ends" constraintid="o13173" />
      </structure>
      <structure id="o13172" name="glume" />
      <structure id="o13173" name="end" />
      <structure id="o13174" name="beak" constraint="apical">
        <character name="shape" value="subcylindric" />
        <character name="shape" value="2-toothed" constraint="at orifice" constraintid="o13175" />
      </structure>
      <structure id="o13175" name="orifice" />
    </statement>
    <statement id="1428.txt-9">
      <text>Nutlets yellow when young, tightly enveloped, ovate-elliptic, trigonous with angles constricted above, apical beak short, curved, expanding into an annulate orifice;</text>
