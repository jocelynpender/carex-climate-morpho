        <character name="architecture" value="flowered" modifier="densely" />
        <character name="architecture" value="pedunculate" />
      </structure>
    </statement>
    <statement id="1428.txt-7">
      <text>Female glumes green-yellow, lanceolate, ca. 4 mm, margins scabrous, 3-veined costa excurrent into a scabrous mucro at sharp apex.</text>
