    <statement id="1428.txt-1">
      <text>Culms very short.</text>
      <structure id="o13156" name="culm">
        <character name="height_or_length" value="short" modifier="very" />
      </structure>
    </statement>
    <statement id="1428.txt-2">
      <text>Leaves 25–70 cm, blades dark green, broadly linear, 1–1.5 cm wide, flat, base clothed with dark brown sheaths disintegrating into fibers, upper margins slightly scabrous, apex acute or acuminate.</text>
      <structure id="o13157" name="leaf">
        <character char_type="range_value" name="size" from="25" from_unit="cm" to="70" to_unit="cm" />
      </structure>
      <structure id="o13158" name="blade">
