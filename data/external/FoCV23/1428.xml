        <character name="size" value="4" unit="mm" />
      </structure>
      <structure id="o13167" name="margin">
        <character name="external_texture_or_relief" value="scabrous" />
        <character name="architecture_or_feature" value="3-veined" />
        <character name="architecture" value="excurrent" constraint="into mucro" constraintid="o13169" />
      </structure>
      <structure id="o13168" name="costa" />
      <structure id="o13169" name="mucro">
