        <character char_type="range_value" name="length" from="1.5" from_unit="cm" to="2.5" to_unit="cm" />
        <character name="width" value="0.8" unit="cm" />
      </structure>
      <structure id="o1962" name="flower" constraint="male">
        <character name="count" value="few" />
      </structure>
      <relation id="r424" name="with" from="o1961" to="o1962" negation="false" modifier="occasionally" />
      <structure id="o1963" name="base" />
      <relation id="r425" name="at" from="o1962" to="o1963" negation="false" />
