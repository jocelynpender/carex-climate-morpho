    </statement>
    <statement id="1058.txt-1">
      <text>Culms tufted, 25–50 cm tall, 1–2.5 mm wide, trigonous, scabrous above, clothed at base with purple or dark purple bladeless sheaths disintegrating into fibers.</text>
      <structure id="o1948" name="culm">
        <character name="external_texture_or_habit" value="tufted" />
        <character char_type="range_value" name="size" from="25" from_unit="cm" to="50" to_unit="cm" />
        <character name="height" value="tall" />
        <character char_type="range_value" name="width" from="1" from_unit="mm" to="2.5" to_unit="mm" />
        <character name="shape" value="trigonous" />
        <character name="external_texture_or_relief" value="scabrous" />
      </structure>
      <structure id="o1949" name="base" />
      <relation id="r421" name="clothed at" from="o1948" to="o1949" negation="false" />
      <structure id="o1950" name="sheath" constraint="bladeless">
        <character name="coloration" value="purple" />
        <character name="coloration" value="purple" modifier="dark" />
        <character name="dehiscence" value="disintegrating" constraint="into fibers" constraintid="o1951" />
      </structure>
      <relation id="r422" name="with" from="o1949" to="o1950" negation="false" />
      <structure id="o1951" name="fiber" />
    </statement>
    <statement id="1058.txt-2">
      <text>Leaves shorter than culm, blades 2–3.5 mm wide, flat, apex acuminate.</text>
      <structure id="o1952" name="leaf">
        <character name="height_or_length" value="shorter" constraint="than culm" constraintid="o1953" />
      </structure>
      <structure id="o1953" name="culm" />
      <structure id="o1954" name="blade">
        <character char_type="range_value" name="width" from="2" from_unit="mm" to="3.5" to_unit="mm" />
        <character name="prominence_or_shape" value="flat" />
      </structure>
      <structure id="o1955" name="apex">
        <character name="shape" value="acuminate" />
      </structure>
    </statement>
    <statement id="1058.txt-3">
      <text>Lowest involucral bract leafy, overtopping inflorescence, sheathless, remaining bracts setaceous.</text>
      <structure id="o1956" name="bract" constraint="involucral">
        <character name="feature" value="lowest" />
