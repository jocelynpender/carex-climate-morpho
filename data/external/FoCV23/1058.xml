        <character name="count" value="4" />
        <character name="arrangement" value="contiguous" />
        <character name="arrangement" value="lowest" modifier="distant" />
      </structure>
    </statement>
    <statement id="1058.txt-5">
      <text>terminal spike gynaecandrous, 1.5–2.5 × ca. 0.8 cm;</text>
      <structure id="o1960" name="spike" constraint="terminal">
