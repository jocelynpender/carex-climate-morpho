  <description>
    <statement id="573.txt-0">
      <text>Rhizomes short.</text>
      <structure id="o32135" name="rhizome">
        <character name="height_or_length" value="short" />
      </structure>
    </statement>
    <statement id="573.txt-1">
      <text>Culms slightly tufted, 0.7–1.5 m tall, obtusely 3-angled, 7- or 8-nodose, smooth but slightly scabrous near inflorescence.</text>
      <structure id="o32136" name="culm">
        <character name="external_texture_or_habit" value="tufted" modifier="slightly" />
        <character char_type="range_value" name="size" from="0.7" from_unit="m" to="1.5" to_unit="m" />
        <character name="height" value="tall" />
        <character name="shape" value="3-angled" modifier="obtusely" />
        <character name="shape" value="7-or-8-nodose" />
        <character name="architecture_or_external_texture_or_relief" value="smooth" modifier="slightly scabrous" constraint="near inflorescence" constraintid="o32137" />
      </structure>
      <structure id="o32137" name="inflorescence" />
    </statement>
    <statement id="573.txt-2">
      <text>Leaves cauline, shorter than inflorescence;</text>
      <structure id="o32138" name="leaf">
        <character name="position" value="cauline" />
        <character name="height_or_length" value="shorter" constraint="than inflorescence" constraintid="o32139" />
      </structure>
      <structure id="o32139" name="inflorescence" />
    </statement>
    <statement id="573.txt-3">
      <text>sheath ± long;</text>
      <structure id="o32140" name="sheath">
        <character name="character" value="length" modifier="more or less" />
      </structure>
    </statement>
    <statement id="573.txt-4">
      <text>leaf blade 7–10 mm wide, abaxial midvein and margin slightly scabrous.</text>
      <structure id="o32141" name="blade" constraint="leaf">
        <character char_type="range_value" name="width" from="7" from_unit="mm" to="10" to_unit="mm" />
      </structure>
      <structure id="o32142" name="midvein" constraint="abaxial">
        <character name="external_texture_or_relief" value="scabrous" modifier="slightly" />
      </structure>
      <structure id="o32143" name="margin" />
    </statement>
    <statement id="573.txt-5">
      <text>Involucral bracts 2 or 3, leaflike, overtopping inflorescence.</text>
      <structure id="o32144" name="bract" constraint="involucral">
        <character name="count" value="2" />
        <character name="count" value="3" />
        <character name="architecture" value="leaflike" />
      </structure>
      <structure id="o32145" name="inflorescence" />
      <relation id="r6774" name="overtopping" from="o32144" to="o32145" negation="false" />
    </statement>
    <statement id="573.txt-6">
      <text>Inflorescence a terminal large compound anthela, with many rays;</text>
      <structure id="o32146" name="inflorescence">
        <character name="type" value="terminal large compound anthela" />
      </structure>
      <structure id="o32147" name="anthela">
        <character name="position" value="terminal" />
        <character name="size" value="large" />
        <character name="architecture" value="compound" />
      </structure>
      <structure id="o32148" name="ray">
        <character name="count" value="many" />
      </structure>
      <relation id="r6775" name="with" from="o32146" to="o32148" negation="false" />
    </statement>
    <statement id="573.txt-7">
      <text>first rays to 9 cm, glabrous.</text>
      <structure id="o32149" name="ray">
        <character char_type="range_value" name="size" from="0" from_unit="cm" to="9" to_unit="cm" />
        <character name="external_texture" value="glabrous" />
      </structure>
    </statement>
    <statement id="573.txt-8">
      <text>Spikelets solitary, oblong-ovoid to narrowly ovoid, 5–8 × ca. 2 mm, many flowered, apex acuminate.</text>
      <structure id="o32150" name="spikelet">
        <character name="arrangement_or_habit" value="solitary" />
        <character char_type="range_value" name="shape" from="oblong-ovoid" to="narrowly ovoid" />
        <character name="shape" value="oblong-ovoid" />
        <character name="shape" value="ovoid" modifier="narrowly" />
        <character char_type="range_value" name="length" from="5" from_unit="mm" to="8" to_unit="mm" />
        <character name="width" value="2" unit="mm" />
        <character name="count" value="many" />
        <character name="architecture" value="flowered" />
      </structure>
      <structure id="o32151" name="apex">
        <character name="shape" value="acuminate" />
      </structure>
    </statement>
    <statement id="573.txt-9">
      <text>Glumes densely arranged, oblong, ca. 2 mm, membranous, both surfaces dark grayish black, midvein pale yellow, base sometimes straw-colored, margin apically ciliate, apex rounded.</text>
      <structure id="o32152" name="glume">
        <character name="arrangement" value="arranged" modifier="densely" />
        <character name="shape" value="oblong" />
        <character name="size" value="2" unit="mm" />
        <character name="feature" value="membranous" />
        <character name="coloration" value="black" modifier="dark-c-grayish" />
      </structure>
      <structure id="o32153" name="midvein">
        <character name="coloration" value="yellow" modifier="pale" />
      </structure>
      <structure id="o32154" name="base">
        <character name="coloration" value="straw-colored" modifier="sometimes" />
      </structure>
      <structure id="o32155" name="margin">
        <character name="architecture_or_shape" value="ciliate" modifier="apically" />
      </structure>
      <structure id="o32156" name="apex">
        <character name="shape" value="rounded" />
      </structure>
    </statement>
    <statement id="573.txt-10">
      <text>Perianth bristles 6, usually 2–4 × as long as nutlet, prominently flexuose, retrorsely scabrous only near apex.</text>
      <structure id="o32157" name="bristle" constraint="perianth">
        <character name="count" value="6" />
        <character name="course" value="flexuose" modifier="prominently" />
        <character name="external_texture_or_relief" value="scabrous" modifier="retrorsely;only" constraint="near apex" constraintid="o32159" />
      </structure>
      <structure id="o32158" name="nutlet" />
      <relation id="r6776" name="2-4×as long as" from="o32157" to="o32158" negation="false" modifier="usually" />
      <structure id="o32159" name="apex" />
    </statement>
    <statement id="573.txt-11">
      <text>Anthers ca. 1 mm, linear-oblong.</text>
      <structure id="o32160" name="anther">
        <character name="size" value="1" unit="mm" />
        <character name="shape" value="linear-oblong" />
      </structure>
    </statement>
    <statement id="573.txt-12">
      <text>Style slightly short;</text>
      <structure id="o32161" name="style">
        <character name="height_or_length" value="short" modifier="slightly" />
      </structure>
    </statement>
    <statement id="573.txt-13">
      <text>stigmas 3.</text>
      <structure id="o32162" name="stigma">
        <character name="count" value="3" />
      </structure>
    </statement>
    <statement id="573.txt-14">
      <text>Nutlet pale yellow, obovoid, ca. 1 mm, compressed 3-sided.</text>
    </statement>
    <statement id="573.txt-15">
      <text>Fl. and fr. Jun–Sep.</text>
      <structure id="o32163" name="nutlet">
        <character name="coloration" value="yellow" modifier="pale" />
        <character name="shape" value="obovoid" />
        <character name="size" value="1" unit="mm" />
        <character name="shape" value="compressed" />
      </structure>
    </statement>
    <statement id="573.txt-16">
      <text>2n = 56.</text>
      <structure name="chromosome" id="o32164">
        <character name="count" value="56" />
      </structure>
    </statement>
  </description>
  <distribution>In water, swampy places; 400–900 m. Heilongjiang, Jilin, Liaoning, Nei Mongol [Japan, Kazakhstan, Korea, Mongolia, Russia; Europe].</distribution>
