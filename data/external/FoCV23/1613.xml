      </structure>
      <structure id="o18564" name="orifice">
        <character name="shape" value="2-toothed" modifier="minutely" />
      </structure>
    </statement>
    <statement id="1613.txt-8">
      <text>Nutlet tightly enclosed, oblong, trigonous, ca. 1.5 mm;</text>
      <structure id="o18565" name="nutlet">
        <character name="position" value="enclosed" modifier="tightly" />
        <character name="shape" value="oblong" />
