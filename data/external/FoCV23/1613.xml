    </statement>
    <statement id="1613.txt-1">
      <text>Culms tufted, ca. 20 cm tall, smooth, surrounded at base by pale brown to brown leaf sheaths.</text>
      <structure id="o18553" name="culm">
        <character name="external_texture_or_habit" value="tufted" />
        <character name="size" value="20" unit="cm" />
        <character name="height" value="tall" />
        <character name="architecture_or_external_texture_or_relief" value="smooth" />
      </structure>
      <structure id="o18554" name="sheath" constraint="leaf">
        <character char_type="range_value" name="coloration" from="pale brown" to="brown" />
        <character name="coloration" value="brown" modifier="pale" />
        <character name="coloration" value="brown" />
      </structure>
      <relation id="r3540" name="surrounded at base by" from="o18553" to="o18554" negation="false" />
    </statement>
    <statement id="1613.txt-2">
      <text>Leaves shorter than culm, blades 0.5–0.8 mm wide, hairlike, soft.</text>
      <structure id="o18555" name="leaf">
        <character name="height_or_length" value="shorter" constraint="than culm" constraintid="o18556" />
