        <character name="count" value="several" modifier="densely" />
        <character name="architecture" value="flowered" />
        <character name="count" value="few" modifier="sometimes loosely" />
        <character name="architecture" value="flowered" constraint="at part" constraintid="o5727" />
      </structure>
      <structure id="o5724" name="base" />
      <relation id="r1086" name="borne at" from="o5723" to="o5724" negation="false" modifier="sometimes" />
      <structure id="o5725" name="culm" />
      <relation id="r1087" name="part_of" from="o5724" to="o5725" negation="false" />
      <structure id="o5726" name="spike">
