      </structure>
      <structure id="o5743" name="base">
        <character name="architecture" value="subsessile" />
      </structure>
      <structure id="o5744" name="apex">
        <character name="size" value="contracted" modifier="abruptly" constraint="into beak" constraintid="o5745" />
      </structure>
      <structure id="o5745" name="beak">
        <character name="height_or_length" value="short" />
      </structure>
    </statement>
    <statement id="1182.txt-11">
      <text>style base slightly thickened;</text>
      <structure id="o5746" name="base" constraint="style">
