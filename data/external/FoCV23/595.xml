        <character name="arrangement" value="arranged" modifier="laxly" />
        <character name="shape" value="oblong-ovate" />
        <character char_type="range_value" name="size" from="3" from_unit="mm" to="4" to_unit="mm" />
        <character name="feature" value="membranous" />
      </structure>
      <structure id="o32810" name="apex">
        <character name="shape" value="acute" constraint="with costa" constraintid="o32811" />
      </structure>
      <structure id="o32811" name="costa">
        <character name="architecture" value="excurrent" constraint="into mucro" constraintid="o32812" />
      </structure>
      <structure id="o32812" name="mucro">
        <character name="height_or_length" value="short" />
      </structure>
    </statement>
    <statement id="595.txt-9">
      <text>Perianth bristles 6, longer than nutlet, apically sparsely antrorsely scabrous.</text>
      <structure id="o32813" name="bristle" constraint="perianth">
        <character name="count" value="6" />
        <character name="length" value="longer" constraint="than nutlet" constraintid="o32814" />
