        <character name="shape" value="oblong" />
        <character char_type="range_value" name="length" from="6" from_unit="mm" to="7" to_unit="mm" />
        <character char_type="range_value" name="width" from="2" from_unit="mm" to="3" to_unit="mm" />
        <character name="count" value="several" />
        <character name="architecture" value="flowered" />
      </structure>
    </statement>
    <statement id="595.txt-8">
      <text>Glumes reddish brown to brown, laxly arranged, oblong-ovate, 3–4 mm, membranous, apex acute with a costa excurrent into a short mucro.</text>
      <structure id="o32809" name="glume">
        <character char_type="range_value" name="coloration" from="reddish brown" to="brown" />
