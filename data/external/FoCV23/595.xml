      </structure>
    </statement>
    <statement id="595.txt-14">
      <text>Nutlet brown, oblong to oblong-obovoid, ca. 1.8 mm, 3-sided.</text>
    </statement>
    <statement id="595.txt-15">
      <text>Fl. and fr. Apr–May.</text>
      <structure id="o32819" name="nutlet">
        <character name="coloration" value="brown" />
        <character char_type="range_value" name="shape" from="oblong" to="oblong-obovoid" />
        <character name="shape" value="oblong" />
        <character name="shape" value="oblong-obovoid" />
        <character name="size" value="1.8" unit="mm" />
      </structure>
    </statement>
  </description>
  <distribution>Wet places at forest margins; ca. 900 m. Anhui, Fujian, Guangdong, Guangxi, Guizhou, Henan, Hubei, Shanxi, Zhejiang [Vietnam].</distribution>
