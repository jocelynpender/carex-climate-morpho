      <structure id="o32677" name="apex">
        <character name="shape" value="obtuse" />
      </structure>
    </statement>
    <statement id="590.txt-9">
      <text>Perianth bristles 10 or more, whitish, 1–1.5 cm.</text>
      <structure id="o32678" name="bristle" constraint="perianth">
        <character name="count" value="10" />
        <character name="count" value="more" />
        <character name="coloration" value="whitish" />
