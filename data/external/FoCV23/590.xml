        <character name="coloration" value="yellow" />
        <character name="external_texture" value="tomentose" />
      </structure>
    </statement>
    <statement id="590.txt-8">
      <text>Glumes dark black to gray, broadly ovate, 4.5–5 mm, many veined, keeled, margin hyaline, apex obtuse.</text>
