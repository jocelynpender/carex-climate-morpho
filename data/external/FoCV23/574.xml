    <place_of_publication>
      <publication_title>Bot. Mag. (Tokyo)</publication_title>
      <place_in_publication>18: 119. 1904</place_in_publication>
    </place_of_publication>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <species_name>华东藨草 hua dong biao cao</species_name>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <genus_name>Scirpus</genus_name>
    <species_name>fuirenoides</species_name>
    <species_authority>Maximowicz</species_authority>
    <variety_name>jaluanus</variety_name>
