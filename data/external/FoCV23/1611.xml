        <character name="coloration" value="brown" modifier="pale" />
      </structure>
    </statement>
    <statement id="1611.txt-4">
      <text>Inflorescence 1-spiked, terminal, androgynous, ovate to orbicular-ovate, 4–7 × 4–5 mm;</text>
    </statement>
    <statement id="1611.txt-5">
      <text>male part not distinct, 3–5-flowered;</text>
    </statement>
    <statement id="1611.txt-6">
