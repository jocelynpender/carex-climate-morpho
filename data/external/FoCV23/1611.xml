        <character name="architecture" value="leafy" modifier="in lower 1/4" />
      </structure>
    </statement>
    <statement id="1611.txt-2">
      <text>Leaves shorter than culm, blades narrow, 0.5–1 mm wide, involute;</text>
      <structure id="o18514" name="leaf">
