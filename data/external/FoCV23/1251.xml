        <character char_type="range_value" name="size" from="1.3" from_unit="cm" to="1.5" to_unit="cm" />
      </structure>
    </statement>
    <statement id="1251.txt-5">
      <text>lateral spikes female, oblong, 8–12 mm, ca. 3 mm wide, densely many flowered;</text>
