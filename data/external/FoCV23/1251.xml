        <character name="dehiscence" value="disintegrating" modifier="slightly" constraint="into fibers" constraintid="o7708" />
      </structure>
      <relation id="r1413" name="with" from="o7706" to="o7707" negation="false" />
      <structure id="o7708" name="fiber" />
    </statement>
    <statement id="1251.txt-1">
      <text>Leaves nearly equaling culm, blades linear, ca. 2 mm wide.</text>
      <structure id="o7709" name="leaf" />
      <structure id="o7710" name="culm" />
      <relation id="r1414" name="equaling" from="o7709" to="o7710" negation="false" modifier="nearly" />
      <structure id="o7711" name="blade">
        <character name="arrangement_or_shape" value="linear" />
        <character name="width" value="2" unit="mm" />
      </structure>
    </statement>
    <statement id="1251.txt-2">
      <text>Lowest involucral bracts leaflike, sheathless, shorter than inflorescence, upper ones glumelike.</text>
      <structure id="o7712" name="bract" constraint="involucral">
        <character name="feature" value="lowest" />
        <character name="architecture" value="leaflike" />
        <character name="feature" value="sheathless" />
