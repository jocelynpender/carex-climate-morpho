    <place_of_publication>
      <publication_title>Acta Phytotax. Sin.</publication_title>
      <place_in_publication>32: 181. 1994</place_in_publication>
    </place_of_publication>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <species_name>糙毛薹草 cao mao tai cao</species_name>
