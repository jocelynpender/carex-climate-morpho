  <TaxonIdentification Status="SYNONYM">
    <species_name>新疆薹草 xin jiang tai cao</species_name>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <genus_name>Carex</genus_name>
    <species_name>nitida</species_name>
    <species_authority>Host</species_authority>
    <variety_name>aspera</variety_name>
