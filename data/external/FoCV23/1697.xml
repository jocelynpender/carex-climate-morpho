        <character name="shape" value="obovate" />
        <character name="shape" value="planoconvex" />
        <character name="length" value="3" unit="mm" />
        <character name="width" value="1" unit="mm" />
        <character name="architecture_or_feature" value="veined" modifier="obscurely finely" />
      </structure>
      <structure id="o20461" name="glume" />
      <structure id="o20462" name="base">
        <character name="arrangement_or_shape" value="cuneate" modifier="broadly" />
        <character name="architecture" value="stipitate" />
      </structure>
      <structure id="o20463" name="apex">
        <character name="shape" value="tapering" modifier="gradually" constraint="into beak" constraintid="o20464" />
      </structure>
      <structure id="o20464" name="beak">
