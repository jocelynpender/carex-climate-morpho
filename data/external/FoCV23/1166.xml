      </structure>
      <structure id="o5242" name="apex">
        <character name="size" value="contracted" modifier="abruptly" constraint="into beak" constraintid="o5243" />
      </structure>
      <structure id="o5243" name="beak">
        <character name="height_or_length" value="short" />
      </structure>
      <structure id="o5244" name="orifice">
        <character name="shape" value="2-toothed" modifier="shortly" />
      </structure>
    </statement>
    <statement id="1166.txt-10">
      <text>Nutlets brown at maturity, obovate-elliptic, trigonous, 4–4.5 mm, base abruptly contracted into a short stipe, apex beakless;</text>
      <structure id="o5245" name="nutlet">
        <character name="coloration" value="brown" />
