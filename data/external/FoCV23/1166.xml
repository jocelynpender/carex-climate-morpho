      </structure>
      <structure id="o5229" name="part">
        <character name="position" value="middle" />
        <character name="position" value="lower" />
      </structure>
      <relation id="r1031" name="borne on" from="o5227" to="o5229" negation="false" />
      <structure id="o5230" name="culm" />
      <relation id="r1032" name="part_of" from="o5229" to="o5230" negation="false" />
    </statement>
    <statement id="1166.txt-5">
      <text>terminal spike ovate, 2.5–3 cm, 8–10 mm thick;</text>
    </statement>
    <statement id="1166.txt-6">
      <text>male part of spike slightly shorter than or equaling female part, oblong, densely many flowered;</text>
      <structure id="o5232" name="spike" />
    </statement>
    <statement id="1166.txt-7">
