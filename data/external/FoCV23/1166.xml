      <structure id="o5231" name="spike" constraint="terminal">
        <character name="shape" value="ovate" />
        <character char_type="range_value" name="size" from="2.5" from_unit="cm" to="3" to_unit="cm" />
        <character char_type="range_value" name="size" from="8" from_unit="mm" to="10" to_unit="mm" />
        <character name="width" value="thick" />
