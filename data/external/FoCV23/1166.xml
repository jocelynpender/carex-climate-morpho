        <character name="reproduction" value="female" />
        <character name="count" value="many" modifier="densely" />
        <character name="architecture" value="flowered" />
      </structure>
    </statement>
    <statement id="1166.txt-8">
      <text>Female glumes pale greenish laterally, green at middle, ovate or broadly ovate, 4.5–5.5 mm, membranous, 3-veined, thinly several veined on both sides, apex acute or acuminate.</text>
      <structure id="o5233" name="glume" constraint="female">
        <character name="coloration" value="greenish" modifier="laterally;pale" />
        <character name="coloration" value="green" modifier="at middle" />
        <character name="shape" value="ovate" />
        <character name="shape" value="ovate" modifier="broadly" />
        <character char_type="range_value" name="size" from="4.5" from_unit="mm" to="5.5" to_unit="mm" />
        <character name="feature" value="membranous" />
        <character name="architecture_or_feature" value="3-veined" />
        <character name="count" value="several" modifier="thinly" />
        <character name="architecture_or_feature" value="veined" constraint="on sides" constraintid="o5234" />
      </structure>
      <structure id="o5234" name="side" />
      <structure id="o5235" name="apex">
        <character name="shape" value="acute" />
        <character name="shape" value="acuminate" />
      </structure>
    </statement>
    <statement id="1166.txt-9">
      <text>Utricles greenish, longer than glume, obovate-oblong, trigonous, 6–6.5 mm, thickly papery, several veined, pubescent on veins, base attenuate into a short stipe, margins densely hispidulous, apex abruptly contracted into a short beak, orifice shortly 2-toothed.</text>
      <structure id="o5236" name="utricle">
        <character name="coloration" value="greenish" />
        <character name="length" value="longer" constraint="than glume" constraintid="o5237" />
        <character name="shape" value="obovate-oblong" />
        <character name="shape" value="trigonous" />
        <character char_type="range_value" name="size" from="6" from_unit="mm" to="6.5" to_unit="mm" />
        <character name="feature_or_internal_texture" value="papery" modifier="thickly" />
        <character name="count" value="several" />
        <character name="architecture_or_feature" value="veined" />
      </structure>
      <structure id="o5237" name="glume" />
      <structure id="o5238" name="vein" />
      <relation id="r1033" name="on" from="o5236" to="o5238" negation="false" />
      <structure id="o5239" name="base">
        <character name="shape" value="attenuate" constraint="into stipe" constraintid="o5240" />
      </structure>
      <structure id="o5240" name="stipe">
