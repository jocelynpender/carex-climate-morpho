  <description>
    <statement id="1166.txt-0">
      <text>Rhizome slightly short.</text>
      <structure id="o5213" name="rhizome">
        <character name="height_or_length" value="short" modifier="slightly" />
      </structure>
    </statement>
    <statement id="1166.txt-1">
      <text>Culms tufted, 40–45 cm tall, 1.2–1.5 mm thick, stiff, triquetrous, scabrous on upper part of angles.</text>
      <structure id="o5214" name="culm">
        <character name="external_texture_or_habit" value="tufted" />
        <character char_type="range_value" name="size" from="40" from_unit="cm" to="45" to_unit="cm" />
        <character name="height" value="tall" />
        <character char_type="range_value" name="size" from="1.2" from_unit="mm" to="1.5" to_unit="mm" />
        <character name="width" value="thick" />
        <character name="external_texture" value="stiff" />
        <character name="shape" value="triquetrous" />
        <character name="external_texture_or_relief" value="scabrous" constraint="on part" constraintid="o5215" />
