      <structure id="o5216" name="angle" />
      <relation id="r1028" name="part_of" from="o5215" to="o5216" negation="false" />
    </statement>
    <statement id="1166.txt-2">
      <text>Leaves longer than culm, flat, blades 10–11 mm wide, stiff, slightly scabrous on veins of lower surface and margins, glabrous, few sheaths brownish, persistent at base.</text>
      <structure id="o5217" name="leaf">
        <character name="length" value="longer" constraint="than culm" constraintid="o5218" />
        <character name="prominence_or_shape" value="flat" />
      </structure>
      <structure id="o5218" name="culm" />
      <structure id="o5219" name="blade">
        <character char_type="range_value" name="width" from="10" from_unit="mm" to="11" to_unit="mm" />
        <character name="external_texture" value="stiff" />
        <character name="external_texture_or_relief" value="scabrous" modifier="slightly" constraint="on veins" constraintid="o5220" />
        <character name="external_texture" value="glabrous" />
      </structure>
      <structure id="o5220" name="vein" />
      <structure id="o5221" name="surface" constraint="lower" />
      <structure id="o5222" name="margin" />
      <relation id="r1029" name="part_of" from="o5220" to="o5221" negation="false" />
      <relation id="r1030" name="part_of" from="o5220" to="o5222" negation="false" />
      <structure id="o5223" name="sheath">
        <character name="count" value="few" />
        <character name="coloration" value="brownish" />
        <character name="duration" value="persistent" constraint="at base" constraintid="o5224" />
      </structure>
      <structure id="o5224" name="base" />
    </statement>
    <statement id="1166.txt-3">
      <text>Involucral bracts leaflike, longer than inflorescence, shortly sheathed.</text>
      <structure id="o5225" name="bract" constraint="involucral">
        <character name="architecture" value="leaflike" />
        <character name="length" value="longer" constraint="than inflorescence" constraintid="o5226" />
        <character name="architecture_or_feature" value="sheathed" modifier="shortly" />
      </structure>
      <structure id="o5226" name="inflorescence" />
    </statement>
    <statement id="1166.txt-4">
      <text>Spikes single, terminal, sometimes 1 or 2 lateral spikes borne on middle or lower part of culm, remote, not well developed;</text>
      <structure id="o5227" name="spike">
        <character name="count" value="single" />
        <character name="position" value="terminal" />
        <character name="arrangement" value="remote" />
        <character name="development" value="developed" modifier="not well" />
      </structure>
      <structure id="o5228" name="spike" constraint="lateral">
        <character name="count" value="1" modifier="count list" />
