  <TaxonIdentification Status="SYNONYM">
    <species_name>镜子薹草 jing zi tai cao</species_name>
  </TaxonIdentification>
  <TaxonIdentification Status="SYNONYM">
    <genus_name>Carex</genus_name>
    <species_name>shichiseitensis</species_name>
    <species_authority>Hayata.</species_authority>
